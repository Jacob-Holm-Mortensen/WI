            var init = true;
            function validate() {
                if (init && $) {
                    $('form').formValidate({
                        success: ajaxSubmitForm
                    });
                    init = false;
                }
            }
            var didSubmit = false;
            function ajaxSubmitForm(e, data) {
                if (didSubmit) return;

                var $form = $('form.contact');
                var formData = {async: true};

                $.each($form.serializeArray(), function(i, e) {
                    formData[e.name] = e.value;
                });

                $('.contact-us').addClass('loading').removeClass('success failure');
                $('.fullscreen-wrapper').click(function(){
                    $form = $('.contact-us');
                    $form.removeClass('loading');

                    if ($form.hasClass('success')) {
                         $form.children('form').find("input[type=text], input[type=email], textarea, select").val('');
                    }
                });

                $.post($form.attr('action') || '', formData, function(data, status) {
                    if (status == 'success') {
                        if (data == 'true') {
                            didSubmit = true;
                            $('.contact-us').addClass('success');
                            $('.contact-us .response .html').html(' ');
                        } else {
                            if (data.indexOf('<html>') >= 0) {
                                $('.contact-us').removeClass('loading');
//                                var newDoc = document.open("text/html", "replace");
//                                newDoc.write(data);
//                                newDoc.close();
                            }
                            else {
                                $('.contact-us').addClass('failure');
                                $('.contact-us .response .html').html(data);
                            }
                        }
                    } else {
                        $('.contact-us').removeClass('loading');
                    }
                });
            }
