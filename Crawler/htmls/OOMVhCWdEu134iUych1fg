            jQuery(function($){

                /* Toggle-style Navigation Buttons */

                $('#sell-side-pages').hide(); // hide sell-side on load
                $('.tabbed-nav .btn').on('click touchstart', function(){  //touchstart avoids second click req. on touch device.
                  if(!$(this).hasClass('active-tab')) {
                    var theButton = $(this);
                    $('#hp-solutions-wrapper').css('min-height', $('#hp-solutions-wrapper').height()); //set a current min-height so lower page elements don't pop up and down
                    $('.tabbed-nav .btn.active-tab').removeClass('active-tab');
                    $(this).addClass('active-tab');
                    $('.tab-content.active-tab').fadeOut(500,function(){
                      $('.tab-content').toggleClass('active-tab').promise().done(function() {
                        $('.tab-content.active-tab').fadeIn(500).promise().done(function() {
                          if (theButton.hasClass('buy-side')) {
                            setCardHeight('buy-side');
                          } else {
                            setCardHeight('sell-side');
                          }
                        });
                      });
                    });
                  }
                });

                $(window).resize(function() {
                  $('#hp-solutions-wrapper').css('min-height', 'unset');
                  toggleSlideDesc();
                  setCardHeight('buy-side');
                  setCardHeight('sell-side');
                });

                function toggleSlideDesc() {
                  if ($(window).width() < 500) {
                    keepThese = $(".hero-slide-desc p").has(".btn");
                    $(".hero-slide-desc p").not(keepThese).css('display', 'none')
                  } else {
                    $(".hero-slide-desc p").css('display', 'block');
                  }
                }
                toggleSlideDesc();
            });

            function setCardHeight(carouselId) {
              cardContent = $('#'+carouselId + " .card .card-content");
              var newHeight = 0;
              if ($("#"+carouselId+"-pages").css('display') == 'block') {
                cardContent.height('auto');
                cardContent.each(function(index) {
                  theHeight = $(this).height();
                  if (theHeight > newHeight) {
                    newHeight = theHeight;
                  }
                });
                cardContent.height(newHeight);
              }
            }

        </script>
    
