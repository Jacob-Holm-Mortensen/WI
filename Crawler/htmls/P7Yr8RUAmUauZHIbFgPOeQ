<script type="text/javascript">
var bullet = new Array(2);
bullet[0]=new Image();bullet[0].src="https://sports.cbsimg.net/images/arrow-d.gif";
bullet[1]=new Image();bullet[1].src="https://sports.cbsimg.net/images/arrow-r.gif";
function toggleupdate(desc,pic) {  
    
            if (DOM){
                update = document.getElementById(desc).style;
            }
            else if (IE){
                update = document.all[desc].style;
            }

            if (IE || DOM) {
                if ((update.display == "none")||(!update.display)) {
                    update.display="block";
                    pic.src = bullet[0].src;
            } 
            else {
                update.display="none";
                pic.src = bullet[1].src;
            }
        }
        
}
</script>
