    <script type="text/javascript" src="https://sports.cbsimg.net/js/global-footer-min-v00101.js"></script>
<span id="footerScripts">
    <span id="footerScriptsExternal">
        
    </span>
    <script id="footerScriptsRegistered" type="text/javascript">
    
    </script>
    <script id="footerScriptsReady" type="text/javascript">
    $(document).ready(function() {
        
        if ($('.updateTime, .gmtTime').length) {CBSi.widget.Time.update('body span');}
        if ($('.tooltip').length) {CBSi.widget.ToolTip.init(CBSi.widget.ToolTip.cfg);}
        if ($('.actionMenu').length) {
            var actionMenuLeft = $(".actionMenu").position().left;
            var userInfoLeft = (actionMenuLeft + 2) +"px";
            $("#userInfo").css({"left":userInfoLeft});
        }
        $('body').delegate("[om-event]", "click", function(e) {
            om.trackClick( {'item':$(this).attr("om-event")} );
        });
        
            var adSkinLeft       = $('#adSkinLeft');
            var adSkinRight      = $('#adSkinRight');
            var adSkin           = $(adSkinLeft).add(adSkinRight);
            var bodyNavClass 	 = 'is-sticky-nav';
            var bodyTag          = $('body');
            var mantle           = $('#mantle_skin');
            var headerCont       = $('#site-header');
            var navBar           = $('#cbs-site-nav');
            var navBarHeight     = navBar.outerHeight();
            var networkBar       = $('#nav-network-bar');
            var networkBarHeight = networkBar.outerHeight();
            var networkBarPadding = 8;
            var promoBarHeight   = $('#fantasy_promo_bar_row').outerHeight();
            var saagHeight       = $('#saagWrapper').outerHeight() || 0 ;
            var skyBoxHeight     = $('#skybox-above').height();
            var skyboxSpacerId   = 'skybox-nav-spacer';
    
            // Calculate when to make the nav bar sticky.
            if (networkBar.length) {
                // Create an empty spacer for behind the sticky nav.
                // CBSi.log('_toggleStickyNav: There is a Network Bar on this page.');
                headerCont.before('<div id="' + skyboxSpacerId + '" class="' + skyboxSpacerId + '" style="height: 0"></div>');
            }
        
            var skyboxSpacer = $('#skybox-nav-spacer');
        
            $(window).on("scroll resize",function(e) {
            
                var windowScrollTop = parseInt($(this).scrollTop());
                
                // BEGIN Scroll code for Prestitial:
                //check if scroll_prestitial is visible
                var prestitialVisible = $('#scroll_prestitial:visible').length;
                //CBSi.log('prestitialVisible ', prestitialVisible);

                //get height of presitial element
                var prestitialHeight = $('#scroll_prestitial iframe').height();
                //CBSi.log('prestitialHeight ', prestitialHeight);

                //Determine via tricky conditional states of DOM element if the
                //  prestitial is still there
                var hasPrestitial = 0;
                if (prestitialHeight != null) {
                    hasPrestitial = 1;
                } else if (prestitialHeight > 0) {
                    hasPrestitial = 1;
                }
                //finally check visibility but no height
                if (prestitialVisible == 0) {
                    hasPrestitial = 0;
                } else if (prestitialVisible == 1 && prestitialHeight == 0) {
                    hasPrestitial = 0;
                }
                // CBSi.log('hasPrestitial', hasPrestitial);
                
                var skyboxMaxHeight = parseInt($('.ad-top-strip').attr('data-skyboxmaxheight')) || 0;
                var skyboxMinHeight = parseInt($('.ad-top-strip').attr('data-skyboxminheight')) || 0;
                var skyboxHeight = ( (skyboxMaxHeight - windowScrollTop) < skyboxMinHeight  )
                    ? skyboxMinHeight
                    : skyboxMaxHeight - windowScrollTop;
        
                var skyboxDiv = $('.ad-top-strip div');
                var skyboxIframe = $('.ad-top-strip div iframe');
        
                if ( windowScrollTop > 0 && hasPrestitial == 0) {
                    /*
                     * The page is now scrolling...
                     */
                    if (!bodyTag.hasClass('skybox-loaded-dfp')) {
                        skyboxDiv.css({ 'position' : 'fixed' , 'height' : skyboxHeight + 'px' });
                        skyboxIframe.css({ 'height': skyboxHeight + 'px' });
                    }
                                
                    if (skyboxHeight <= skyboxMinHeight) {
                     
                        /*
                         * The skybox ad is collapsed to its minumum height and it has added the class skybox-collapsed to the body tag.
                         * Alternately, there is no skybox ad on the page.
                         */
                     
                        if (windowScrollTop > skyboxMaxHeight - skyboxHeight + networkBarHeight) {
                        
                            /*
                             * Tests to determine if the Navigation should pin.
                             * Note that the navigation does not pin until after the network row has scrolled out of view.
                             */

                            /*
                             * The skybox ad is minimized and the navigation should pin.
                             */
                             
                            // CBSi.log('_toggleStickyNav: The Skybox is at minHeight or absent. The nav should pin.');
        
                            skyboxSpacer.css({ 'height' : skyboxMaxHeight + navBarHeight + 'px' });
                            navBar.css({ 'position': 'fixed', 'top' : skyboxHeight + 'px' });
        
                            mantle.css({ 'margin-top': 0 });
                            
                            if (windowScrollTop > networkBarHeight + saagHeight ) {
                                /*
                                 * Even if the nav is pinned, the Scores At a Glance (SAAG) might still be scrolling so we do not pin the
                                 * adSkinLeft and adSkinRight until the SAAG has scrolled out of view.
                                 */
                                adSkin.css({'position' : 'fixed', 'top': navBarHeight + skyboxMinHeight + 'px' });
                            } else {
                                adSkin.css({ 'position' : 'absolute', 'top': 0 });
                            }
        
                            if (!bodyTag.hasClass(bodyNavClass)) {
                                bodyTag.addClass(bodyNavClass);
                            }
        
                        } else {

                            /*
                             * The skybox ad is minimized but the navigation is not yet pinned
                             * because the network row is still partially visible.
                             */

                            // CBSi.log('_toggleStickyNav: The Skybox is at minHeight or absent. Network bar is scrolling so nav is not pinned.');
        
                            skyboxSpacer.css({ 'height' : skyboxMaxHeight + 'px' });
                            navBar.css({ 'position': 'relative', 'top': 0 });

                            mantle.css({ 'margin-top' : 0 });
                            
                            if (bodyTag.hasClass(bodyNavClass)) {
                                bodyTag.removeClass(bodyNavClass);
                            }
                        }
                    } else if (skyboxHeight > skyboxMinHeight) {
                        /* The ad is in an expanded state so we need to address the height of the spacer
                         * and the position of the navigation rows below the ad.
                         */
                        // CBSi.log('_toggleStickyNav: The Skybox is expanded.');

                        skyboxSpacer.css({ 'height' : skyboxMaxHeight + 'px' });
                        navBar.css({ 'position': 'relative', 'top': 0 });

                        mantle.css({ 'margin-top' : 0 });
                        adSkin.css({'position' : 'absolute', 'top': 0 });
                        
                        if (bodyTag.hasClass(bodyNavClass)) {
                            bodyTag.removeClass(bodyNavClass);
                        }

                        if (!bodyTag.hasClass('skybox-loaded-dfp')) {
                            skyboxDiv.css({ 'height' : skyboxHeight + 'px' });
                            skyboxIframe.css({ 'height': skyboxHeight + 'px' });
                        }        
                    }
                } else {
                    /*
                     * The page is not scrolled at all so we want to set the spacer to a height of 0.
                     */
                    
                    // CBSi.log('_toggleStickyNav: The page is not scrolled.');
                    
                    skyboxSpacer.css({ 'height' : 0 });
                    navBar.css({ 'position': 'relative', 'top': 0 });

                    mantle.css({ 'margin-top' : 0 });
                    adSkin.css({'position' : 'absolute', 'top': 0 });
                    
                    if (bodyTag.hasClass(bodyNavClass)) {
                        bodyTag.removeClass(bodyNavClass);
                    }

                    if (!bodyTag.hasClass('skybox-loaded-dfp')) {
                        skyboxDiv.css({ 'position' : 'relative' , 'height' : skyboxHeight + 'px' });
                        skyboxIframe.css({ 'height': skyboxHeight + 'px' });
                    }
                }
            });
        
        if ($('.cbsGifAnimation').length) {CBSi.app.cbsGifAnimation.prototype.init();}
        //setTimeout('CBSi.app.cbsGifAnimation.prototype.init();',3000);
    });
    </script>
</span>

