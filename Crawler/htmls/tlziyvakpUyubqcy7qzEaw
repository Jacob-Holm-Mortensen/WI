															<script type="text/javascript">
									jQuery(document).ready(function() {
							            var refreshData = "10";
							            var refreshSecs = refreshData * 1000;

							           jQuery.getJSON("https://www.nascar.com/live/feeds/series_2/4642/live_feed.json").done(function(data) {
									    	getData(data);
						                });

							            function getData(data) {
											var currentLap = data.lap_number;
									        var currentFlagStatus = data.flag_state;

											var currentLeaderFirstName='';
											var currentLeaderLastName='';
									        //var currentLeader;
									        var currentLeaderVehicleNumber;
										    jQuery(data.vehicles).each(function( i,content ) {
									            var runningPosition = content.running_position;
									            if(runningPosition===1){
									                //currentLeader = content.driver.full_name;
													currentLeaderFirstName=content.driver.first_name;
													currentLeaderLastName=content.driver.last_name;
									                currentLeaderVehicleNumber=content.vehicle_number;
									                return false;
									            }
									        });
										    jQuery(".race-center-series-2 .lap").text(currentLap);

										    jQuery(".race-center-series-2 tr .status").attr('class', 'status');
										    jQuery(".race-center-series-2 tr .status").addClass("flag-"+currentFlagStatus);
										    if(currentFlagStatus == "4" || currentFlagStatus == "9"){
										    	jQuery(".race-center-series-2 .titlebar th.col1Header").text("Result");
										    }
										    else{
										    	jQuery(".race-center-series-2 .titlebar th.col1Header").text("Current Leader");
										    }
									        jQuery(".race-center-series-2 .leader-car-num").text(currentLeaderVehicleNumber);
									        jQuery(".race-center-series-2 .leader-car-driver").html(currentLeaderFirstName + '<br/>' +currentLeaderLastName);
										}

										setInterval(function(){
										    jQuery.getJSON("https://www.nascar.com/live/feeds/series_2/4642/live_feed.json").done(function(data) {
										    	getData(data);
							                });
										}, refreshSecs);
									});
						        </script>
								<table class="race-center-series-2">
									<tbody>
										<tr class="titlebar">
												<th class="col1Header">Current Leader</th>
											<th class="col2Header">Lap</th>
											<th>Status</th>
										</tr>
										<tr>
											<td class="name"><span class="leader-car-num"></span><span class="leader-car-driver"></span></td>
											<td class="lap"></td>
											<td class="status"><span></span></td>
										</tr>
									</tbody>
								</table>
