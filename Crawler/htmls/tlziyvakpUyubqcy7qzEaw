</div></div></div></div></div><div id="pg-1039-2"  class="panel-grid panel-has-style" ><div class="panel-row-style panel-row-style-for-1039-2" ><div id="pgc-1039-2-0"  class="panel-grid-cell" ><div class='nascar-ad-container'><div class='nascar-advertisement ad_bnr_btf_01 item-component' id='ad_bnr_btf_01'></div></div>    <script type="text/javascript">
    jQuery(document).ready(function(){
      function checkBannerWidth(){
        jQuery('.nascar-advertisement').each(function(){
          var bannerAdWidth = jQuery(this).find('iframe').first().attr('width');
          if(bannerAdWidth == null || bannerAdWidth == "undefined" || bannerAdWidth === 0){
            bannerAdWidth = jQuery(this).find('iframe').first().css('width');
          }
          jQuery(this).css({'width':bannerAdWidth});
        });
      }
      setInterval(function(){
        checkBannerWidth();
      }, 500);
    });

    </script>
    </div></div></div><div id="pg-1039-3"  class="panel-grid panel-has-style" ><div class="full-width-container max-width-box panel-row-style panel-row-style-for-1039-3" ><div id="pgc-1039-3-0"  class="panel-grid-cell" ><link rel="stylesheet" type="text/css" href="https://www.nascar.com/wp-content/plugins/ndms_widgets/videoList/css/style.css"/>
