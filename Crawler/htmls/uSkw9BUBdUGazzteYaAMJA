<script>
$(document).ready(function(){
	$(window).on('scroll', function() {
		if ($(window).scrollTop() > 60) {
			$('.mobile-nav-container').addClass('fixed');
		} else {
			$('.mobile-nav-container').removeClass('fixed');
		}
	});
});
$('a#hamburger').click( function(event){ 				
	event.preventDefault();
	if ($(this).hasClass('show') ) {
		$('#modal-bg').css('display', 'none');		
		$('ul#hamburger-menu').slideUp('fast');	
		$(this).removeClass('show');
	} else {
		$(this).addClass('show');
		$('#modal-bg').css('display', 'block');	
		$('ul#hamburger-menu').slideToggle('fast');
	}
	return false;
});
$("#modal-bg").click( function(event){ 				
	event.preventDefault();
	$(this).css('display', 'none');		
	$('ul#hamburger-menu').slideUp('fast');	
	$('a#hamburger').removeClass('show');
	return false;
});
</script>
