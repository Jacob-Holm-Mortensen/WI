<!-- (3 of 3) ATRERO dynamic load -->
<script type="text/javascript">
    // This keeps the widgets individual so we can have more than one per page.
    var atrero = null;
    highlightButton = function() {
        $('#atreroMoreBarTextOnly').text('MORE ');
        $("#atreroMoreBarCtr").mouseover(function(){
            $('#atreroMoreBarTextOnly').addClass('blueText');
            $('#atreroMoreBarArrows').addClass('blueArrow');
        });
        $("#atreroMoreBarCtr").mouseout(function(){
            $('#atreroMoreBarTextOnly').removeClass('blueText');
            $('#atreroMoreBarArrows').removeClass('blueArrow');
        });
    };

    $(document).ready(function(){
        atrero = new CBSi.widget.Atrero({'nodeServer':'https://api.cbssports.com/na', 'version':'v5', 'container':'#atrero', 'categories':'latest,content247', 'recordsInSet':50, 'getBody':'1', 'view':'SynopsisWithoutLogo', 'ids':'581,599,647,704,816,849,882,457,471,477,669,703,701,854', 'childContainer':'#atreroInner', 'omniture_arena':'CollegeFootball', 'limit':5});
        highlightButton();
        // Fire this so templates can execute their javascript functions
        $(document).trigger('atreroUpdate');
        // See if we hide the button entirely
        // DDS - Removed 4/8/13 - User may have more data more than 2 weeks old
        //atrero.checkPagination()
    });
</script>

    </div>
