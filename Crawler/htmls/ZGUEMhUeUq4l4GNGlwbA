        googletag.cmd.push(function() {
            if (typeof index_headertag_lightspeed !== 'undefined') {
                      var cb = (function () {
                          return function () {
                              var slots = googletag.pubads().getSlots();
                              index_headertag_lightspeed.set_slot_targeting(slots);
                               googletag.display("top_strip");
                            };
                      })();
                      index_headertag_lightspeed.add_session_end_hook(cb, true);
                      index_headertag_lightspeed.refresh();
                  } else {
                      googletag.display("top_strip");
                  }
        });
        if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="top_strip" style="width:100%"><br>').insertBefore('#top-strip');
    }
    </script>
    </div>
