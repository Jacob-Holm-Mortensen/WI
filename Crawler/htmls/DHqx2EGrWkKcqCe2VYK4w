	<script type="text/javascript">
function autoCompleteNewsUrl(item) {
    var league, name, pid, pos, team, playerDiv, url;
    league = item.league;
    name = item.value.substring(0, 25);
    pid = item.playerId;
    pos = item.pos;
    team = item.Team;
    seoName = item.seoName;
    return "http://www.cbssports.com/mlb/players/updates/" + pid + '/' + seoName;
}

function autoCompletePlayerUrl(item) {
    var league, name, pid, pos, team, playerDiv, url;
    league = item.league;
    name = item.value.substring(0, 25);
    pid = item.playerId;
    pos = item.pos;
    team = item.Team;
    seolink = parseSeoLink(item.url);
    seoName = item.seoName
    return "http://www.cbssports.com/mlb/players/playerpage/" + pid + '/' + seoName;
}

function autoCompletePlayerDiv(ul, item) {
    var label = (typeof(item.label) != "undefined") ? item.label : "";
    var position = (typeof(item.pos) != "undefined" && item.pos != null) ? item.pos : "";
    var team = (typeof(item.Team) != "undefined" && item.Team != null) ? item.Team : "";

    return $("<li style='display:block;'></li>")
    .data("item.autocomplete", item)
    .append("<a><div style='width:250px; height:16px;'><div style='width:150px; float:left;'>" + label + "</div><div style='width:15px;float:left;'>" + position + "</div><div style='float:right;'>" + team + "</div></div></a>")
    .appendTo(ul);
};


if( !Fantasy ) {var Fantasy = {};}

var autoCompleteInfoHeader = {
    initalQueryLength : 1,
    searchSize   : 20,
    inputTagId   : "name",
    inputTagName : "name",
    divTagId     : "playerSearchAutoCompleteHeader",
    leagueAbbr   : "baseball",
	site         : "cbssports",
	arena        : "mlb",
    setPlayerSearchActivity : 
        function (searchState) {
            if( $("#" + autoCompleteInfoHeader.inputTagId).autocomplete ) {
                if( searchState == 'on' ) {
                    $("#" + autoCompleteInfoHeader.inputTagId).autocomplete( "option", "minLength", 1 );
                } else { 
                    $("#" + autoCompleteInfoHeader.inputTagId).autocomplete( "option", "minLength", 999 );
                }
            }
    },
    formatResult : autoCompletePlayerDiv
};
</script>
