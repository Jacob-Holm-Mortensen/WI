   var _email = "";
   var is_masterpass = false;
   

  
  function openCS(url, network){
	if (url.indexOf("musictoday") !=-1) {
        var url = escape( url );
        var urlString = 'http://www.cbsstore.com/info.php?cs='+url;
	    if( network ) { 
	        urlString += '&net='+network;
	    }
	} else {
	    var urlString = url;
	}
	window.open(urlString, 'cswin','height=700,width=980,toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no,directories=no,status=no');
  }

  function popUpWindow(URL, w, h) {
    radiopage = window.open(URL, 'radiopage', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width='+w+',height='+h+',left = 312,top = 184');
    radiopage.focus();
  }
   
   
  function SetAllCheckBoxes(FormName, FieldName, CheckValue) {
    if(!document.forms[FormName])
      return;
    var objCheckBoxes = document.forms[FormName].elements[FieldName];
    if(!objCheckBoxes)
      return;
    var countCheckBoxes = objCheckBoxes.length;
    if(!countCheckBoxes)
      objCheckBoxes.checked = CheckValue;
    else
      // set the check value for all check boxes
      for(var i = 0; i < countCheckBoxes; i++)
          objCheckBoxes[i].checked = CheckValue;
  }
  
 //if click dropdown arrow
 jQuery.noConflict();
	(function($){
        $(window).ready(function(){
            if ( is_masterpass == true) {
               $("#guestEmail").val(_email);
            }
           });
	
        $('.dropdownTrigger').on('click', function(e){
			e.preventDefault();
			if ($(this).parent().parent('#socialBtns').length > 0) {
				$('.open').not($('.socialLinks')).removeClass('open');
				$('.active').not($('.socialLinks')).removeClass('active');
				$(this).parent().parent().parent().toggleClass('open');
			}
			else {
				$('.open').not(this).removeClass('open');
				$('.active').not($(this).siblings()).removeClass('active');
				$(this).siblings('.dropdown').toggleClass('active');
				$(this).toggleClass('open');
			}
            e.stopPropagation();
        });
        //if html is clicked turn off drop down
        $('html').on('click', function(e){
            if ($('.dropdown').hasClass('active')) {
                $('.dropdown').removeClass('active');
                stopPropEvents();
            }
			$('.socialLinks').removeClass('open');
			if ($('.dropdownTrigger').hasClass('open')) {
			   $('.dropdownTrigger').removeClass('open');
			}
        });
        //if ESC is press close drop down
        $('html').on({
            'keydown': function(e){
                if (e.keyCode == 27 && $('.dropdown').hasClass('active')) {
                   $('.dropdown').removeClass('active');
				   $('.socialLinks').removeClass('open');
                    stopPropEvents();
                }
            }
        });
		
        $('.closeBtn').on('click', function(e){
            hideOverlay('bodyOverlay');
            hideEmailPopup();
        });
        $('#optinCheck').on('click', function(e){
			if ($(this).is(':checked')) {
				if($('#cableProvider').is(":hidden")) {
					$('#cableProvider').show();
					if ($('#cableProvider select').attr('disabled')) {
						$('#cableProvider select').removeAttr('disabled')
					}
				}
			}
			else {
				if($('#cableProvider').is(":visible")) {
					$('#cableProvider').hide();
				    $('#cableProvider select').attr({'disabled': 'disabled'});
				}
			}
		
		});
        //prevent body event from closing the dropdown when dropdown is clicked
        stopPropEvents();
        enterEvent();
        function toggleNav(navTriggerId,navContentId) {
		$(navContentId).toggleClass('on');
		if (!$(navContentId).is(":visible")) {
			$(navContentId).show();
		}
		else {	   
			$(navContentId).hide();
		}};
        function stopPropEvents(){
            $('#socialBtns').on('click', function(e){
                e.stopPropagation();
            });
        }
		 function enterEvent(){
    
            $('#emailSignup').on({
                
                'submit': function(){
                    if(ValidateForm('promo')){
                        newsletterRegister();
                        
                    }
                    return false;
                }
            });    

        }
		// collapsible left navigation for TCM facets 
		if ($('.view-tcm #sliNav').length || $('.device-mobile').length) {
			$("#sliNav li > span").click(function(){
						$(this).next().toggle();
			return false;
			});
			$("#sliNavFacet_studio span").next().hide();
		};
		
		// ASOTV Dropdown to accomodate second row of navigation (alphabet index) 
		  })(jQuery);
  
</script>

    
	
				 									

