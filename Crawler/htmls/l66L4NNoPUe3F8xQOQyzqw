<script type="text/javascript">

            

function couponPost(){
	$.post( "ajax/test.html", function( data ) {
	$( ".result" ).html( data );
	});
}

function expressProcess(action, expressId) {
	if (document.getElementById('expressProcess') !== null) {
		showLoader();
		if (action == "add"){
				location.href="/cart.php?p=" + expressId;
			}
		else {location.href="/cart.php?f=remove&sku=" + expressId;
		};
	}
}
function ajaxCall(form, values){ 
    var payload = (form) ? $(form) : values;
     
    var ajaxOptions = {
        url: 'checkout.php?ajax_call=1&data_only=true',
		onRequest : function(){
			showLoader();
		},
        onComplete: function(results){
            if(results.error != ""){
                var ie6 = Browser.Engine.trident4;       
                if(ie6){
                    var el = $('shipping_option');
                    el.style.visibility = el.lbBackupStyle;
                }
                
                $$('.error_message').each(function(error){
                    error.destroy();
                });
                $each(results.error,function( msg, field ){
                    var error = new Element('div', {'class': 'error_message'}).set('html', msg).injectAfter($('promoError'));
                });
                if(form == 'cartcoup'){
                    $('couponcode').value = '';
                }
				document.getElementById("loadingOverlay").remove();
            } else {
                document.location = "cart.php";
            }
        }
    }        
    ajaxRequest = new Request.JSON(ajaxOptions).post(payload);     
}

function invokeMasterPassLightBox(){
		jQuery.ajax({
			type: "POST",
			url: "/cart.php",
			data: {masterpass_button_clicked: '1'},
			dataType: "json",
			beforeSend : function(){
				showLoader();
			},
			success: function(data){
				MasterPass.client.checkout(data);
				document.getElements('div[id=loadingOverlay]').each(function(overlay){
				overlay.destroy();
				});
			},
			error: function(){
				alert("error...");
			}			
		});

return false;
}
function showLoader(){
    var ie6 = Browser.Engine.trident4;       
    if(ie6){
        var el = $('shipping_option');
        el.lbBackupStyle = el.style.visibility;
        el.style.visibility = "hidden";
    }
    holder = $('cartInner');
	if (holder == null && document.getElementById('cart-totals') !== null)holder=$('cart-totals');
	size = holder.getSize();
    loadOverlay = new Element("div", {id: "loadingOverlay"}).inject(holder, 'top').setStyles({ width: size.x, height: size.y, opacity: 0 });
    var openOverlay = new Fx.Morph(loadOverlay,{duration: 500});
    
    loadOverlay.style.opacity = 0;
    
    openOverlay.start({
        'opacity': [0,OVERLAY_OPACITY]            
    });
    
}

function removeCoupon( couponField, couponForm ) {
    ajaxCall(null, {'couponcode': ''});
}

//submit promo code on keydown
if (document.getElementById('couponcode') !== null) {
document.getElementById('couponcode').onkeydown=function(event){if (event.keyCode == '13') { ajaxCall('cartcoup');return false;}};
};

