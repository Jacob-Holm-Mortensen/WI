<script type="text/javascript">
    
    function visaCheckoutJqueryAjaxCall(ajaxUrl, redirecturl, visaParams){
        
        jQuery(function($) {
            
            ajaxUrl+= '&ts=' + Math.floor(+new Date() / 1000);        
            $.ajax({
                type: 'POST',
                data: visaParams,
                url: ajaxUrl,
                dataType: "json",
                beforeSend: function(){
                    if(redirecturl){
                        showLoader();
                    }       
                },
                success: function(response){
                    if (!response['error']){
                        if(redirecturl){
                            window.location = redirecturl;        
                        } else {
                            hideLoader();
                            //todo : this seems to be some mobiles stores stuff, check /www/da.store/templates/store_default/mobile/modules/checkout/checkout.tpl
                            //where we use 'call_id_store' elements. hiding it for now
                            $("#visa_call_id_store").val(visaParams['visaCallId']);
                            jqueryShowVisa();
                        }
                    } else {
                        //todo: display lightbox instead
                        //hide loader - hideLoader() doesn't seem to be working
                        alert(response['error']);
                        hideLoader();
                        
                        
                    }
                }     
            });
        
        }); 
        
    }
    
    
    //todo: do we need these functions here when we have them in ajaxAllInOneCart-1.2.js?
    function displayVme(){ 
       if ($('vButton') && !$('vButtonCart')) {
        document.getElementById('vButton').style.display="block";
        }
       if ($('checkoutRightContents')) {
        document.getElementById('checkoutRightContents').style.visibility="hidden";
        }
    }
    
    function hideVme(){
        if ($('vButton')) {
        document.getElementById('vButton').style.display="none";
        }
        if ($('checkoutRightContents')) {
            $('checkoutRightContents').style.visibility="visible";
        }
    }
    function showLoader(){
    if ($('#overlay')){$('#overlay').css("display", "block");}
}

</script>
      
