        });
    })();

    $('.jcarousel-list li:nth-child(6) .week_item').addClass('week_item_on');

    $(function () {

        $('.week_item').on('click', function() {

            var container = $('#js-schedule-block'),
                self = $(this),
                season = self.data('season'),
                seasonType = self.data('type'),
                week = self.data('week');

            $.ajax({
                url: '/shows/nfl-on-cbs/xhr/weekly-data/',
                data: {season: season, seasonType: seasonType, week: week},
                dataType: 'json',
                error: function (resp) {
                },
                success: function (resp) {
                    if (resp.success) {
                        container.html(resp.html);
                        $('#disp' + self.attr('id') ).fadeIn(250);
                    }
                }
            });

            $('.week_item').removeClass('week_item_on');
            $('#' + self.attr('id')).addClass('week_item_on');
        });
    });
</script>
