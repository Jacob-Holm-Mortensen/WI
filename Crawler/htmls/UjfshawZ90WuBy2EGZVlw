        <div id="oldWhatsNew" style="margin-top:30px">

    <script type="text/javascript">

        var noData = false;
        var loading = false;
        var page = 1;
        var theState;
        var timer;

        function reloadAds()
        {
            googletag.cmd.push(function() {  googletag.pubads().refresh();  });
        }

        function loadNewContent()
        {
            theUrl = '/shows/nfl-on-cbs/feeds-more/' + parseInt(page + 1);

            jQuery('#loader').show();

            jQuery.ajax({
                type:'GET',
                url: theUrl,
                success:function(data){
                    if(data === 'false') {
                        jQuery('#loader').hide();
                    } else {
                        History.pushState({state:(page+1)}, "NFL on CBS Feeds - Page " + (page+1) + " - CBS.com",  "/shows/nfl-on-cbs/whats-new/" + (page+1) + "/");
                        if(data != "" && data !== 'false'){
                            loading = false;
                            jQuery("#idContainer").append(data);
                            jQuery('#loader').hide();
                            page = page+1;
                        }
                    }
                }
            });
        }


        jQuery(document).ready(function(){

            jQuery(window).scroll(function(){

                var window_top = jQuery(window).scrollTop();

                var wintop = window_top, docheight = jQuery(document).height(), winheight = jQuery(window).height();
                var  scrolltrigger = 0.95;

                if  ((wintop/(docheight-winheight)) > scrolltrigger) {
                    if(!loading) {
                        loading = true;
                        if(!noData)
                            loadNewContent();
                    }
                }

            });

        });

        History.Adapter.bind(window, 'statechange', function() {

            var State = History.getState();
            theState = State.data['state'];

            // remove div refresh ads if user clicks back button
            if(theState < page || theState === undefined) {
                theDiv = "#photo_unit_" + page;
                jQuery(theDiv).remove();
                page = page-1;
                loading = true;
                // sets timeout so scroll doesn't hit when div is removed as user will be at bottom of page
                setTimeout(function(){ loading = false },1000);
                reloadAds();
            }
        });

    </script>

    <!-- full-width well container 953px -->
