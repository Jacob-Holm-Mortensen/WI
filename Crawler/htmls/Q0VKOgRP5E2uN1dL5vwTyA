ot;3&quot;:&quot;21&quot;,&quot;4&quot;:&quot;0&quot;}},&quot;field_position&quot;:{&quot;distance&quot;:&quot;10&quot;,&quot;team_in_possession&quot;:&quot;791&quot;,&quot;side&quot;:&quot;UGA&quot;,&quot;goal_to_go&quot;:&quot;No&quot;,&quot;yardline&quot;:&quot;42&quot;,&quot;redzone&quot;:&quot;No&quot;,&quot;down&quot;:&quot;3&quot;,&quot;nextplay&quot;:&quot;&quot;},&quot;last_score&quot;:{&quot;id&quot;:&quot;153&quot;,&quot;home_score&quot;:&quot;42&quot;,&quot;description&quot;:&quot;8-D.Hodges complete to 6-K.Mcknight. 6-K.Mcknight runs 18 yards for a touchdown. 64-J.Weaver extra point is good.&quot;,&quot;away_score&quot;:&quot;14&quot;,&quot;drive_plays&quot;:&quot;4&quot;,&quot;drive_time&quot;:&quot;01:11&quot;,&quot;type&quot;:&quot;Touchdown&quot;,&quot;scoring_team&quot;:&quot;791&quot;,&quot;drive_yds&quot;:&quot;-32&quot;,&quot;time_remaining&quot;:&quot;13:49&quot;,&quot;quarter&quot;:&quot;4&quot;},&quot;last_play&quot;:{&quot;score_on_play&quot;:&quot;No&quot;,&quot;short_score&quot;:&quot;&quot;,&quot;medium&quot;:&quot;J. Curry rush for -2 yd&quot;,&quot;short&quot;:&quot;-2 yd run&quot;,&quot;id&quot;:&quot;175&quot;,&quot;team_in_possession&quot;:&quot;791&quot;,&quot;distance&quot;:&quot;8&quot;,&quot;side&quot;:&quot;UGA&quot;,&quot;under_review&quot;:&quot;No&quot;,&quot;description&quot;:&quot;14-J.Curry to UGA 42 for -2 yards.&quot;,&quot;drive_id&quot;:&quot;23&quot;,&quot;yardline&quot;:&quot;40&quot;,&quot;down&quot;:&quot;2&quot;,&quot;subplays&quot;:{&quot;subplay&quot;:[{&quot;rush&quot;:{&quot;rusher_name&quot;:&quot;J. Curry&quot;,&quot;yards_on_play&quot;:&quot;-2&quot;,&quot;team_in_possession&quot;:&quot;791&quot;,&quot;rusher&quot;:&quot;2186034&quot;,&quot;yards_to_endzone&quot;:&quot;40&quot;},&quot;order&quot;:&quot;1&quot;,&quot;type&quot;:&quot;Rush&quot;}]},&quot;quarter&quot;:&quot;4&quot;,&quot;time_remaining&quot;:&quot;1:36&quot;},&quot;awayscore&quot;:{&quot;total&quot;:&quot;14&quot;,&quot;timeouts_remaining&quo