ot;:&quot;13&quot;,&quot;4&quot;:&quot;7&quot;}},&quot;field_position&quot;:{&quot;distance&quot;:&quot;12&quot;,&quot;team_in_possession&quot;:&quot;703&quot;,&quot;side&quot;:&quot;LSU&quot;,&quot;goal_to_go&quot;:&quot;No&quot;,&quot;yardline&quot;:&quot;21&quot;,&quot;redzone&quot;:&quot;No&quot;,&quot;down&quot;:&quot;3&quot;,&quot;nextplay&quot;:&quot;&quot;},&quot;last_score&quot;:{&quot;id&quot;:&quot;146&quot;,&quot;home_score&quot;:&quot;37&quot;,&quot;description&quot;:&quot;7-N.Fitzgerald complete to 18-D.Thomas. 18-D.Thomas runs 20 yards for a touchdown. 47-J.Christmann extra point is good.&quot;,&quot;away_score&quot;:&quot;7&quot;,&quot;drive_plays&quot;:&quot;5&quot;,&quot;drive_time&quot;:&quot;01:59&quot;,&quot;type&quot;:&quot;Touchdown&quot;,&quot;scoring_team&quot;:&quot;703&quot;,&quot;drive_yds&quot;:&quot;0&quot;,&quot;time_remaining&quot;:&quot;13:01&quot;,&quot;quarter&quot;:&quot;4&quot;},&quot;last_play&quot;:{&quot;score_on_play&quot;:&quot;No&quot;,&quot;short_score&quot;:&quot;&quot;,&quot;medium&quot;:&quot;Rush for -1 yd&quot;,&quot;short&quot;:&quot;-1 yd run&quot;,&quot;id&quot;:&quot;182&quot;,&quot;team_in_possession&quot;:&quot;703&quot;,&quot;distance&quot;:&quot;11&quot;,&quot;side&quot;:&quot;LSU&quot;,&quot;under_review&quot;:&quot;No&quot;,&quot;description&quot;:&quot;10-K.Thompson kneels at LSU 21 for -1 yard.&quot;,&quot;drive_id&quot;:&quot;22&quot;,&quot;yardline&quot;:&quot;20&quot;,&quot;down&quot;:&quot;2&quot;,&quot;subplays&quot;:{&quot;subplay&quot;:[{&quot;rush&quot;:{&quot;rusher_name&quot;:&quot;&quot;,&quot;yards_on_play&quot;:&quot;-1&quot;,&quot;team_in_possession&quot;:&quot;703&quot;,&quot;rusher&quot;:&quot;0&quot;,&quot;yards_to_endzone&quot;:&quot;20&quot;},&quot;order&quot;:&quot;1&quot;,&quot;type&quot;:&quot;Rush&quot;}]},&quot;quarter&quot;:&quot;4&quot;,&quot;time_remaining&quot;:&quot;0:55&quot;},&quot;awayscore&quot;:{&quot;total&quot;:&quot;7&quot;,&quot;timeouts_remaining&quot;:&quot;2&quot;,&