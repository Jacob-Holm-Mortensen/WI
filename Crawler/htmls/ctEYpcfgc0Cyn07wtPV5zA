<script>
    var showMarquee;
    function renderAdCallback(adObj) {
        if(typeof(showMarquee) !== "undefined"){
            if (navigator.userAgent.indexOf("Firefox") > 0) { //For some reason Firefox doesn't calculate the page height correctly at the moment this callback is fired. Delaying the callback by a second seems to fix it.
                setTimeout(function(){
                    showMarquee.setScrollContainerHeight();
                }, 1000);
            } else {
                showMarquee.setScrollContainerHeight();
            }
        }
    }

    if(typeof(window.runningTests) === "undefined"){
        window.runningTests  = {};
    }
    if(window.runningTests && typeof(window.runningTests.showLandingMarquee) !== "undefined" && window.runningTests.showLandingMarquee === true) {
        document.addEventListener("DOMContentLoaded", function() {
            CBS.Marquee.BlankShowLanding();
        });
    }else{
        if (typeof ShowMarquee === 'undefined') {
            //throw "marqueeModule must be defined globally and is imported from show/marquee.js";
        }

        /*var MarqueeShowHeader;
        document.addEventListener("DOMContentLoaded", function() {
            showMarquee = new ShowMarquee();
        });*/


    }

</script>



