<script type="text/javascript">
    

        var videoModuleLoading = false;
        var videoModPage = 0;
        var totalVideoItemsfound = 50;
        var videoModsize = 3;

        function loadNewVideoContent()
        {


            $(".load-more-video-posts").show();
            theUrl = '/shows/watch_magazine/video-mod-more/' + parseInt(videoModPage + 1)+'/';


            if (totalVideoItemsfound < videoModsize){
            $(".load-more-video-posts").hide();
             }


            jQuery('#videoloader').show();

            jQuery.ajax({
                type:'GET',
                url: theUrl,
                dataType: 'json',
                success:function(data){
                    if(data === 'false') {

                        jQuery('#videoloader').hide();
                        $(".load-more-video-posts").hide();
                    } else {
                        var parts = window.location.pathname.split('/');
                        if (!parts[parts.length - 1]) {
                            parts.pop();
                        }

                        if (!isNaN(parts[parts.length - 1])) {
                            parts.pop();
                        }


                        if(data != "" && data !== 'false'){



                            videoModuleLoading = false;

                            jQuery("#idVideoModContainer").append(data.html);

                            jQuery('#videoloader').hide();

                            videoModPage = videoModPage+1;
                            totalVideoItemsfound = data.totalItemsfound;


                            if (totalVideoItemsfound <= videoModPage * videoModsize || data.hideLoadMore ) {
                                $(".load-more-video-posts").hide();
                            }

                        }
                    }
                },
                complete: function() {
                    if (typeof showMarquee != "undefined") {
                        //showMarquee.setScrollContainerHeight();
                    }
                }
            });


        }

        jQuery(document).ready(function () {
            if (totalVideoItemsfound < videoModsize){
                $(".load-more-video-posts").hide();

            }
            jQuery("#idPaginateVideo").bind('click', function() {
                loadNewVideoContent();
            });

        });


        function hitUtag(utag_data) {
        alert("pre utag");
        utag.view(utag_data);
        alert("post utag");
    }

</script>



