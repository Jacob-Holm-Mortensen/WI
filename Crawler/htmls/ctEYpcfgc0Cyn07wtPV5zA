<script type="text/javascript">
    
        var noData = false;
        var loading = false;
        var page = 1;
        var theState;
        var timer;
        var currentType = 'all';
        var totalItemsfound = 467;
        var size = 4;

        function reloadAds()
        {
            googletag.cmd.push(function() {  googletag.pubads().refresh();  });
        }

        function loadNewContent()
        {


            $(".load-more-posts").show();
            theUrl = '/shows/watch_magazine/feeds-more/' + parseInt(page + 1)+'/'+currentType;


            if (totalItemsfound < size){
            $(".load-more-posts").hide();
             }


            jQuery('#loader').show();

            jQuery.ajax({
                type:'GET',
                url: theUrl,
                dataType: 'json',
                success:function(data){
                    if(data === 'false') {

                        jQuery('#loader').hide();
                        $(".load-more-posts").hide();
                    } else {
                        var parts = window.location.pathname.split('/');
                        if (!parts[parts.length - 1]) {
                            parts.pop();
                        }

                        if (!isNaN(parts[parts.length - 1])) {
                            parts.pop();
                        }

                        var pushStateUrl = parts.join('/') + '/' + (page + 1) + "/";
                        History.pushState({state:(page+1)}, "Watch! Magazine Feeds - Page " + (page+1) + " - CBS.com",  pushStateUrl);
                        if(data != "" && data !== 'false'){



                            loading = false;

                            jQuery("#idContainer").append(data.html);

                            jQuery('#loader').hide();

                            page = page+1;
                            totalItemsfound = data.totalItemsfound;


                            if (totalItemsfound <= page * size) {
                                $(".load-more-posts").hide();
                            }

                        }
                    }
                },
                complete: function() {
                    if (typeof showMarquee != "undefined") {
                        //showMarquee.setScrollContainerHeight();
                    }
                }
            });


        }

        function loadNewFilteredContent(type,typeTitle)
        {
            currentType = type;
            $('div#dropdown').hide();
            page = 0;

            $(".load-more-posts").show();
            theUrl = '/shows/watch_magazine/feeds-change/' + type;


            jQuery('#loader').show();

            jQuery("#idContainer").empty();
            jQuery.ajax({
                type:'GET',
                url: theUrl,
                dataType: 'json',
                success:function(data){
                    if(data.success === 'false' || data.totalItemsfound == 0 ) {
                        jQuery('#loader').hide();
                        $('#rankingBy').empty();
                        $('#rankingBy').text(typeTitle);

                        $('#idContainer').empty();
                        $('#idContainer').html("<div style='font-size: 16px; margin-left: 10px;margin-top: 40px;'>No content available.</div>");
                        $(".load-more-posts").hide();
                        $('#rankingBy').text(typeTitle);


                    } else {
                        //History.pushState({state:(page+1)}, "Watch! Magazine Feeds - Page " + (page+1) + " - CBS.com",  "/shows/watch_magazine/adaptive-test/whats-new/" + (page+1) + "/");
                        if(data != "" && data !== 'false'){
                            loading = false;
                            jQuery("#idContainer").append(data.html);
                            jQuery('#loader').hide();
                            $('#rankingBy').empty();
                            $('#rankingBy').text(typeTitle);
                            totalItemsfound = data.totalItemsfound;

                            page = page+1;
                            if ((totalItemsfound < size) || (totalItemsfound < (page * size ))){
                                $(".load-more-posts").hide();
                            }


                        }
                    }
                }
            });


        }

        jQuery(document).ready(function () {
            if (totalItemsfound < size){
                $(".load-more-posts").hide();

            }
            jQuery("#idPaginate").bind('click', function() {
                loadNewContent();
            });

        });

        History.Adapter.bind(window, 'statechange', function() {
            var State = History.getState();
            theState = State.data['state'];

            // remove div refresh ads if user clicks back button
            if(theState < page || theState === undefined) {
                theDiv = "#photo_unit_" + page;
                jQuery(theDiv).remove();
                page = page-1;
                loading = true;
                // sets timeout so scroll doesn't hit when div is removed as user will be at bottom of page
                setTimeout(function(){ loading = false },1000);
                reloadAds();
            }

            // returns { data: { params: params }, title: "Search": url: "?search" }
            //console.log(State);

            // or you could recall search() because that's where this state was saved
    //        if (State.url == "?search") {
    //            search(data.params);
    //        }

        });
        function hitUtag(utag_data) {
        alert("pre utag");
        utag.view(utag_data);
        alert("post utag");
    }

</script>

