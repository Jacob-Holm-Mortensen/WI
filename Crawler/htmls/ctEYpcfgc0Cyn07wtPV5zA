        jQuery(window).scroll(function(){

            var window_top = jQuery(window).scrollTop();

            if (window_top > 150) {
                $('.loog').addClass('logoSmall');
                $('.nav-div').addClass('nav-up');
            }

            if (window_top < 150) {
                $('.loog').removeClass('logoSmall');
                $('.nav-div').removeClass('nav-up');
            }

        });

    });





