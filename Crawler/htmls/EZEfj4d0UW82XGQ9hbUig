        elements[i].addEventListener("click", function(){
            om.trackVideoClick({'moduleInfo': 'Right Rail|CBSN LIVE Promo|MainImage'});
            window.location.href = 'https://www.cbsnews.com/live/?ftag=CNMe94798';
        }, false);
    }

    var popouticon = document.getElementsByClassName('popout-icon')[0];
    popouticon.addEventListener("click", function(e){
        e.preventDefault();
        om.trackVideoClick({'moduleInfo': 'Right Rail|CBSN LIVE Promo|PopOutImage'});
        window.open('https://www.cbsnews.com/live/compact/?ftag=CNMe94798', 'CBSNews Live', 'width=418, height=330');
    }, false);
</script><div class="module module-listing module-bulk CBSN Top DVR (RR)" id="module-right-rail-cbsn-top-dvr">
       
