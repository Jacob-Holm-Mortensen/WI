        function removeCurlyQuotes(str) {
            //first convert curly quotes to normal quotes
            str = str.replace(/[\u2018\u2019]/g, "'").replace(/[\u201C\u201D]/g, '"');
            return str;
        }
        var CBSHost = 'http://www.cbs.com';
        shareParams = {};
        shareParams.init = function () {
            this.title = 'CBS All Access Acquires $1, A Thrilling New Series About Money And Murder ';
            this.title = removeCurlyQuotes(this.title);
            this.description = 'Is money the root of all evil? Find out when $1 premieres on CBS All Access.';
            this.description = removeCurlyQuotes(this.description);
            this.caption = 'CBS All Access Acquires $1, A Thrilling New Series About Money And Murder ';
            this.caption = removeCurlyQuotes(this.caption);
            this.url = $("link[rel='canonical']").attr("href");
            this.imageUrl = "http://wwwimage3.cbsstatic.com/base/files/blog/eeb9469f52276ff9_cbsaa_blk-blu_1440x400.jpg";
            this.FBappId = '30708972585';
        }
        //social share functions
        function __share_to_fb() {
            shareParams.init();
            social = new CBS.V2.Social();
            CBS.Host = CBSHost;
            social.settings.type = 'facebook';
            social.settings.app_id = shareParams.FBappId;
            social.packet.link = shareParams.url;
            social.packet.caption = shareParams.caption;
            social.packet.picture = shareParams.imageUrl;
            social.packet.title = shareParams.title;
            social.packet.description = shareParams.description;
            social.Share();
        }
        function __share_to_tw() {
            shareParams.init();
            CBS.V2.Social.TW.Share(shareParams.caption +' @CBS', '', shareParams.url);
        }
        function __share_to_gp() {
            shareParams.init();
            window.open('https://plus.google.com/share?url=' + encodeURIComponent(shareParams.url), '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
        }
        var iframeResize = function(key) {
            for (var i=0; i < arguments.length; i++) {
                var iframes = document.getElementsByTagName('iframe');
                var ratio = 9 / 16;
                var container = $('.blog-copy').width();

                for (var i = 0; i != iframes.length; ++i) {
                    var match = iframes[i].src.search(key);
                    if (match != -1) {
                        iframes[i].width = '100%';
                        iframes[i].height = ratio * container;
                    }
                }
            }
        }
        $(window).resize(function(){
            iframeResize(/www.youtube.com/);
        });
        $(document).ready(function(){
            iframeResize(/www.youtube.com/);
            var fontValue = $.cookie("font");
            if ( fontValue) {
                if (fontValue == 'small') $('.blog-content-text').css({'font-size':'100%', 'line-height':'120%'});;
                if (fontValue == 'medium') $('.blog-content-text').css({'font-size':'120%', 'line-height':'140%'});
                if (fontValue == 'large') $('.blog-content-text').css({'font-size':'150%', 'line-height':'160%'});
            } else {
                $.cookie("font", "small");
            }
            $('.socialFont').click(function(){
                if($.cookie("font") == "small" || null){
                    $.cookie("font", "medium");
                    $('.blog-content-text ').css({'font-size': '120%', 'line-height':'140%'});
                } else if ($.cookie("font") == "medium"){
                    $.cookie("font", "large");
                    $('.blog-content-text').css({'font-size':'150%', 'line-height':'160%'});
                } else {
                    $.cookie("font", "small");
                    $('.blog-content-text').css({'font-size':'100%', 'line-height':'120%'});
                }
            });
            $('.socialIcon.googlePlusShareIcon').click(function(){ __share_to_gp(); });
            $('.socialIcon.twitterShareIcon').click(function(){ __share_to_tw(); });
            $('.socialIcon.facebookShareIcon').click(function(){ __share_to_fb(); });
        });
