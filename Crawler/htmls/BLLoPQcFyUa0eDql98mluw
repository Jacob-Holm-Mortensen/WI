    <div style='clear: both;'></div>    
    <table class="newsWhatsNewContainer" style="position:relative;"> <!-- What's new table -->
    </table>
</div>

<script>
    var contentFeedEndpoint = "/whatsNew/recommended/fetchContentFeed/";  //The What's New endpoint, $contentFeedEndpoint, is set in the action, beforeRun of the Show Controller
    var whatsNewItemOffset = 1; // this is being used in cbs.ads.js, counting how many What'sNew item were loaded. so that if there's any blank ads, we can fetch content correctly

    var renderAdCallback = (function() {
        var original_renderAdCallback = null; //If 'renderAdCallback' was defiend somewhere else, we make sure to execute that after we execute our render ad callback code.
        if (renderAdCallback) {
            original_renderAdCallback = renderAdCallback;
        } 
        return function(adParameters) {
            if (typeof original_renderAdCallback === "function") {
                original_renderAdCallback(); //Executes the 'renderAdCallback' function if it was defined previously
            }
            var $whatsNewTable = $("#whatsNewContainer > table");
            if ($whatsNewTable.length < 1) {
                console.error("Could not find %s", $whatsNewTable.selector);
            }
            var $imageTemplate = $("<img class='itemImage'>");
            var $titleTemplate = $("<div class='title'>");
            var $showNameTemplate = $("<div class='showName'>");

            var $linkTemplate = $("<a class='url'>");
            var $fadeTemplate = $("<img class='fade' src='/assets/images/whats_new/gradient_fade.png'>");

            var $borderContainerTemplate = $("<div class='borderContainer'>");
            $.fn.addWhatsNewNewsItem = function(imagePath, altText, title, showName, contentType, url, clipContentId) {
                var $icon = IconClasses.getIcon(contentType);
                var $image = $imageTemplate.clone().attr("src", imagePath).attr("alt", altText);
                var $title = $titleTemplate.clone().text(title);
                var $showName = $showNameTemplate.clone().text(showName);

                var $link = $linkTemplate.clone().attr("href", url);
                if (clipContentId) {
                    $link.attr("vilynx-id", clipContentId);

                }
                var $fade = $fadeTemplate.clone();

                var $borderContainer = $borderContainerTemplate.clone().append($title).append($showName);

                $link.append($fade)
                    .append($icon)
                    .append($image)
                    .append($borderContainer);

                this.append($link);
                return this;
            };

            var contentFeedEndpointSixItems = contentFeedEndpoint + '1/6/';

            $.get(contentFeedEndpointSixItems, {}, function(response) { //Retrieves the items for the What's New Module
                    var items = response.data;
                    var $adDiv;

                    for (var row = 0; row <= 1; row++) {//Adds three rows
                        var $tableRow = $("<tr class='loadMoreRow'>");
                        for (var column = 0; column < 3; column++) { //Adds three columns to each row  
                            if (row === 1 && column === 2 && adParameters.isBlank === true) { //We only want to display an ad for the bottom right slot if we are using a blank ad skin (aka none at all). If we are displaying an ad skin, then we should not render an add and instead just show another What's New item.
                                $adDiv = AdUtilities.giveMeABottomAdPlz();              
                                $tableRow.append($("<td class='adCell'>").append($adDiv));
                            } else {
                                var item = items.shift();
                                if (item) {
                                    whatsNewItemOffset++;
                                    $tableRow.append($("<td>").addWhatsNewNewsItem(item.imagePath, item.altText, item.title, item.showName, item.contentType, item.url, item.clipContentId));
                                } else {
                                    console.warn("No more content feed items returned from the API");
                                    break; //We ran out of data, no point in continuing further
                                }
                            }
                        }
                        $tableRow.appendTo($whatsNewTable);
                    }
                }, "json").fail(function() {
                    console.error("Failed to get more data from %s", contentFeedEndpoint);
            });
        };
    })();
    

</script>

