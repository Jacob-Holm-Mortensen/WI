         googletag.pubads().addEventListener("slotRenderEnded", function(event) {
            var slotName = event.slot.getSlotElementId();
            
            if (slotName == 'leader_plus_top' || slotName == 'leader_top') {
               // CBSi.log("ENTER Collapse empty leaderboard.");
               var slotID = '#' + slotName;
