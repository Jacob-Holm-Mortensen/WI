function displayDFPAds()
{
displayDFPAds = null;
var googletag = window.googletag;

googletag.cmd.push(function()
{

if(window.MaxPreps == null
|| MaxPreps.Ads == null)
{
return;
}

var sessionData = MaxPreps.Ads.GetSessionData();
if(sessionData != null)
{
for(var property in sessionData)
{
if(!sessionData.hasOwnProperty(property))
{
continue;
}
googletag.pubads().setTargeting(property, sessionData[property]);
}
}

// query string overrides
var _params = ['adregion','env','session','subses'];
if(_params.indexOf != null)
{
var qsp = window.queryStringParams
for(var property in qsp)
{
if(!qsp.hasOwnProperty(property)
|| qsp[property] == null)
{
continue;
}
if(_params.indexOf(property) > -1) googletag.pubads().setTargeting(property, qsp[property]);
else if(property.indexOf('adtargeting_') == 0 && property !== 'adtargeting_') googletag.pubads().setTargeting(property.slice(12), qsp[property]);
}
}

if(typeof index_headertag_lightspeed == 'undefined') {
googletag.display("ad_171");
googletag.display("skin_ad_call");
googletag.display("ad_172");
googletag.display("ad_174");

return;
}
index_headertag_lightspeed.add_session_end_hook(function(){
var slots = googletag.pubads().getSlots();
index_headertag_lightspeed.set_slot_targeting(slots);
googletag.display("ad_171");
googletag.display("skin_ad_call");
googletag.display("ad_172");
googletag.display("ad_174");

}, true);
index_headertag_lightspeed.refresh();
});
};
if(!window.secondaryExchangeStatus
|| ('success,failed').indexOf(window.secondaryExchangeStatus) > -1)
{
if(window.displayDFPAds) displayDFPAds();
}

