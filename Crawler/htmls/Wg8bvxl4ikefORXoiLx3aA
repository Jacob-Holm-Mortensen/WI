      <script type="text/javascript">
      function checkForm() {
            var form = document.forms.audsvcs;
            var mtype = form.FEEDBACKTYPE;
            var e_mail = form.EMAIL.value;
            var EMAIL = form.EMAIL;
            var category = form.CATEGORY.options[0];
            /*choose a cbsnews category*/
            if(category.selected==true){alert("Please select a CBS category!");form.CATEGORY.focus();return false}
            /*email edit ????????????????????????????????????????????????????? check for spaces*/
            if (e_mail.length < 1) {alert('Please enter your E-Mail address before submitting');EMAIL.focus();return false;}
            /* split on the @ and .*/var fl = e_mail.split("@");if (fl.length != 2) {alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}first = fl[0];last = fl[1];
            if (first.length < 1){alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}
            if (last.length < 1){alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}last = last.split(".");
            if (last.length < 2){alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}first = last[0];last = last[1];
            if (first.length < 1) {alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}
            if (last.length < 1) {alert('Please enter a valid E-Mail address before submitting');EMAIL.focus();return false;}
            if (form.MSG.value == ''){alert("You must enter a message to be sent.");return false;}return true;}
            holder=this.window.name;
            var lookUpTable = new Array('','3420','3410','3415','3475','3430','3425','3435','3480','3445','3465','3460','3455','','','')
      function look_through(holder) {
            for(var i=0; i<lookUpTable.length;i++)
            {//alert(holder);
                  if(holder==lookUpTable[i])
                        {
                        thisOne = i;
                        thisOne = thisOne;
                        return set_to(thisOne);
                        }
                  else set_to(0);
            }
      }
      function set_to(thisOne){if (thisOne!=0){eval(document.cbsnews.CATEGORY.options[thisOne+0].selected = true);}else{eval(document.cbsnews.CATEGORY.options[0].selected = true);}}
      </script>


      


<script language="JavaScript">
var message = "Welcome to CBS Feedback";
function rtclickcheck(keyp){ if (navigator.appName == "Netscape" && keyp.which == 3){ alert(message); return false; }
if (navigator.appVersion.indexOf("MSIE") != -1 && event.button == 2) { alert(message); return false; } }
document.onmousedown = rtclickcheck;
</script> 
