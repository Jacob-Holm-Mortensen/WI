            <script src="//ajax.googleapis.com/ajax/libs/mootools/1.4.5/mootools-yui-compressed.js"></script>
		
    
    
	    	
    
    

<script type="text/javascript">
   var _email = "";
   var is_masterpass = false;
   

  
  function openCS(url, network){
	if (url.indexOf("musictoday") !=-1) {
        var url = escape( url );
        var urlString = '/info.php?cs='+url;
	    if( network ) { 
	        urlString += '&net='+network;
	    }
	} else {
	    var urlString = url;
	}
	window.open(urlString, 'cswin','height=700,width=980,toolbar=no,menubar=no,scrollbars=no,resizable=no,location=no,directories=no,status=no');
  }

  function popUpWindow(URL, w, h) {
    radiopage = window.open(URL, 'radiopage', 'toolbar=0,scrollbars=0,location=0,statusbar=0,menubar=0,resizable=0,width='+w+',height='+h+',left = 312,top = 184');
    radiopage.focus();
  }
   
   
  function SetAllCheckBoxes(FormName, FieldName, CheckValue) {
    if(!document.forms[FormName])
      return;
    var objCheckBoxes = document.forms[FormName].elements[FieldName];
    if(!objCheckBoxes)
      return;
    var countCheckBoxes = objCheckBoxes.length;
    if(!countCheckBoxes)
      objCheckBoxes.checked = CheckValue;
    else
      // set the check value for all check boxes
      for(var i = 0; i < countCheckBoxes; i++)
          objCheckBoxes[i].checked = CheckValue;
  }
  
 //if click dropdown arrow
 jQuery.noConflict();
	(function($){
        $(window).ready(function(){
            if ( is_masterpass == true) {
               $("#guestEmail").val(_email);
            }
           });
	
        $('.dropdownTrigger').on('click', function(e){
			e.preventDefault();
			if ($(this).parent().parent('#socialBtns').length > 0) {
				$('.open').not($('.socialLinks')).removeClass('open');
				$('.active').not($('.socialLinks')).removeClass('active');
				$(this).parent().parent().parent().toggleClass('open');
			}
			else {
				$('.open').not(this).removeClass('open');
				$('.active').not($(this).siblings()).removeClass('active');
				$(this).siblings('.dropdown').toggleClass('active');
				$(this).toggleClass('open');
			}
            e.stopPropagation();
        });
        //if html is clicked turn off drop down
        $('html').on('click', function(e){
            if ($('.dropdown').hasClass('active')) {
                $('.dropdown').removeClass('active');
                stopPropEvents();
            }
			$('.socialLinks').removeClass('open');
			if ($('.dropdownTrigger').hasClass('open')) {
			   $('.dropdownTrigger').removeClass('open');
			}
        });
        //if ESC is press close drop down
        $('html').on({
            'keydown': function(e){
                if (e.keyCode == 27 && $('.dropdown').hasClass('active')) {
                   $('.dropdown').removeClass('active');
				   $('.socialLinks').removeClass('open');
                    stopPropEvents();
                }
            }
        });
		
        $('.closeBtn').on('click', function(e){
            hideOverlay('bodyOverlay');
            hideEmailPopup();
        });
        $('#optinCheck').on('click', function(e){
			if ($(this).is(':checked')) {
				if($('#cableProvider').is(":hidden")) {
					$('#cableProvider').show();
					if ($('#cableProvider select').attr('disabled')) {
						$('#cableProvider select').removeAttr('disabled')
					}
				}
			}
			else {
				if($('#cableProvider').is(":visible")) {
					$('#cableProvider').hide();
				    $('#cableProvider select').attr({'disabled': 'disabled'});
				}
			}
		
		});
        //prevent body event from closing the dropdown when dropdown is clicked
        stopPropEvents();
        enterEvent();
        function toggleNav(navTriggerId,navContentId) {
		$(navContentId).toggleClass('on');
		if (!$(navContentId).is(":visible")) {
			$(navContentId).show();
		}
		else {	   
			$(navContentId).hide();
		}};
        function stopPropEvents(){
            $('#socialBtns').on('click', function(e){
                e.stopPropagation();
            });
        }
		 function enterEvent(){
    
            $('#emailSignup').on({
                
                'submit': function(){
                    if(ValidateForm('promo')){
                        newsletterRegister();
                        
                    }
                    return false;
                }
            });    

        }
		// collapsible left navigation for TCM facets 
		if ($('.view-tcm #sliNav').length || $('.device-mobile').length) {
			$("#sliNav li > span").click(function(){
						$(this).next().toggle();
			return false;
			});
			$("#sliNavFacet_studio span").next().hide();
		};
		
		// ASOTV Dropdown to accomodate second row of navigation (alphabet index) 
		  })(jQuery);
  
</script>

    
	


	
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
					
										
					
					
					
					
					
					
					
					
					
					
					
					
					
					
				


      
    <script type="text/javascript">
      jQuery.noConflict();
    </script>
    <script type="text/javascript">
    jQuery(function($) {
      
    enterEvent();
    
    //open newsletter signup dropdown on link click
    openDropdownEvent();
    
    //open register options lightbox
    $('.newsEmailBtn').on('click', function(e){
        if(ValidateForm('promomain2')){
            newsletterRegister(document.promomain2.email.value);
        }
    });
    $('.newsRegOptLightAction').on('click', function(e){
        if(ValidateForm('promomain')){
            newsletterRegister(document.promomain2.email.value);
        }
    });
    
    //close dropdown when click on close button or away from dropdown
    $('body').on('click', function(e){
        closeDropdown();
        openDropdownEvent();
        stopPropEvents();
    });
    $('.newsCloseBtn').on('click', function(e){
        closeDropdown();
        hideOverlay('bodyOverlay');
        hideNewsPopup('#newsletterPopupInfo');
        openDropdownEvent();
        stopPropEvents();
    });
    
    //update info button event    
    $('.newsUpdateBtn').on('click', function(e){
		if (ValidateForm('promo')) {
			newsletterUpdate();
			$('.newsUpdateBtn').hide();
		}
    });
    
    //prevent body event from closing the dropdown when dropdown or newlink is clicked
    stopPropEvents();
    
    function newsPopup(element, popupId, email, alignRight, evar){
        if(typeof email != 'undefined'){hideNewsPopup('#newsletterPopupEmail');}
        var ph = $(popupId).outerHeight();
        var pw = $(popupId).outerWidth();
        var p = $(popupId).detach();
        var h = $(window).height();
        var w = (element != '') ? $(element).outerWidth(true) : $(window).width();
        var scrollAmount = getYOffset();
        var itemPos = (element != '') ? $(element).position() : '';
        var posTop = h/2 - ph/2;
        var posLeft = w/2 - pw/2;
        if(alignRight) {posLeft -= pw/4;}
        if(element != ''){
            $('body').append(p);
            showOverlay('initialOverlay');
			//$(popupId).css({'top': posTop, 'left': posLeft});
            $(popupId).css({'top': posTop});
            $(popupId).css('marginLeft', '-' + $(popupId).width() / 2 + 'px');
            $(popupId).clearQueue().finish().fadeIn('500');
        }else{
            showOverlay();
            $('body').append(p);
            $(popupId).css({'top': posTop, 'left': posLeft})
                           .fadeIn('500')
                           .find("input[name='email']")
                           .val(email);
        }
		newsletterPopupImpressionTracking('delagentcbs,delagentglobalrollup',evar);
        escapeEvent(element);
    }
    
    function openDropdownEvent(){
        $('.newsPopupAction').on('click', function(e){
            newsPopup($(this), '#newsletterPopupEmail', null, null, 'eVar69');
            $(this).unbind();
            showOverlay();
            closeDropdownEvent();
            stopPropEvents();
        });
    }
    
    function closeDropdownEvent(){
        $('.newsPopupAction').on('click', function(e){
            closeDropdown();
            $(this).unbind();
            hideOverlay('bodyOverlay');
            openDropdownEvent();
            stopPropEvents();
        });
    }
    
    function stopPropEvents(){
        $('.newsPopupAction, #newsletterPopupEmail').on('click', function(e){
            e.stopPropagation();
        });
    }
    
    function escapeEvent(element){
        var $this = $(element).prop('id') + ', #newsletterPopupEmail, body';
        $($this).on({
            'keydown': function(e){
                if (e.keyCode == 27) {
                    $(element).unbind();
                    closeDropdown();
                    hideOverlay('bodyOverlay');
                    hideNewsPopup('#newsletterPopupInfo');
                    openDropdownEvent();
                    stopPropEvents();
                }
            }
        });   
    }
    
    function closeDropdown(){
        $('#newsletterPopupEmail').fadeOut('500');
    }
    
    function enterEvent(){
        $("#signupFormPopup, #signupForm").each(function(){
            var formName = $(this).prop('name');
            var formID = '#' + $(this).prop('id');
            if($(this).prop('id') == 'signupFormPopup'){
                $(formID).on({
                    'keydown': function(e){
                        if (e.keyCode == 13) {
                            //submit email on enter keypress if in the signup form popup
                            if(ValidateForm('promomain2')){
                                newsletterRegister(document.promomain2.email.value);
                                
                            }
                            
                        }
                    },
                    'submit': function(){return false;}
                });    
            }else{
            $(formID).on({
                'keydown': function(e){
                    if (e.keyCode == 13) {
                        if(ValidateForm('promomain')){
                            newsletterRegister(document.promomain.email.value);
                        }
                    }
                },
                'submit': function(){return false;}
            });   
            }
            
        });
    }
    
    function newsletterRegister(email){
        $.ajax({
            type: 'POST',
            url: '/register.php?f=register',
            data: { 'email':email},
            dataType: 'html',
            beforeSend: function(){
                hideNewsPopup('#newsletterPopupEmail');
            },
            error: ajaxErrors,
            success: function(data){
                hideOverlay('bodyOverlay');
                newsPopup('', '#newsletterPopupInfo', email, null, 'eVar70');
                escapeEvent('#newsletterPopupInfo');				dataLayer.push({'event': 'newsletterPopupSubmit'});            }
        });
    }
    
    function newsUpdateBtnEvent(){        
        $('.newsUpdateBtn').on('click', function(e){
            newsletterUpdate();
        });
    }
    
    function delayedClose() {
        closeDropdown();
        hideOverlay('bodyOverlay');
        hideNewsPopup('#newsletterPopupInfo');
        openDropdownEvent();
        stopPropEvents();
    }
    
    function newsletterUpdate(){
        $.ajax({
            type: 'POST',
            url: '/register.php?f=register',
            data: $('#newsregister').serialize(),
            dataType: 'html',
            beforeSend: function(){
                showOverlay('#newsletterPopupInfo');
                clearHideHtml('.registerComplete');
            },
            error: ajaxErrors,
            success: function(data){
                $('.registerComplete').html('Thanks for registering!').show();
            },
            complete: function(data, status){
                hideOverlay('ajaxOverlay');
                setTimeout(delayedClose, 1000);
            }
        });
    }
    
    function hideNewsPopup(popupId){
        $(popupId).fadeOut('500').find('input[type=text], select').each(function(i){
           /*if($(this).attr('name') == 'email'){
               $(this).val('Enter Your Email Address');
            }else{
                $(this).val('');
            }*/
			//$(this).prop('disabled', true);
        });
        clearHideHtml('.registerComplete');
    }
    
    function clearHideHtml(selector){
        $(selector).html('').hide();
    }
    
    function showOverlay(element){
        var overlay = (typeof element != 'undefined' && element != '') ? getOverlay('ajaxOverlay', true) : getOverlay('bodyOverlay');
        (typeof element != 'undefined' && element != '') ? $(element).append(overlay) : $('body').append(overlay);
    }
    
    function hideOverlay(id){
        var id = '#' + id;
        $(id).remove();
    }
    
    function getOverlay(id, showLoader){
        showLoader = (typeof showLoader != 'undefined') ? showLoader : false;
        var overlay = $('<div/>', {
            id: id,
            css: {
            'position': (showLoader) ? 'absolute' : 'fixed',
            'top': '0px',
            'left': '0px',
            'display': 'block',
            'opacity': (showLoader) ? '0.3' : '0.7',
            'width': '100%',
            'height': '100%', 
            'background': "#000 url('') no-repeat center center",
            'z-index': (showLoader) ? '9500' : '8500'  
            },
            click: function(e){        
                if(!showLoader){
                    hideOverlay(id);
                    hideNewsPopup('#newsletterPopupInfo');
                }
            }
        });
        return overlay;
    }
    
    function ajaxErrors(request, type, errorThrown){
      var message = "There was an error with the AJAX request.\n";
      switch (type) {
        case 'timeout':
          message += "The request timed out.";
          break;
        case 'notmodified':
          message += "The request was not modified but was not retrieved from the cache.";
          break;
        case 'parsererror':
          message += "XML/Json format is bad.";
          break;
        default:
          message += "HTTP Error (" + request.status + " " + request.statusText + ").";
      }
      message += "\n";
      alert(message);  
    }
    
    function getYOffset() {
      var pageY;
      if(typeof(window.pageYOffset)=='number') {
         pageY=window.pageYOffset;
      } else {
         pageY=document.documentElement.scrollTop;
      }
      return pageY;
    }
 
});

</script>
    

<script type="text/javascript">
    jQuery.noConflict();
    (function($) {
        
        var nlPopup = {
            init:function(popupCookie){
                nlPopup.checkCookie(popupCookie);
            },
            setCookie:function(c_name,value,exdays){
                var exdate=new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
                document.cookie=c_name + "=" + c_value + ";path=/";
            },
            checkCookie:function(c_name){
                var i, x = 0, cookieName, cookieValue, siteCookies = document.cookie.split(';');
                for(i=0; i < siteCookies.length; i++){
                    cookieName = siteCookies[i].substr(0,siteCookies[i].indexOf("="));
                    cookieValue = siteCookies[i].substr(siteCookies[i].indexOf("=")+1);
                    cookieName=cookieName.replace(/^\s+|\s+$/g,"");
                    if(cookieName === c_name){x = 1;}
                }
            
                if(x === 1){
                    return false;
                }else{
                    nlPopup.displayPopup();
                    nlPopup.setCookie(c_name, 1, 10);
                }
            },
            displayPopup:function(){
					$(window).bind("load", function() {
						$('.newsPopupAction').click();
					});
            }
        }
        

        nlPopup.init('DA_NEWSLETTER_POPUP');

	})(jQuery);
</script>
				 									

