<script type="text/javascript">

	var browserTabActive = (function(){
		var stateKey, eventKey, keys = {
			hidden: "visibilitychange",
			webkitHidden: "webkitvisibilitychange",
			mozHidden: "mozvisibilitychange",
			msHidden: "msvisibilitychange"
		};
		for (stateKey in keys) {
			if (stateKey in document) {
				eventKey = keys[stateKey];
				break;
			}
		}
		return function(c) {
			if (c) document.addEventListener(eventKey, c);
			return !document[stateKey];
		}
	})();

	var adListIn = ["leader_top","mpu_top"];

	function testViewability() {
		var navBarHeight    = $('.cbs-site-nav').outerHeight(); 
		var windowScrollTop = window.top.pageYOffset;
		var domHeight = window.top.document.documentElement.clientHeight;
		var viewPortHeight = window.innerHeight;
		adListOut = [adSlots["XFP_top_strip"]];
		
		if (!$('#leaderboard-bottom').length) {
			$('#leaderboard-wrap').after('<div id="leaderboard-bottom"></div>');
		}
		
		//var debugInfo =
		//	"Viewport: " + viewPortHeight + ' / '
		//	+ "Scroll Position Y: " + windowScrollTop + '<br>';

		for (var i = 0, len = adListIn.length; i < len; i++) {
			var adID = document.getElementById(adListIn[i]);

			if (adID.getBoundingClientRect) {
				// Internet Explorer, Firefox 3+, Google Chrome, Opera 9.5+, Safari 4+
				var rect = adID.getBoundingClientRect();
				x = rect.left;
				y = rect.top;
				w = rect.right - rect.left;
				h = rect.bottom - rect.top;

				if (adListIn[i] == 'leader_top' && h == 0) {
					h = 60;
					y = document.getElementById("leaderboard-bottom").getBoundingClientRect().top;
				}

				var skyboxMaxHeight = parseInt($('.ad-top-strip').attr('data-skyboxmaxheight')) || 0;
				var skyboxMinHeight = parseInt($('.ad-top-strip').attr('data-skyboxminheight')) || 0;
				var skyboxHeight = ( (skyboxMaxHeight - windowScrollTop) < skyboxMinHeight  )
					? skyboxMinHeight
					: skyboxMaxHeight - windowScrollTop;

				// Is the ad that we are testing below the browser viewport.
				var adBelow = ( parseInt(y + h/2) < viewPortHeight) ? false : true;
				// Is the ad that we are testing scrolled past (above) the browser viewport.
				var adNegative = ( y + h > h/2 + skyboxHeight + navBarHeight ) ? false : true;
				// If both tests return false, that means the ad is at least 50% visible in the browser viewport.
				var adInView = (adBelow==false && adNegative==false);

				if (adInView) {
					var refreshID = "XFP_" + adListIn[i];
					adListOut.push(adSlots[refreshID]);
				}

				//debugInfo += '<hr>'
				//	+ '<b>' + adListIn[i] + '</b><br>'
				//	+ " top: " + parseInt(y) +' / '
				//	+ " bottom: " + parseInt(y + h) + '<br>'
				//	+ " Below Fold: " + adBelow +'<br>'
				//	+ " Too Far: " + adNegative +'<br>'
				// 	+ " Is 50% Viewable: <b>" + adInView +'</b>';
			}
		}

		// BEGIN DEBUG RENDER
		// You may also comment out anything with degugInfo above.
		// document.title = browserTabActive() ? 'Visible' : 'Not visible';
		//if (!$('#viewDebug').length) {
		//	$('#tooltip').after('<div id="viewDebug" style="position:fixed;margin-top:250px;margin-left:400px;border:2px solid red;width:300px;z-index: 9000000; padding: 5px; background-color: white;"></div>');		
		//}
		
		//var visible = browserTabActive(); // gives current state		
		//if (visible == true) {
		//	document.title = 'Active Tab. Refresh Ads.';
        //} else {
		//	document.title = 'Inactive Tab. No Refresh.';
        //    return;
		//}
		//$('#viewDebug').html(debugInfo);
		// END DEBUG RENDER
	
		window.scrollTo(window.pageXOffset, window.pageYOffset + 1);
		googletag.pubads().refresh(adListOut);
		return;
	}
	
	var XFPinterval  = setInterval('testViewability()',300000); // 5 Minutes.
	setTimeout('clearInterval(XFPinterval)',86400000); // dont refresh after a day
</script>   
<script type="text/javascript">
CBSi.namespace("widget.Table");(function(){CBSi.widget.Table=function(id){this.id=id;this.data={};this.dataNode=null;this.dataBody=null;this.tableData=null;this.element={};this.attributes={};this.header={};this.row={};this.column=[];this.defaultSort=false;this.initialized=false;this.processed=false;this.init()};CBSi.widget.Table.prototype={init:function(){this.element=document.getElementById(this.id);this.header=this.element.getElementsByTagName("th");this.setupDataBody();this.loadAttributes()},setupDataBody:function(){if(!document.getElementById(this.id+"Data")){var headTbody=this.header[0].parentNode.parentNode;var dataTbody=document.createElement("tbody");dataTbody.setAttribute("id",this.id+"Data");if(headTbody.nextSibling){this.element.insertBefore(dataTbody,headTbody.nextSibling)}else{this.element.appendChild(dataTbody)}}},loadAttributes:function(){var defaultAttributes={enableSort:false,defaultSort:false,onSort:"CBSi.widget.Table.sort",debug:false};for(var attribute in defaultAttributes){if(!this.element.getAttribute(attribute)){this.element.setAttribute(attribute,defaultAttributes[attribute])}this.attributes[attribute]=this.element.getAttribute(attribute)}this.loadRowAttributes();this.loadColumnAttributes()},loadRowAttributes:function(){var attributes={recordId:"[]",recordMax:null,recordIndex:"[]",recordData:"[]"};var row=this.header[0].parentNode;for(var attribute in attributes){if(row.getAttribute(attribute)){this.row[attribute]=row.getAttribute(attribute)}}},loadColumnAttributes:function(){var attributes={dataAlign:"left",dataValign:"middle",dataClass:null,dataDefault:null,dataType:null,dataSort:null,dataSource:null,dataInsert:null,dataLink:null};for(var i=0;i<this.header.length;i++){this.header[i].setAttribute("index",i);this.column[i]=new Object();for(var attribute in attributes){if(this.header[i].getAttribute(attribute)){this.column[i][attribute]=this.header[i].getAttribute(attribute)}}if(!this.defaultSort&&this.attributes.defaultSort){if(this.attributes.defaultSort==this.header[i].getAttribute("index")||this.attributes.defaultSort==this.column[i]["dataSource"]||this.attributes.defaultSort==this.column[i]["dataSource"]){this.defaultSort=this.header[i]}}if((this.attributes.onSort)&&(this.attributes.enableSort=="true")){var sortFunc=this.attributes.onSort;if(sortFunc=="CBSi.widget.Table.sort"){this.header[i].onclick=this.sortFunc(this.header[i],this)}else{this.header[i].onclick=function(){eval(sortFunc+"(this)")}}}}this.findDataNode()},arraySniffer:function(attribute){var results=new Array();var matches=attribute.match(/([a-zA-Z0-9]+[\_\.]?[a-zA-Z0-9]+)+\[\]/g);if(matches){results=results.concat(matches)}else{results.push(matches)}return results},findDataNode:function(){var dataNode=new Array();for(var i=0;i<this.column.length;i++){for(var attribute in this.column[i]){switch(attribute){case"dataDefault":case"dataSource":case"dataInsert":if(this.column[i][attribute]){var match=this.arraySniffer(this.column[i][attribute]);dataNode=dataNode.concat(match)}break}}}var nodeHint=new Array();for(var i=0;i<dataNode.length;i++){var found=0;if(dataNode[i]){dataNode[i]=dataNode[i].replace("[]","")}for(var j=0;j<nodeHint.length;j++){if(nodeHint[j].node==dataNode[i]){nodeHint[j].count++;found++;break}}if(found==0){nodeHint.push(new Object());var last=nodeHint.length-1;nodeHint[last].node=dataNode[i];nodeHint[last].count=1}}var nodeOccurence=0;for(var i=0;i<nodeHint.length;i++){if(nodeHint[i].count>nodeOccurence){nodeOccurence=nodeHint[i].count;this.dataNode=nodeHint[i].node}}},validateData:function(){var result=false;this.tableData=(this.dataNode)?this.data[this.dataNode]:this.data;return result},parse:function(markup,row){var pattern=/(\{(\w+[\[\]\w\.\_\-]*(\|[\w\_\-\.]+[\:\w\'\%\_\-\s]*){0,1})\})/g;var vars=markup.match(pattern)||[];for(var i=0;i<vars.length;i++){var temp=vars[i].replace(/[{}]/g,"");var vals=(temp.match(/[\|]/))?this.dataModifier(temp,row):this.dataLookup(temp,row);if(vals==undefined){vals=(this.attributes.debug=="true")?temp:""}if((vals.constructor.toString().match(/Array|Object/))&&(temp.indexOf(this.dataNode)==0)){vals=row}markup=markup.replace(vars[i],vals)}return markup},render:function(data){this.data=data||new Array();this.validateData();var tbody=document.getElementById(this.id+"Data");while(tbody.hasChildNodes()){tbody.removeChild(tbody.lastChild)}var max=(this.tableData)?this.tableData.length:1;var max=Math.min(this.row.recordMax,max)||max;for(var row=0;row<max;row++){var tr=document.createElement("tr");tr.className=(row%2==1)?"row2":"row1";if(this.row.recordId){tr.setAttribute("id",this.recordId(row))}if(this.row.recordIndex){tr.setAttribute("index",this.recordIndex(row))}if(this.row.recordData){var keyValuePair=this.recordData(row)}for(var i in keyValuePair){tr.setAttribute("data-"+i,keyValuePair[i])}for(var col=0;col<this.column.length;col++){var dl=document.createElement("dl");var dt=document.createElement("dt");var dd=document.createElement("dd");var td=document.createElement("td");dl.style.margin="0";dt.style.display="none";dd.style.margin="0";dl.appendChild(dt);dl.appendChild(dd);td.appendChild(dl);for(var attribute in this.column[col]){switch(attribute){case"dataAlign":this.dataAlign(td,col,row);break;case"dataValign":this.dataValign(td,col,row);break;case"dataClass":this.dataClass(td,col,row);break;case"dataSort":this.dataSort(td,col,row);break;case"dataSource":this.dataSource(td,col,row);break;case"dataInsert":this.dataInsert(td,col,row);break;case"dataLink":this.dataLink(td,col,row);break}}tr.appendChild(td)}tbody.appendChild(tr)}data=null;this.data=null;this.tableData=null;if(this.defaultSort){CBSi.widget.Table.sort(this.defaultSort,true)}},dataAlign:function(td,col,row){td.align=this.column[col].dataAlign},dataValign:function(td,col,row){td.vAlign=this.column[col].dataValign},dataClass:function(td,col,row){var data=this.column[col].dataClass;data=this.parse(data,row);td.className=data},dataDefault:function(td,col,row){var data=this.column[col].dataDefault;if(data){return this.parse(data,row)}else{return""}},dataSource:function(td,col,row){var data=this.column[col].dataSource;if(data.match(/({[\w\.\-\_\|\:\[\]]+})/g)==null){data="{"+data+"}"}data=this.parse(data,row);if(this.column[col].dataSource==data&&this.column[col].dataDefault!=undefined){data=this.dataDefault(td,col,row)}var dd=td.getElementsByTagName("dd")[0];var text=document.createTextNode(data);dd.appendChild(text);var dt=td.getElementsByTagName("dt")[0];var text=document.createTextNode(data);if(dt.hasChildNodes()==false){dt.appendChild(text)}},dataInsert:function(td,col,row){var data=this.parse(this.column[col].dataInsert,row);var dd=td.getElementsByTagName("dd")[0];dd.innerHTML=data;var dt=td.getElementsByTagName("dt")[0];var text=document.createTextNode(data);if(dt.hasChildNodes()==false){dt.appendChild(text)}},dataLink:function(td,col,row){var href=this.parse(this.column[col].dataLink,row);if(href){var dd=td.getElementsByTagName("dd")[0];var a=document.createElement("a");a.innerHTML=dd.innerHTML;if(href.indexOf("javascript:")>-1){a.setAttribute("href",href)}else{a.setAttribute("href",href)}while(dd.hasChildNodes()){dd.removeChild(dd.lastChild)}dd.appendChild(a)}},dataSort:function(td,col,row){var data=this.column[col].dataSort;if(data.match(/({[\w\.\-\_\|\:\[\]]+})/g)==null){data="{"+data+"}"}data=this.parse(data,row);var dt=td.getElementsByTagName("dt")[0];var sort=document.createTextNode(data);dt.appendChild(sort)},dataLookup:function(dataVar,dataRow){var data=dataVar.replace("[]","."+dataRow);var data=data.replace(/[\[]/g,".");var data=data.replace(/[\]]/g,"");var node=data.split(".");var nest=(this.dataNode)?this.data:this.tableData[dataRow];if(nest==undefined){nest=this.data}try{switch(node.length){case 1:data=nest[node[0]];break;case 2:data=nest[node[0]][node[1]];break;case 3:data=nest[node[0]][node[1]][node[2]];break;default:var dataNode="nest";for(var i=0;i<node.length;i++){dataNode+=(isNaN(node[i]))?"['"+node[i]+"']":"["+node[i]+"]"}data=eval(dataNode)}}catch(e){}return data},dataModifier:function(data,dataRow){var item=data.split("|");if(item.length>1){var variable=item[0];var modifier=item[1];var argument=modifier.split(":");var func=argument[0];var data=this.dataLookup(variable,dataRow);switch(func){case"substr":data=data.toString();data=data.substr(argument[1],argument[2]);break;case"formatDateTime":try{var args,format;var dateTime=new Date(data*1000);args=modifier.split(func+":");args=(args.length>0)?args[1]:null;format=(args)?args.match(/'(.+)'/):null;format={format:format[1]};data=YAHOO.util.Date.format(dateTime,format)}catch(e){}break;default:data=eval(func+"(data, argument);");break}}return data},recordId:function(index){var recordAttr=this.row.recordId;if(recordAttr.match(/({[\w\.\-\_\|\:\[\]]+})/g)==null){recordAttr="{"+recordAttr+"}"}var recordId=this.id+this.parse(recordAttr,index);return recordId},recordIndex:function(index){var recordAttr=this.row.recordIndex;if(recordAttr.match(/({[\w\.\-\_\|\:\[\]]+})/g)==null){recordAttr="{"+recordAttr+"}"}var recordIndex=this.parse(recordAttr,index);return recordIndex},recordData:function(index){var recordAttr=this.row.recordData;if(recordAttr.match(/({[\w\.\-\_\|\:\[\]]+})/g)==null){recordAttr="{"+recordAttr+"}"}var recordIndex=this.parse(recordAttr,index);var recordAttrKeys=recordIndex.split("|");var recordAttrKeyValue={};for(var i=0;i<recordAttrKeys.length;i++){var keys=recordAttrKeys[i];var keyValuePair=keys.split(":");recordAttrKeyValue[keyValuePair[0]]=keyValuePair[1]}return recordAttrKeyValue},sortFunc:function(header,tableObj){return function(){CBSi.widget.Table.sort(header);tableObj.defaultSort=header}},debug:function(value){try{console.debug(value)}catch(e){}}};CBSi.widget.Table.sort=function(header,update){var sortDir=CBSi.widget.Table.sortDir(header,update);var sortIndex=header.getAttribute("index");var table=header.parentNode.parentNode.parentNode;var tableID=table.getAttribute("id");var dataBody=document.getElementById(tableID+"Data");var dataType=header.getAttribute("dataType")||"Alpha";var dataRows=new Array();for(var i=0;i<dataBody.rows.length;i++){dataRows.push(dataBody.rows[i])}table.setAttribute("sortIndex",sortIndex);switch(dataType){case"Num":dataRows.sort(CBSi.widget.Table.sortNum);break;default:dataRows.sort(CBSi.widget.Table.sortAlpha)}if(sortDir=="asc"){dataRows.reverse();header.setAttribute("sortDir","desc")}else{header.setAttribute("sortDir","asc")}CBSi.widget.Table.sortClass(table,sortIndex,sortDir);for(var i=0;i<dataRows.length;i++){dataRows[i].className=(i%2==1)?"row2":"row1";dataBody.appendChild(dataRows[i])}};CBSi.widget.Table.sortData=function(row){var index=row.parentNode.parentNode.getAttribute("sortIndex")||0;index=parseInt(index);var td=row.getElementsByTagName("td")[index];var dt=td.getElementsByTagName("dt")[0];var data=dt.firstChild.nodeValue||"";return data};CBSi.widget.Table.sortNum=function(a,b){var aa=CBSi.widget.Table.sortData(a);var bb=CBSi.widget.Table.sortData(b);aa=aa.replace(/[^0-9\+\-\.\s]/g,"");bb=bb.replace(/[^0-9\+\-\.\s]/g,"");aa=(aa)?parseFloat(aa):Number.MAX_VALUE;bb=(bb)?parseFloat(bb):Number.MAX_VALUE;aa=(isNaN(aa))?0:aa;bb=(isNaN(bb))?0:bb;return aa-bb};CBSi.widget.Table.sortAlpha=function(a,b){var aa=CBSi.widget.Table.sortData(a);var bb=CBSi.widget.Table.sortData(b);aa=aa.toLowerCase();bb=bb.toLowerCase();if(aa==bb){return 0}if(aa<bb){return -1}return 1};CBSi.widget.Table.sortDir=function(header,toggle){var sortDir=header.getAttribute("sortDir")||"asc";switch(sortDir){case"asc":if(toggle){sortDir="desc"}break;case"desc":if(toggle){sortDir="asc"}break}header.setAttribute("sortDir",sortDir);return sortDir};CBSi.widget.Table.sortClass=function(table,sortIndex,sortDir){var col=table.getElementsByTagName("col")||[];for(var i=0;i<col.length;i++){var className=col[i].className||"";className=className.replace(/ sortasc| sortdesc/gi,"");if(i==sortIndex){className+=" sort"+sortDir}col[i].className=className}}})();
//-----------------------------------------------------------------------------
var Leaderboard = {
//-----------------------------------------------------------------------------
    tid             : null,
    year            : 2011,
    round           : 1,
    tourCode        : "PGA",
    seoLink         : "pga-tour",
    inProgress      : false,
    leaderId        : null,
    majorStats      : null,
    profile         : [],
    sponsors        : [],
    highlight       : {},
    alerts          : [],
    dataSet         : null,
    dataStatus      : {"default":1,"leaders":1,"course":1,"alerts":1},
    leadersSet      : null,
    table           : null,
    colspan         : 0,
    sortby          : "sortindex",
    statTables      : {},
    openScorecard   : {},
 //-----------------------------------------------------------------------------
    showInitial: function(firstname) {
//-----------------------------------------------------------------------------
	    
	    var initial = firstname.substring(0,1) + ".";
	    return(initial);
	    
    },

    //-----------------------------------------------------------------------------
    init: function () {
    //-----------------------------------------------------------------------------
        Leaderboard.renderSchedule();
        Leaderboard.controller();
        Leaderboard.auxillary();
        setTimeout(function(){
            Leaderboard.displayProjectedCut();
           
        }, 1000);
    },

    //-----------------------------------------------------------------------------
    controller: function (round) {
    //-----------------------------------------------------------------------------
    // The logic traffic cop, which determines the order of execution
        round = (round == null) ? Leaderboard.round : round

        Leaderboard.fetchData("default", round);
        Leaderboard.fetchData("leaders", round);
        Leaderboard.update();
    },

    //-----------------------------------------------------------------------------
    auxillary: function () {
    //-----------------------------------------------------------------------------
        setInterval('Leaderboard.fetchAlerts()', 220000);
        setInterval('Leaderboard.fetchHoleStats()', 150000);
    },

    //-----------------------------------------------------------------------------
    update: function (interval) {
    //-----------------------------------------------------------------------------
        interval = (interval == null) ? 240000 : interval;

        if (Leaderboard.inProgress == false) {
            Leaderboard.inProgress = setInterval('Leaderboard.controller()', interval);
        }
        else {
            if (Leaderboard.dataSet.status == "Official") clearInterval(Leaderboard.inProgress);
        }
    },

    //-----------------------------------------------------------------------------
    fetchData: function (type, round) {
    //-----------------------------------------------------------------------------
        type  = (type == null)  ? "default" : type;
        round = (round == null) ? Leaderboard.round : round;

        var rnd      = "r"+round;
        var year     = Leaderboard.year;
        var tid      = Leaderboard.tid;
        var stat     = Leaderboard.dataStatus[type] || 0;
        var url      = "/golf/leaderboard/data/"+type+"/"+year+"/"+tid+"/"+rnd+"/"+stat+"?debug=2";
        var callback = {'success':function (o) {Leaderboard.loadData(type, round, o);}};
        var request  = YAHOO.util.Connect.asyncRequest("GET", url, callback);
    },

    //-----------------------------------------------------------------------------
    fetchScorecard: function (playerId) {
    //-----------------------------------------------------------------------------
        var year     = Leaderboard.year;
        var tid      = Leaderboard.tid;
        var url      = "/golf/leaderboard/data/scorecard/"+year+"/"+tid+"/"+playerId;
        var callback = {'success':function (o) {Leaderboard.renderScorecard(playerId, o)}};
        var request  = YAHOO.util.Connect.asyncRequest("GET", url, callback);
    },

    //-----------------------------------------------------------------------------
    fetchPlayerSkills: function (playerId) {
    //-----------------------------------------------------------------------------
        var year     = Leaderboard.year;
        var tid      = Leaderboard.tid;
        var url      = "/golf/leaderboard/data/skills/"+year+"/"+tid+"/"+playerId;
        var callback = {'success':function (o) {Leaderboard.skillsGauge(playerId, o)}};
        var request  = YAHOO.util.Connect.asyncRequest("GET", url, callback);
    },

    //-----------------------------------------------------------------------------
    fetchHoleStats: function () {
    //-----------------------------------------------------------------------------
        var year     = Leaderboard.year;
        var tid      = Leaderboard.tid;
        var round    = Leaderboard.round;
        var stat     = Leaderboard.dataStatus['course'] || 0;
        var url      = "/golf/leaderboard/data/course/"+year+"/"+tid+"/"+round+"/"+stat;
        var callback = {'success':function (o) {Leaderboard.renderCourseStats(o)}};
        var request  = YAHOO.util.Connect.asyncRequest("GET", url, callback);
    },

    //-----------------------------------------------------------------------------
    fetchAlerts: function () {
    //-----------------------------------------------------------------------------
        var year     = Leaderboard.year;
        var tid      = Leaderboard.tid;
        //var tid      = 1186102;
        var round    = Leaderboard.round;
        var stat     = Leaderboard.dataStatus['alerts'] || 0;
        var url      = "/golf/leaderboard/data/alerts/"+year+"/"+tid+"/"+round+"/"+stat;
        var callback = {'success':function (o) {Leaderboard.renderAlerts(o)}};
        var request  = YAHOO.util.Connect.asyncRequest("GET", url, callback);
    },

    //-----------------------------------------------------------------------------
    loadData: function (type, round, o) {
    //-----------------------------------------------------------------------------
        try {
            var text = o.responseText.match(/<!\-\-([0-9]*)\-\->/);
            var find = text[0];
            var time = text[1];
            var text = o.responseText.replace(find, "");
            var data = eval("("+text+")");
            Leaderboard.dataStatus[type] = time;
            switch (type) {
                case "leaders":
                    Leaderboard.leadersSet  = data;
                    Leaderboard.renderStatLeaders();
                    break;
                default:
                    Leaderboard.round       = round;
                    Leaderboard.dataSet     = data;
                    //console.log(">>>>"+ data);
                    Leaderboard.render();
                    if (Leaderboard.tourCode == "PGA") Leaderboard.renderLeader();
                    break;
            }
        }
        catch (e) {}//Unable to load data
    },

    //-----------------------------------------------------------------------------
    render: function () {
    //-----------------------------------------------------------------------------
        if (!Leaderboard.table) {
            Leaderboard.renderTitle();
            Leaderboard.buildTable();
            Leaderboard.addHeader("Leaderboard");
            Leaderboard.table = new CBSi.widget.Table("Leaderboard");
        }
        if (Leaderboard.dataSet.status == "Official" && Leaderboard.dataSet.currentround == Leaderboard.dataSet.numberofrounds)
            Leaderboard.table.element.className = "data noMargin condensed";
        else
            Leaderboard.table.element.className = "data noMargin condensed";

        Leaderboard.hideAllScorecards();
        Leaderboard.removeProjectedCut();
        //console.log(">>>>>" + Leaderboard.sortby);
        Leaderboard.removeAdRow();
        Leaderboard.highlightRound();
        
        //truncate first name to initial
        for(var x=0; x<Leaderboard.dataSet.players.length; x++){
	       var initial =  Leaderboard.showInitial(Leaderboard.dataSet.players[x].firstname + ".");
	        Leaderboard.dataSet.players[x].firstname = initial;
        }
        
        for(var y=0; y<Leaderboard.dataSet.players.length; y++){
	      var featured =  Leaderboard.dataSet.players[y].featured;
	      if(featured) {
		      Leaderboard.dataSet.players[y].featured = "<a href=\""+Leaderboard.dataSet.players[y].featuredurl+"\" target=\"_new\"><span class=\"featuredLink\"></span></a>";
	      } else {
		       Leaderboard.dataSet.players[y].featured = "";
	      }
	        
        }
        
/*
        for(var x=0; x<Leaderboard.dataSet.players.length; x++){
	     	       console.log(Leaderboard.dataSet.players[x].featured);
        }
        
*/
        Leaderboard.table.render(Leaderboard.dataSet.players);
        Leaderboard.displayAdRow();
        //Leaderboard.displayProjectedCut();
        Leaderboard.showAllScorecards();
        setTimeout(function(){ //bandaid for function firing off before the dom
             Leaderboard.playerHighlight();
            },500);
       
    },

    //-----------------------------------------------------------------------------
    renderLeader: function () {
    //-----------------------------------------------------------------------------
        
        document.getElementById("Leader").style.display = "block";
        var state = (Leaderboard.dataSet.status == "Official" && Leaderboard.dataSet.currentround == Leaderboard.dataSet.numberofrounds) ? "Winner" : "Leader";
        //LeaderName
        var playerId   = Leaderboard.dataSet.players[0].id;
        var playerName = Leaderboard.dataSet.players[0].firstname + " " + Leaderboard.dataSet.players[0].lastname;
        var playerLink = "/golf/players/playerpage/" + playerId;
        var LeaderName = "<a href="+playerLink+">"+playerName+"</a>";
        document.getElementById("LeaderName").innerHTML = state+": "+LeaderName;
        //LeaderPhoto
        var playerImg = "http://sports.cbsimg.net/images/golf/players/60x80/"+playerId+".jpg";
        var LeaderPhoto = "<a href="+playerLink+"><img src="+playerImg+"></a>";
        document.getElementById("LeaderPhoto").innerHTML = LeaderPhoto;
        //LeaderLink
        var LeaderLink = "/golf/players/playerpage/" + playerId; //+ "/" + Leaderboard.tid + "/" + Leaderboard.year + "/" + Leaderboard.tourCode;
        document.getElementById("LeaderLink").setAttribute("href", LeaderLink);
        
        //LeaderStats
        if (Leaderboard.majorStats) {
            setTimeout(function(){
                Leaderboard.renderMajorStats();
            }, 1000);
            
            
        } else {
            
            Leaderboard.renderLeaderStats();
            
        }

        //Run Skills Gauge
        if (playerId != Leaderboard.leaderId) {
            Leaderboard.leaderId = playerId;
            
            Leaderboard.fetchPlayerSkills(playerId);
        }
    },

    //-----------------------------------------------------------------------------
    renderLeaderStats: function () {
    //-----------------------------------------------------------------------------

        var LeaderStats = document.getElementById("LeaderStats");
        var LeaderPhoto = document.getElementById("LeaderPhoto");
        var th = LeaderStats.getElementsByTagName("th");
        var trHeader = document.getElementById("LeaderStatsHeader");
        var trFooter = document.getElementById("LeaderStatsFooter");
        var tbody = trFooter.parentNode;
        //Remove rows between header and footer
        while (trHeader.nextSibling != trFooter) tbody.removeChild(trHeader.nextSibling);

        var stats = new Array();
        if (Leaderboard.dataSet.players[0].statsavailable) {
            stats.push(["Tournament Stats","No.","Rank"]);
            stats.push(["Driving Distance","avgdrivingdistance","avgdrivingdistancerank"]);
            stats.push(["Driving Accuracy","drivingaccuracy","drivingaccuracyrank"]);
            stats.push(["Greens In Regulation","greensinreg","greensinregrank"]);
            stats.push(["Putting Average","puttspergir","puttspergirrank"]);
            //stats.push(["Average Birdies","avgbirdies","avgbirdiesrank"]);
            //stats.push(["Driving Accuracy","drivingaccuracy","drivingaccuracyrank"]);
            //stats.push(["Driving Accuracy","drivingaccuracy","drivingaccuracyrank"]);
        }
        else {
            stats.push(["Score Today","Thru","Total"]);
            stats.push(["today","thru","trnstokes"]);
        }

        for (var i = 0; i < stats.length; i++) {
            if (i > 0) {
                var tr = document.createElement("tr");
                tr.className = (j%2 == 0) ? "row1" : "row2";;
                tbody.insertBefore(tr, trFooter);
            }
            for (var j = 0; j < stats[i].length; j++) {
                var data = stats[i][j];
                if (i == 0) th[j+1].innerHTML = data;
                else {
                    var td = document.createElement("td");
                    td.innerHTML = (Leaderboard.dataSet.players[0][data] == undefined) ? data : Leaderboard.dataSet.players[0][data];
                    tr.appendChild(td);
                    td = null;
                }
            }
        }

        LeaderPhoto.rowSpan = stats.length;
    },

    //-----------------------------------------------------------------------------
    renderMajorStats: function () {
    //-----------------------------------------------------------------------------
        var playerId = Leaderboard.dataSet.players[0].id;        
        var LeaderStats = document.getElementById("LeaderStats");
        var LeaderPhoto = document.getElementById("LeaderPhoto");
        var th = LeaderStats.getElementsByTagName("th");
        var trHeader = document.getElementById("LeaderStatsHeader");
        var trFooter = document.getElementById("LeaderStatsFooter");
        var tbody = trFooter.parentNode;
        //Remove rows between header and footer
        while (trHeader.nextSibling != trFooter) tbody.removeChild(trHeader.nextSibling);

        var stats = new Array();

        var playerStats = Leaderboard.majorStats.leader.player[playerId].stat;
        
        var headCount = 1;
        var statCount = 0;
        var trStats = new Array();
        for (var i = 0; i < playerStats.length; i++) {
            if (th[headCount] == undefined) {
                var newTh = document.createElement("th");
                th[0].parentNode.appendChild(newTh);
            }
            th[headCount].className = "major";
            th[headCount].innerHTML = playerStats[i]["label"];
            headCount++;

            //Populate stats
            for (var j = 0; j < playerStats[i]["value"].length; j++) {
                if (trStats[j] == undefined) {
                    var tr = document.createElement("tr");
                    tr.className = (j%2 == 0) ? "row1" : "row2";
                    tbody.insertBefore(tr, trFooter);
                    trStats.push(tr);
                    statCount++;
                }
                var td = document.createElement("td");
                td.appendChild(document.createTextNode(playerStats[i]["value"][j]));
                trStats[j].appendChild(td);
            }
        }

        LeaderPhoto.rowSpan = statCount + 1;
        trFooter.getElementsByTagName("td")[0].colSpan = headCount;
    },
    


    //-----------------------------------------------------------------------------
    buildTable: function () {
    //-----------------------------------------------------------------------------
        var round = Leaderboard.dataSet.currentround - 1;
        var table = document.getElementById("Leaderboard");
        var tr = table.getElementsByTagName("tr")[0];

        for (var i = 0; i < Leaderboard.profile.length; i++) {
	        //console.log(Leaderboard.profile[i]);
        switch (Leaderboard.profile[i]) {
            case "scorecard":
                var th = document.createElement("th");
                th.innerHTML = "Card";
                th.setAttribute("dataClass", "scorecard");
                th.setAttribute("dataType", "Num");
                th.setAttribute("dataSort", "sortindex");
                th.setAttribute("dataInsert", "<img src='http://sports.cbsimg.net/images/golf/leaderboard/btn_open.gif'/>");
                th.setAttribute("dataLink", "javascript:Leaderboard.showScorecard('Leaderboard{id}');");
                th.setAttribute("dataAlign", "center");
                th.setAttribute("width", "20");
                tr.appendChild(th);
                th = null;
                break;
            case "pos":
                var th = document.createElement("th");
                th.innerHTML = "Pos";
                th.setAttribute("dataType", "Num");
                th.setAttribute("dataSort", "sortindex");
                th.setAttribute("dataSource", "currentpos");
                th.setAttribute("dataAlign", "center");
                th.setAttribute("width", "30");
                tr.appendChild(th);
                th = null;
                break;
            case "country":
                var th = document.createElement("th");
                th.innerHTML = "Ctry";
                th.setAttribute("dataType", "Alpha");
                th.setAttribute("dataSort", "countryCode");
                th.setAttribute("dataClass", "countryCode");
                th.setAttribute("dataInsert", "<div class='{countryCode}' title='{countryName|Leaderboard.preSort:countryName}'/>");
                th.setAttribute("dataAlign", "center");
                th.setAttribute("width", "30");
                tr.appendChild(th);
                th = null;
                break;
            case "playername":
                var th = document.createElement("th");
                th.innerHTML = "Player Name";
                th.setAttribute("dataClass", "playername");
                th.setAttribute("dataType", "Alpha");
                th.setAttribute("dataSort", "lastname");
                th.setAttribute("dataInsert", "{firstname} {lastname} {coursecode|Leaderboard.preSort:course}");
                th.setAttribute("dataLink", "/golf/players/playerpage/"+"{id}");
                th.setAttribute("width", "200");
                th.setAttribute("align", "left");
                tr.appendChild(th);
                
                th = null;
                break;
            case "featured" :
            console.log(Leaderboard.dataSet.players.featured);
            	var th = document.createElement("th");
            	th.innerHTML = "Live";
            	th.setAttribute("dataClass", "playername");
            	th.setAttribute("dataType", "Alpha");
	            th.setAttribute("dataInsert", "{featured}");        	
            	th.setAttribute("dataLink", "{featuredurl}");
                th.setAttribute("width", "40");
                th.setAttribute("align", "center");
                tr.appendChild(th);
                th = null;
            break;
            case "earnings":
                if (Leaderboard.dataSet.status == "Official" && Leaderboard.dataSet.currentround == Leaderboard.dataSet.numberofrounds) {
                var th = document.createElement("th");
                th.innerHTML = "Earnings";
                th.className = "earnings";
                th.setAttribute("dataClass", "earnings");
                th.setAttribute("dataType", "Num");
                th.setAttribute("dataSource", "projmoney");
                tr.appendChild(th);
                th = null;
                }
                break;
            case "today":
                var th = document.createElement("th");
                th.innerHTML = "Today";
                th.setAttribute("dataType","Num");
                th.setAttribute("dataSort","{today|Leaderboard.preSort:today}");
                th.setAttribute("dataSource","today");
                th.setAttribute("dataAlign","center");
                th.setAttribute("width","64");
                tr.appendChild(th);
                th = null;
                break;
            case "thru":
                var th = document.createElement("th");
                th.innerHTML = "Thru";
                th.setAttribute("dataType","Num");
                th.setAttribute("dataSort","{thru|Leaderboard.preSort:thru}");
                th.setAttribute("dataInsert","{thru} {rounds."+round+".backninestart|Leaderboard.preSort:backnine}");
                th.setAttribute("dataAlign","center");
                th.setAttribute("width","45");
                tr.appendChild(th);
                th = null;
                break;
            case "topar":
                var th = document.createElement("th");
                th.innerHTML = "To Par";
                th.setAttribute("dataType","Num");
                th.setAttribute("dataSort","sortindex");
                th.setAttribute("dataSource","trnparrelative");
                th.setAttribute("dataAlign","center");
                th.setAttribute("dataClass","topar");
                th.setAttribute("width","45");
                tr.appendChild(th);
                th = null;
                break;
            case "rounds":
                Leaderboard.addRounds("Leaderboard");
                break;
            case "total":
                var th = document.createElement("th");
                th.innerHTML = "Total<br>Score";
                th.setAttribute("dataType","Num");
                th.setAttribute("dataSort","sortindex");
                th.setAttribute("dataSource","trnstokes");
                th.setAttribute("dataAlign","right");
                th.setAttribute("width","50");
                tr.appendChild(th);
                th = null;
                break;
        }
        }
        Leaderboard.colspan = tr.getElementsByTagName("th").length;
        tr = null;
    },
    

    //-----------------------------------------------------------------------------
    addHeader: function (id) {
    //-----------------------------------------------------------------------------
    // Add the two header rows to the Leaderboard table with tournament info.
    // Since the number of rounds will alter the number of columns on the table,
    // thereby affecting the colspan of the header column, we perform this action
    // after addRounds.

        var table   = document.getElementById(id);
        var tbody   = table.getElementsByTagName("tbody")[0];
        var title   = document.createElement("div");
        var tableTh = table.getElementsByTagName("th");
        var tableTr = tableTh[0].parentNode;
        var titleTr = document.createElement("tr");
        var titleTd = document.createElement("td");
        titleTd.colSpan = tableTh.length;
        title.setAttribute("id", "RoundTitle");
        titleTd.appendChild(title);
        /*
        //Course Info
        var course    = document.createElement("p");
        var courseTxt = new Array();
        for (var id in Leaderboard.dataSet.courses) {
            if (Leaderboard.dataSet.courses[id].par)
                courseTxt.push(Leaderboard.dataSet.courses[id].name+": Par "+Leaderboard.dataSet.courses[id].par);
        }
        course.appendChild(document.createTextNode(courseTxt.join(", ")));
        titleTd.appendChild(course);
        */
        //Status of Play
        var state = document.createElement("div");
        var stateLabel = "Status of play: " + Leaderboard.dataSet.status;
        state.setAttribute("id", "StateOfPlay");
        state.appendChild(document.createTextNode(stateLabel));
        titleTd.appendChild(state);

        titleTr.className = "title";
        titleTr.appendChild(titleTd);
        tbody.insertBefore(titleTr, tableTr);

        //Highlight info
        if (Leaderboard.tourCode == "PGA") {
            var hliteTr = document.createElement("tr");
            var hliteTd = document.createElement("td");
            var hliteHTML  = new Array();
            var hliteLink  = '/customize/players?default_tab=PGA&xurl=/golf/leaderboard/'+Leaderboard.seoLink+'/'+Leaderboard.tid+'/'+Leaderboard.round+'/'+Leaderboard.year;
            hliteHTML.push('<a href="/customize/players?default_tab=PGA&xurl=/golf/leaderboard/'+Leaderboard.seoLink+'/'+Leaderboard.tid+'/'+Leaderboard.round+'/'+Leaderboard.year+'"></a>');
            //hliteHTML.push('<img src="http://sports.cbsimg.net/images/golf/leaderboard/img_favs.gif">');
            hliteHTML.push('<span style="float: right">Page refreshes every four minutes.</span>');
            hliteTd.innerHTML = hliteHTML.join("");
            hliteTd.colSpan = tableTh.length;
            hliteTr.appendChild(hliteTd);
            hliteTr.className = "highlight row1";
            tbody.insertBefore(hliteTr, tableTr);
        }

    },

    //-----------------------------------------------------------------------------
    addRounds: function (id) {
    //-----------------------------------------------------------------------------
    // The number of rounds can vary from tournament to tournament, so this method
    // dynamically inserts the round columns into the Leaderboard table.
    //-----------------------------------------------------------------------------
        var table = document.getElementById(id);
        tr = table.getElementsByTagName("tr")[0];
        for (var i = 0; i < Leaderboard.dataSet.numberofrounds; i++) {
            round = document.createElement("th");
            round.setAttribute("dataType", "Round");
            round.setAttribute("dataSource", "rounds."+i+".strokes");
            round.setAttribute("dataDefault", "-");
            round.setAttribute("width", 30);
            round.setAttribute("dataAlign", "right");
            round.setAttribute("align", "right");
            round.appendChild(document.createTextNode("R"+(i+1)));
            tr.appendChild(round);
            round = null;
        }
    },

    //-----------------------------------------------------------------------------
    highlightRound: function () {
    //-----------------------------------------------------------------------------
        var title = document.getElementById("RoundTitle");
        title.innerHTML = "Round "+Leaderboard.round;
        var table = document.getElementById("Leaderboard");
        var th = table.getElementsByTagName("th");
        for (var i = 0; i < th.length; i++) {
            if (th[i].getAttribute("dataType") == "Round") {
                var data = th[i].getAttribute("dataSource").split(".");
                var round = parseInt(data[1]) + 1;
                if (round == Leaderboard.round) th[i].className = "selected";
                else th[i].className = "";
            }
        }
    },

    //-----------------------------------------------------------------------------
    showScorecard: function (id) {
    //-----------------------------------------------------------------------------
        var leaderboard = document.getElementById('Leaderboard');
        var playerId    = id.replace('Leaderboard', '');

        //Prep row for Scorecard display
        var tr = document.getElementById(id);
        tr.className = (tr.className) ? tr.className + " scorecard" : " scorecard";
        tr.vAlign = "top";
        tr.setAttribute("valign","top");
        var container = leaderboard.parentNode;

        //Create Scorecard container
        var div  = document.createElement("div");
        var left = leaderboard.clientLeft;

        div.setAttribute("id", "Scorecard" + playerId);
        div.className = "scorecard";
        container.appendChild(div);
        div.style.top  = leaderboard.offsetTop + tr.offsetTop + 20 + "px";
        div.style.left = left + "px";

        //Toggle trigger
        var anchor = tr.getElementsByTagName("a")[0];
        var image  = anchor.getElementsByTagName("img")[0];
        if (image.getAttribute("src") == "http://sports.cbsimg.net/images/golf/leaderboard/btn_open.gif") {
            image.setAttribute("src", "http://sports.cbsimg.net/images/golf/leaderboard/btn_close.gif");
            anchor.onclick = function () { Leaderboard.hideScorecard(id, true); return false; };
        }

        //Save open scorecard and reorder other open scorecards
        Leaderboard.openScorecard[id] = true;
        Leaderboard.reorderScorecards();
        Leaderboard.fetchScorecard(playerId);
    },

    //-----------------------------------------------------------------------------
    showAllScorecards: function () {
    //-----------------------------------------------------------------------------
        for (var playerId in Leaderboard.openScorecard) Leaderboard.showScorecard(playerId);
    },

    //-----------------------------------------------------------------------------
    hideScorecard: function (id, userEvent) {
    //-----------------------------------------------------------------------------
        var leaderboard = document.getElementById('Leaderboard');
        var playerId = id.replace('Leaderboard', '');

        //Did the user request to hide this graph
        userEvent = (userEvent == null) ? false : userEvent;

        //Garbage collect profile
        var tr = document.getElementById(id);
        tr.className = (tr.className) ? tr.className.replace(" scorecard", "") : "";
        var container = leaderboard.parentNode;
        var div = document.getElementById("Scorecard"+playerId);
        if (div) container.removeChild(div);

        //Toggle trigger
        var anchor = tr.getElementsByTagName("a")[0];
        var image  = anchor.getElementsByTagName("img")[0];
        if (image.getAttribute("src") == "http://sports.cbsimg.net/images/golf/leaderboard/btn_close.gif") {
            image.setAttribute("src", "http://sports.cbsimg.net/images/golf/leaderboard/btn_open.gif");
            anchor.onclick = function () { Leaderboard.showScorecard(id); return false; };
        }

        //Delete open scorecard and reorder other open scorecards
        if (userEvent) {
            delete Leaderboard.openScorecard[id];
            Leaderboard.reorderScorecards();
        }
    },

    //-----------------------------------------------------------------------------
    hideAllScorecards: function () {
    //-----------------------------------------------------------------------------
        for (var playerId in Leaderboard.openScorecard) Leaderboard.hideScorecard(playerId);
    },

    //-----------------------------------------------------------------------------
    reorderScorecards: function () {
    //-----------------------------------------------------------------------------
        for (var id in Leaderboard.openScorecard) {
            var leaderboard = document.getElementById("Leaderboard");
            var tr = document.getElementById(id);
            var playerId = id.replace('Leaderboard', '');
            var scorecard = document.getElementById("Scorecard"+playerId);
            if (scorecard) scorecard.style.top = leaderboard.offsetTop + tr.offsetTop + 20 +'px';
        }
    },

    //-----------------------------------------------------------------------------
    renderScorecard: function (playerId, o) {
    //-----------------------------------------------------------------------------
    
    //here is where we need to render the table
    	
    	var obj = JSON.parse(o.responseText);
    	console.log("scorecard:" + JSON.stringify(obj));
        var div = document.getElementById("Scorecard"+playerId);
                
                var currRound = Leaderboard.round;
                var holes = obj.rounds[(currRound - 1)].holes;
        
        		var str='<table class="data condensed" width="100%"><tbody><tr class="subtitle" style="display: none;"><td colspan="25">Round 1 - El Camaleon Golf Club</td> </tr>';
                str +='<tr class="row1" align="right">';
				str +='<td class="subtitle" align="left">Hole</td>';
				if(holes[0].number ==1){
    				str +='<td class="str" align="center"><b>1</b></td>';
				} else {
    				str +='<td class="subtitle" align="center"><b>1</b></td>';
				}
				
				str +='<td class="subtitle" align="center"><b>2</b></td>';
				str +='<td class="subtitle" align="center"><b>3</b></td>';
				str +='<td class="subtitle" align="center"><b>4</b></td>';
				str +='<td class="subtitle" align="center"><b>5</b></td>';
				str +='<td class="subtitle" align="center"><b>6</b></td>';
				str +='<td class="subtitle" align="center"><b>7</b></td>';
				str +='<td class="subtitle" align="center"><b>8</b></td>';
				str +='<td class="subtitle" align="center"><b>9</b></td>';
				str +='<td class="subtitle" align="center"><b>OUT</b></td>';
				if(holes[0].number ==10){
                  str +='<td class="str" align="center"><b>10</b></td>';
				} else {
    			  str +='<td class="subtitle" align="center"><b>10</b></td>';
				}
				
				str +='<td class="subtitle" align="center"><b>11</b></td>';
				str +='<td class="subtitle" align="center"><b>12</b></td>';
				str +='<td class="subtitle" align="center"><b>13</b></td>';
				str +='<td class="subtitle" align="center"><b>14</b></td>';
				str +='<td class="subtitle" align="center"><b>15</b></td>';
				str +='<td class="subtitle" align="center"><b>16</b></td>';
				str +='<td class="subtitle" align="center"><b>17</b></td>';
				str +='<td class="subtitle" align="center"><b>18</b></td>';
				str +='<td class="subtitle" align="center"><b>IN</b></td>';
				str +='<td class="subtitle" align="center"><b>TOT</b></td>';
				str +='</tr>';
				str +='<tr class="row2" align="right">';
				str +='<td class="subtitle" align="left">PAR</td>';

				
				//console.log(currRound);
				
				//console.log(holes[0].holes);
				//console.log(holes[1].holes);
				var length = holes.length;
				//console.log(length);
				
				//console.log(holes);
				var outpar = 0;
				var inpar = 0;
				
				//this rounds par
				for(var i=0; i<9; i++) {
					str +='<td class="subtitle" align="center"><b>'+holes[i].par+'</b></td>';
					outpar = (outpar + holes[i].par);
				}
				
				str +='<td class="subtitle" align="center"><b>'+outpar+'</b></td>';
				
				for(var i=9; i<holes.length; i++) {
					str +='<td class="subtitle" align="center"><b>'+holes[i].par+'</b></td>';
					inpar = (inpar + holes[i].par);
				}
				
				var totalpar = (inpar + outpar);
				str +='<td class="subtitle" align="center"><b>'+inpar+'</b></td>';
				str +='<td class="subtitle" align="center"><b>'+totalpar+'</b></td>';
				str +='</tr>';
				str +='<tr class="row1" align="right">';
				
				//this rounds yardage
				str +='<td class="subtitle" align="left">YARDAGE</td>';
				
				for(var i=0; i<9; i++) {
					str +='<td class="subtitle" align="center"><b>'+holes[i].officialyards+'</b></td>';
				}
				
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				
				for(var i=9; i<holes.length; i++) {
					str +='<td class="subtitle" align="center"><b>'+holes[i].officialyards+'</b></td>';
				}
				
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				str +='</tr>';
				
				//this rounds score per hole
				str +='<tr class="row2" align="right">';
				var outscore = 0;
				var inscore = 0;
				str +='<td class="subtitle" align="left">Rnd '+currRound+'</td>';
				
				//for this part we need to determine front and back nine
				
                var frontNineStr = [];
				var backNineStr = [];
				
				for(var i=0; i<holes.length; i++ ) {
                    var scoreClass = Leaderboard.getScoreClass(holes[i].score, holes[i].par);
                    console.log("score" + score);
					var score = (holes[i].score == 0)? 0 : holes[i].score;
    				if(holes[i].number < 10) {

        				frontNineStr.push({"str" : '<td class="subtitle '+ scoreClass +' " align="center"><b>'+score+'</b></td>', "score": score, "parrel": holes[i].parrel});
        				
    				} else if (holes[i].number > 9) {
        				
        				backNineStr.push({"str" : '<td class="subtitle '+ scoreClass +' " align="center"><b>'+score+'</b></td>', "score": score, "parrel": holes[i].parrel});
    				}

				}
				
				
				console.log(frontNineStr);
				console.log(backNineStr);
				
				for(var i=0; i<9; i++) {
					//var scoreClass = Leaderboard.getScoreClass(holes[i].score, holes[i].par);
					var score = (holes[i].score == 0)? "0" : holes[i].score;
					//str +='<td class="subtitle '+ scoreClass +' " align="center"><b>'+score+'</b></td>';
					str += frontNineStr[i].str;
					//outscore = (outscore + holes[i].score);
					outscore = (outscore + frontNineStr[i].score);
					if(frontNineStr[i].score != "-" ) {
						outscore = (outscore);
					}
				}
				
				str +='<td class="subtitle" align="center"><b>'+outscore+'</b></td>';
				
				for(var i=9; i<holes.length; i++) {
    				console.log(backNineStr[i -9].str);
					//var scoreClass = Leaderboard.getScoreClass(holes[i].score, holes[i].par);
					var score = (holes[i].score == 0)? 0 : holes[i].score;
					str += backNineStr[i-9].str;
					//str +='<td class="subtitle ' + scoreClass +' " align="center"><b>'+score+'</b></td>';
					if(backNineStr[i-9] != "-" ) {
						inscore = (inscore + score);
					}
					
				}
				console.log("inscore:" + inscore + " " + "outscore:" + outscore );
				var totalscore = (inscore + outscore);
				str +='<td class="subtitle" align="center"><b>'+inscore+'</b></td>';
				str +='<td class="subtitle" align="center"><b>'+totalscore+'</b></td>';
				str +='</tr>';
				
				//status
				str +='<tr class="row1" align="right">';
				str +='<td class="subtitle" align="left">Status</td>';
				
				for(var i=0; i<9; i++) {
					var status = (frontNineStr[i].score == 0)? "-" : frontNineStr[i].parrel;
					str +='<td class="subtitle" align="center"><b>'+status+'</b></td>';
				}
				
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				
				for(var i=9; i<holes.length; i++) {
					var status = (backNineStr[i-9].score == 0)? "-" : backNineStr[i-9].parrel;
					str +='<td class="subtitle" align="center"><b>'+status+'</b></td>';
				}
				
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				str +='<td class="subtitle" align="left">&nbsp;</td>';
				str +='</tr>';
				
				//str +='</table>';

        if (div) div.innerHTML = str; //o.responseText;
        
        div.style.backgroundImage = "url(http://sports.cbsimg.net/images/golf/players/60x80/"+playerId+".jpg)";
        div.style.backgroundRepeat = "no-repeat";

        var tbody = div.getElementsByTagName("tbody");
        if (tbody.length >= 1) {
        var tr = tbody[0].getElementsByTagName("tr");

        //Display only the current round info
        var totalRows = Leaderboard.round * 6;
        var startRows = tr.length - totalRows;
        var finalRows = startRows + 6;
        var colspan = 0;
/*
        for (var i = 0; i < tr.length; i++) {
            if (i <= startRows || i >= finalRows) {
                tr[i].style.display = "none";
            }
            else {
                colspan = (colspan > tr[i].cells.length) ? colspan : tr[i].cells.length;
            }
        }
*/

        //Remove extraneous scorecard data
        var scorecard = tbody[0].parentNode.parentNode;
        while ( scorecard.nextSibling ) div.removeChild( scorecard.nextSibling );

        //Player Links
        var footerRow = document.createElement("tr");
        footerRow.className = "footer";
        var footer = document.createElement("td");
        try { footer.colSpan = colspan; } catch (e) {}
        footer.setAttribute("colspan", 20);//colspan);
        var playerLink = '<a href="/golf/players/playerpage/'+playerId+'">Player Profile</a>';
        playerLink += ' | <a href="/golf/players/scorecard/'+playerId+'/'+Leaderboard.tid+'/'+Leaderboard.year+'/'+Leaderboard.tourCode+'">Full Scorecard and Stats</a>';
        footer.innerHTML = playerLink;
        footerRow.appendChild(footer);
        tbody[0].appendChild(footerRow);

        }

    },
    
    getScoreClass: function (score, par) {
	    
				
			    if (score == 0) {
			        return '';
			    }
			
			    var scoreClass = '';
			
			    // hole in one
			    if (score == 1) {
			        scoreClass = 'ace';
			    }
			
			    var rem = par - score;
			
			    if (rem == 0) {
			        scoreClass = 'par';
			    }
			    else if (rem == 1) {
			        scoreClass = 'bir';
			    }
			    else if (rem == 2) {
			        scoreClass = 'eag';
			    }
			    else if (rem == 3) {
			        scoreClass = 'deg';
			    }
			    else if (rem == -1) {
			        scoreClass = 'bog';
			    }
			    else if (rem == -2) {
			        scoreClass = 'dbo';
			    }
			    else if (rem <= -3) {
			        scoreClass = 'oth';
			    }
			    
			    return scoreClass;
	    
    },

    //-----------------------------------------------------------------------------
    renderStatLeaders: function () {
    //-----------------------------------------------------------------------------
        for (var i = 0; i < Leaderboard.leadersSet.available.length; i++) {
            var stat  = Leaderboard.leadersSet.available[i].name;
            var table = document.getElementById(stat);
            var link  = document.getElementById(stat+"_link");
            var rows  = (Leaderboard.leadersSet[stat]) ? Leaderboard.leadersSet[stat].length : null;
            if (table && rows) {
                document.getElementById("StatHeader").style.display = "block";
                table.parentNode.style.display = "block";
                if (!Leaderboard.statTables[stat]) Leaderboard.statTables[stat] = new CBSi.widget.Table(stat);
                Leaderboard.statTables[stat].render(Leaderboard.leadersSet[stat]);
                link.setAttribute("href","/golf/stats/leaders/"+Leaderboard.tourCode+"/"+Leaderboard.year+"/"+Leaderboard.tid+"/"+stat);
            }
        }
    },

    //-----------------------------------------------------------------------------
    renderCourseStats: function (o) {
    //-----------------------------------------------------------------------------
        try {
        if (o.responseText) {
            var text   = o.responseText.match(/<!\-\-([0-9]*)\-\->/);
            var find   = text[0];
            var time   = text[1];
            var text   = o.responseText.replace(find, "");
            var course = eval("("+text+")");
            var stat   = "coursestats";
            Leaderboard.dataStatus['course'] = time;
            //Course Info
            if (!Leaderboard.statTables["courseinfo"]) {
                if (course.shownames === false) { //Remove name column as indicated by course stats data
                    var th = document.getElementById("courseinfo").getElementsByTagName("th")[1];
                    if (th.parentNode) th.parentNode.removeChild(th);
                }
                Leaderboard.statTables["courseinfo"] = new CBSi.widget.Table("courseinfo");
            }
            //Course Stats
            if (!Leaderboard.statTables["coursestats"])
                Leaderboard.statTables["coursestats"] = new CBSi.widget.Table("coursestats");

            if (course["holes"].length > 0) {
                document.getElementById("StatHeader").style.display = "block";
                document.getElementById(stat).parentNode.style.display = "block";
                Leaderboard.statTables["courseinfo"].render(course["holes"]);
                Leaderboard.statTables["coursestats"].render(course["holes"]);
            }
        }
        } catch (e) {}//Unable to load data
    },

    //-----------------------------------------------------------------------------
    renderAlerts: function (o) {
    //-----------------------------------------------------------------------------
        try {
        if (o.responseText) {
            var text = o.responseText.match(/<!\-\-([0-9]*)\-\->/);
            var find = text[0];
            var time = text[1];
            var text = o.responseText.replace(find, "");
            var data = eval("("+text+")");
            if (data) Leaderboard.alerts = data;
            Leaderboard.dataStatus['alerts'] = time;


            for (var i = 0; i < Leaderboard.alerts.length; i++) {
                var li = document.createElement("li");
                var span = document.createElement("span");
                if (i == 0) {
                    var LeaderboardAlerts = document.getElementById("LeaderboardAlerts");
                    LeaderboardAlerts.style.display = "block";
                    var ul = LeaderboardAlerts.getElementsByTagName("ul")[0];
                    while ( ul.hasChildNodes() ) ul.removeChild( ul.lastChild );
                    li.style.display = "block";
                }
                var message = document.createElement("span");
                message.className = "message";
                message.appendChild(document.createTextNode(Leaderboard.alerts[i].message));
                var timestamp = document.createElement("span");
                timestamp.className = "timestamp";
                timestamp.appendChild(document.createTextNode(Leaderboard.alerts[i].timestamp));
                li.appendChild(message);
                li.appendChild(timestamp);
                ul.appendChild(li);
            }

            Leaderboard.toggleAlerts("prev");
        }
        } catch (e) {}//Unable to load data
    },

    //-----------------------------------------------------------------------------
    toggleAlerts: function (direction) {
    //-----------------------------------------------------------------------------
        direction = direction || "next";
        var LeaderboardAlerts = document.getElementById("LeaderboardAlerts");
        var img = LeaderboardAlerts.getElementsByTagName("img");
        var li  = LeaderboardAlerts.getElementsByTagName("li");
        var current = 0;
        var min     = 0;
        var max     = 0;

        for (var i = 0; i < li.length; i++) {
            if (li[i].style.display == "block") {
                li[i].style.display = "none";
                current = i;
            }
            max = i;
        }

        if (direction == "next") {
            if (current < max) {
                current++;
                img[1].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-right.gif";
                img[1].onclick  = function () { Leaderboard.toggleAlerts("next"); }
            }
            if (current == max) {
                img[1].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-right-inactive.gif";
                img[1].onclick  = null;
            }
            if (current == min) {
                img[0].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-left-inactive.gif";
                img[0].onclick  = null;
            }
            if (current > min) {
                img[0].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-left.gif";
                img[0].onclick  = function () { Leaderboard.toggleAlerts("prev"); }
            }
        }
        else {
            if (current > min) {
                current--;
                img[0].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-left.gif";
                img[0].onclick  = function () { Leaderboard.toggleAlerts("prev"); }
            }
            if (current == min) {
                img[0].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-left-inactive.gif";
                img[0].onclick  = null;
            }
            if (current == max) {
                img[1].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-right-inactive.gif";
                img[1].onclick  = null;
            }
            if (current < max) {
                img[1].src      = "http://images.sportsline.com/images/golf/leaderboard/arrow-right.gif";
                img[1].onclick  = function () { Leaderboard.toggleAlerts("next"); }
            }
        }
        if (li[current]) li[current].style.display = "block";
    },

    //-----------------------------------------------------------------------------
    renderTitle: function () {
    //-----------------------------------------------------------------------------
        var title = document.getElementById("LeaderboardTitle");
        title.appendChild(document.createTextNode(Leaderboard.dataSet.name));

        var location = document.getElementById("LeaderboardLocation");
        if (Leaderboard.dataSet.location) {
            var h5      = document.createElement("h5");
            var meta    = new Array();
            meta.push("Location: "+Leaderboard.dataSet.location);

            var course  = new Array();
            for (var id in Leaderboard.dataSet.courses) {
                if (Leaderboard.dataSet.courses[id].name) {
                    var courseInfo = "";
                    if (Leaderboard.dataSet.courses[id].name) courseInfo += Leaderboard.dataSet.courses[id].name;
                    if (Leaderboard.dataSet.courses[id].par)  courseInfo += ", Par "+ Leaderboard.dataSet.courses[id].par;
                    if (Leaderboard.dataSet.courses[id].yard) courseInfo += " | Yardage: "+ Leaderboard.dataSet.courses[id].yard;
                    if (Leaderboard.dataSet.purse)            courseInfo += " | Purse: " + Leaderboard.dataSet.purse;
                    course.push(courseInfo);
                }
            }
            if (course.length > 0) meta.push("Course: "+course.join(" | "));
            location.appendChild(document.createTextNode(meta.join(" | ")));
        }
    },

    //-----------------------------------------------------------------------------
    renderSchedule: function () {
    //-----------------------------------------------------------------------------
        var tournament = [];
        if (Leaderboard.schedule) {
            if (!YAHOO.lang.isArray(Leaderboard.schedule.tournaments.tournament)) {
                tournament.push(Leaderboard.schedule.tournaments.tournament);
            }
            else tournament = Leaderboard.schedule.tournaments.tournament;
        }

        var select = document.getElementById("LeaderboardSchedule");
        for (var i = 0; i < tournament.length; i++) {
            var option = document.createElement("option");
            option.appendChild(document.createTextNode(tournament[i].name));
            option.setAttribute("value", tournament[i].leaderboard);
            if (tournament[i].leaderboard == Leaderboard.tid) {
                select.style.display = "block";
                option.setAttribute("selected", "selected");
            }
           
            select.appendChild(option);
        }
        
    },

    //-----------------------------------------------------------------------------
    select: function () {
    //-----------------------------------------------------------------------------
        var select = document.getElementById("LeaderboardSchedule");
        var option = select.getElementsByTagName("option");
        var index  = select.selectedIndex;
        var tourId = option[index].value;
        var page   = "/golf/leaderboard/"+Leaderboard.seoLink+"/"+tourId;

        switch (tourId) {
            case "447731":
                page = "/golf/tournaments/presidentscup/leaderboard";
                break;
            default:
                page = "/golf/leaderboard/"+Leaderboard.seoLink+"/"+tourId;
        }
        window.location = page;
    },

    //-----------------------------------------------------------------------------
    skillsGauge: function (playerId, o) {
    //-----------------------------------------------------------------------------
        if (o.responseText) {
            var skillsData = eval("("+o.responseText+")");
            SkillsGauge.loadPlayer(playerId, skillsData);
        }
    },

    //-----------------------------------------------------------------------------
    displayAdRow: function () {
    //-----------------------------------------------------------------------------
        var tbody = document.getElementById("LeaderboardData");
        for (var i in Leaderboard.sponsors[0]) {
            if (i < Leaderboard.dataSet.players.length) {
                var randomAd = Math.round(Leaderboard.sponsors[0][i].length * Math.random());
                var adExists = false;
                try { adExists = (Leaderboard.sponsors[0][i][randomAd].sponsorship == "") ? false : true; }
                catch (e) {}
                if (adExists) {
                    var tr = tbody.insertRow(i - 1);
                    tr.className = "adRow";
                    tr.setAttribute("id", "adRow"+i);
                    var td = tr.insertCell(0);
                    td.colSpan = Leaderboard.colspan;
                    td.innerHTML = Leaderboard.sponsors[0][i][randomAd].sponsorship;
                }
            }
        }
    },

    //-----------------------------------------------------------------------------
    removeAdRow: function () {
    //-----------------------------------------------------------------------------
        var tbody = document.getElementById("LeaderboardData");
        for (var i in Leaderboard.sponsors[0]) {
            var adRow = document.getElementById("adRow"+i);
            if (adRow) tbody.removeChild(adRow);
        }
    },

    //-----------------------------------------------------------------------------
    displayProjectedCut: function () {
    //-----------------------------------------------------------------------------
        var date = new Date();
        //console.log('displayed! with ' + Leaderboard.sortby + ' at '+ date.getTime() );

        if (Leaderboard.sortby == "sortindex") {
            var cutcount = 0;
            if (Leaderboard.dataSet.cutcount) cutcount = parseInt(Leaderboard.dataSet.cutcount, 10) + 1;
            if ((Leaderboard.dataSet.projectedcut == true) && (Leaderboard.dataSet.currentround > 1) && (cutcount > 0)) {
                var tbody = document.getElementById("LeaderboardData");
                var tr = tbody.insertRow(cutcount);
                var td = tr.insertCell(0);
                td.colSpan = Leaderboard.colspan;
                tr.setAttribute("id", "cutRow");
            }
        }
    },

    //-----------------------------------------------------------------------------
    removeProjectedCut: function () {
    //-----------------------------------------------------------------------------
        var date = new Date();
        //console.log('removed! at '+ date.getTime() );
        var tbody = document.getElementById("LeaderboardData");
        var tr = document.getElementById("cutRow");
        if (tr && tbody) tbody.removeChild(tr);
    },

    //-----------------------------------------------------------------------------
    sort: function (header) {
    //-----------------------------------------------------------------------------
        if (header.getAttribute("dataType") == "Round") {
            var data = header.getAttribute("dataSource").split(".");
            var round = parseInt(data[1]) + 1;
            Leaderboard.hideAllScorecards();
            Leaderboard.controller(round);
        }
        else {
            Leaderboard.sortby = header.getAttribute("dataSort");
            Leaderboard.removeAdRow();
            Leaderboard.removeProjectedCut();
            Leaderboard.hideAllScorecards();
            CBSi.widget.Table.sort(header);
            Leaderboard.showAllScorecards();
            Leaderboard.displayProjectedCut();
            Leaderboard.displayAdRow();
        }
    },

    //-----------------------------------------------------------------------------
    playerHighlight: function () {
    //-----------------------------------------------------------------------------
        for (var i = 0; i < Leaderboard['highlight']['faves'].length; i++) {
            var playerId  = Leaderboard['highlight']['faves'][i];
            var playerRow = document.getElementById("Leaderboard"+playerId);
            if (playerRow) playerRow.className = "rowFaves";
        }

        //for (var i = 0; i < Leaderboard['highlight']['hated'].length; i++) {
        //    var playerId  = Leaderboard['highlight']['hated'][i];
        //    var playerRow = document.getElementById("Leaderboard"+playerId);
        //    if (playerRow) playerRow.className = "rowHated";
        //}

        for (var i = 0; i < Leaderboard['highlight']['prefs'].length; i++) {
            var playerId  = Leaderboard['highlight']['prefs'][i];
            var playerRow = document.getElementById("Leaderboard"+playerId);
            if (playerRow) playerRow.className = "rowPref";
        }
    },

    //-----------------------------------------------------------------------------
    preSort: function (data, args) {
    //-----------------------------------------------------------------------------
        var type = args[1];
        switch (type) {
            case "today":
                try {
                data = data.replace(/ET/,'');
                data = data.replace(/MT/,'');
                data = data.replace(/PT/,'');
                data = data.replace(/E/,'0');
                if (data.indexOf(':') > -1) {
                    data = data.replace(/:/, '');
                    var time = parseInt(data.substring(0,4), 10);
                    if ((data.indexOf('p') > -1) && (time < 1200)) time += 1200;
                    data = time;
                }
                if (isNaN(data)) data = 1000;
                if (data == '') data = 10000;
                data = parseInt(data);
                } catch (e) {}
                break;
            case "thru":
                data = data.toString();
                data = (data == '' || data == '-') ? -1000 : data.replace(/F/g, 1000);
                break;
            case "backnine":
                data = (data == true) ? "*" : "&nbsp;";
                break;
            case "course":
                try {
                    var title = Leaderboard['dataSet']['courses'][data]['name'];
                    var label = Leaderboard['dataSet']['courses'][data]['acronym'];
                    data = "<span class='course' title='"+title+"'>"+label+"</span>";
                } catch (e) {
                    data = "";
                } //No course data
                break;
            case "countryName":
                if (data.indexOf('countryName') > -1) data = "";
                break;
        }

        return data;
    }
};

//Initialize the Leaderboard application
YAHOO.util.Event.onDOMReady(Leaderboard.init);
</script>
