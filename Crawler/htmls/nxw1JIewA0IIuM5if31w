function ValidateForm(form) {
	if (IsBlank(form)) form=document.forms[0];
	else if ( !form.submit && form.length )
		form=eval("document."+form);
if (form==document.forms['promomain'])
	if (IsBlank(form.email.value))
		AddError("You must enter your email address.", "email");
if (form==document.forms['promomain'])
	if (!IsValidEmail(form.email.value))
		AddError("You must enter your valid email address.", "email");
if (form==document.forms['promomain2'])
	if (!IsValidEmail(form.email.value))
		AddError("You must enter a valid e-mail address.", "email");
if (form==document.forms['promo'])
	if (IsBlank(form.fname.value))
		AddError("Please enter your first name.", "fname");
if (form==document.forms['promo'])
	if (IsBlank(form.lname.value))
		AddError("Please enter your last name.", "lname");
	if (!IsBlank(errstring)){
		alert(errstring);
		if ( !IsBlank(errselect) ) {
			eval( "form." + errselect + ".focus()" );
			eval( "form." + errselect + ".select()" );
		}
		errstring="";
		errselect="";
		return false;
	}
	return true;
