<script type="text/javascript">

    var authentication;

    $(function () {

        // initialize authentication modal
        authentication = new AuthenticationModal('/en/account/management/?redirect_to=http%3a%2f%2fwww.pbr.com%2fen%2fnews%2ffeatures%2fother-features%2f2017%2f10%2fcampbell%e2%80%99s-first-90-in-eight-years-puts-him-on-track-for-world-finals.aspx%3fsf120534990%3d1');

        // check if user is already logged in
        authentication.Initialize(function (user) {
            if (user == undefined) {
                // show register button
                $('a#btn-register').show();
                // show sign in button
                $('a#btn-sign_in').show();


                $('#dd_menu_wrapper').show();
            } else {
                // show sign out button
                $('a#btn-sign_out').show();
            }
        });

        $('a#btn-register').click(function (e) {
            gTracker.TrackEvent('Header', 'ConLink', 'Register');
        });

        $('a#btn-sign_in').click(function (e) {
            e.preventDefault();
            gTracker.TrackEvent('Header', 'ConLink', 'SignIn');
            authentication.Show();
        });

        $('a#btn-sign_out').click(function (e) {
            e.preventDefault();
            gTracker.TrackEvent('Header', 'ConLink', 'SignOut');
            authentication.SignOut();
        });
    });
</script>

