CBSi.namespace("widget.PlayerAutoComplete");(function(){var a;CBSi.widget.PlayerAutoComplete=function(d,f){var b=f,c=this;var e=d.requestURL||"/data/player/search";$("#"+d.inputTagId).autocomplete({open:function(g,i){var h=$("ul.ui-autocomplete");h.css("z-index",6000001)},source:d.source||function(h,g){$.getJSON(e,{as:"json",league_abbr:d.leagueAbbr,site:d.site||"fantasynews",arena_val:d.arena||"mlb",last_name:h.term,limit:d.limit||10,free_agents_only:d.freeAgentsOnly||1},g)},minLength:d.intialQueryLength||1,select:function(g,h){if(!h.item){$(this).val("");return false}else{if(d.callback){d.callback(h.item)}else{if(d.getTargetUrl){window.location=d.getTargetUrl(h.item)}else{window.location=c.getTargetUrl(h.item)}}}}}).data("autocomplete")._renderItem=d.formatResult};CBSi.widget.PlayerAutoComplete.prototype.getTargetUrl=function(b){return b.url}})();
