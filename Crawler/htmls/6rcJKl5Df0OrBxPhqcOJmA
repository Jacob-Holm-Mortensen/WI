.lightboxModalContainer { position: fixed; top: 0px; left: 0px; z-index: 9999999; width: 100%; height: 100%; background-color: rgba(0,0,0,.2); }
.lightboxModalContainer .lightbox { position: absolute; top: 20px; left: 0px; width: 990px; box-sizing: border-box; -moz-box-sizing: border-box; -webkit-box-sizing: border-box; }
