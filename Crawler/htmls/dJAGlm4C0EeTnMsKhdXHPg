	var to = setTimeout(function()
	{
		c.log(f + " -  failed");
		c.timeEnd(f);

		secondaryExchangeStatus = "failed";
		e.removeEventListener("load", successHandler);

		if (window.displayDFPAds)
		{
			displayDFPAds();
		}
	}, 1000);

	// load success
	e.addEventListener("load", successHandler);
}(document));

