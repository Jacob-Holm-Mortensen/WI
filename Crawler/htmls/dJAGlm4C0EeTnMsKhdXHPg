(function()
{
	window.secondaryExchangeStatus = "pending";
	
	// get node
	var e = document.querySelector("script[src='//js.indexww.com/ht/ls-MaxPreps.js']"),
	c = console,
	f = "Secondary Exchange";

	if (!e)
	{
		secondaryExchangeStatus = "failed";
		return;
	}
	
	c.time(f);

	var successHandler = function ()
	{
		c.log(f + " - success");
		c.timeEnd(f);

		clearTimeout(to);
		secondaryExchangeStatus = "success";

		if (window.displayDFPAds)
		{
			displayDFPAds();
		}
	}
	

