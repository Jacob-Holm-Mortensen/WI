    <script type="text/javascript">
        (function() {
            var clipWidth, carousel, needsRefresh, setupComplete = false;

            $('#cast-carousel ul').jcarousel({
                scroll: 4,
                initCallback: function(jCarousel, action) {
                    carousel = jCarousel;
                },
                setupCallback: function() {
                    setupComplete = true;
                    if (needsRefresh) {
                        carousel.reload(true);
                    }
                }
            });

            $(window).resize(function() {
                var newWidth = $('#cast-carousel').find('.jcarousel-clip').width();
                if (clipWidth && (clipWidth != newWidth)) {
                    if (setupComplete) {
                        carousel.reload(true);
                    } else {
                        needsRefresh = true;
                    }
                }

                clipWidth = newWidth;
            });
        })();
    </script>
