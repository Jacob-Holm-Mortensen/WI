                        <script>
                (function () {
                    var vl = document.createElement('script');
                    vl.type = 'text/javascript';
                    vl.async = true;
                    vl.src = document.location.protocol + '//static.vilynx.com/plugins/cbs-vilynx.js';
                    var s = document.getElementsByTagName('script')[0];
                    s.parentNode.insertBefore(vl, s);
                }());
                function renderAdCallback() {
                    if (document.createEvent) { // W3C
                        var event = document.createEvent('Event');
                        event.initEvent('resize', true, true);
                        window.dispatchEvent(event);
                    }
                    else { // IE
                        var event=document.createEventObject();
                        document.documentElement.fireEvent("resize",event);
                    }
                }
            </script>
        
    

