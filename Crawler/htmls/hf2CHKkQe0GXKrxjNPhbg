    <span id="footerScriptsExternal">
        <script type="text/javascript" id="cbssportsFormCore" src="https://sports.cbsimg.net/js/CBSi/app/Form/Core-min.js"></script>
<script type="text/javascript" id="signOnTools" src="https://sports.cbsimg.net/js/CBSi/app/Registration/SocialSignOn-min-v2.js"></script>

