                <div id="leader_top" style="padding-top: 10px;">
                    <script type="text/javascript">
                    if (switchXFP != 'off') {
                        googletag.cmd.push(function() {
                            if (typeof index_headertag_lightspeed !== 'undefined') {
                                      var cb = (function () {
                                          return function () {
                                              var slots = googletag.pubads().getSlots();
                                              index_headertag_lightspeed.set_slot_targeting(slots);
                                               googletag.display("leader_top");
                                            };
                                      })();
                                      index_headertag_lightspeed.add_session_end_hook(cb, true);
                                      index_headertag_lightspeed.refresh();
                                  } else {
                                      googletag.display("leader_top");
                                  }
                        });
                        if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="leader_top" style="width:980px"><br>').insertBefore('#leader_top');
                    }
                    </script>
                </div>
