         adSlots["XFP_leader_top"] = googletag.defineSlot("/8264/aw-cbssports/mlb/media/probable_pitchers/probablepitcher", [[728,90],[970,66],[970,90]], "leader_top").addService(googletag.pubads()).setTargeting("pos", "top"); 
         adSlots["XFP_skin_ad_call"] = googletag.defineSlot("/8264/aw-cbssports/mlb/media/probable_pitchers/probablepitcher", [1600,1000], "skin_ad_call").addService(googletag.pubads()).setTargeting("pos", "top"); 
