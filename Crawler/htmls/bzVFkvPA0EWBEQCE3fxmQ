    jQuery(document).ready(function(){
      function checkBannerWidth(){
        jQuery('.nascar-advertisement').each(function(){
          var bannerAdWidth = jQuery(this).find('iframe').first().attr('width');
          if(bannerAdWidth == null || bannerAdWidth == "undefined" || bannerAdWidth === 0){
            bannerAdWidth = jQuery(this).find('iframe').first().css('width');
          }
          jQuery(this).css({'width':bannerAdWidth});
        });
      }
      setInterval(function(){
        checkBannerWidth();
