									<div class="race-center">
							<a class="event-logo" href="http://www.nascar.com/weekend-schedule.html">
								<img src="https://www.nascar.com/wp-content/uploads/sites/7/2017/10/MENCS_2017_Charlotte_BofA500-190x110-1.png"/>
							</a>
							<div class="event-info">
								<h3>Charlotte Motor Speedway</h3>
								<span>Sunday, October 8 | 1:00 PM ET</span>
								<span> | </span>
								<span>NBC</span>
							</div>

															<script type="text/javascript">
									jQuery(document).ready(function() {
							            var refreshData = "10";
							            var refreshSecs = refreshData * 1000;

							           jQuery.getJSON("https://www.nascar.com/live/feeds/series_1/4610/live_feed.json").done(function(data) {
									    	getData(data);
						                });

							            function getData(data) {
											var currentLap = data.lap_number;
									        var currentFlagStatus = data.flag_state;

											var currentLeaderFirstName='';
											var currentLeaderLastName='';
									        //var currentLeader;
									        var currentLeaderVehicleNumber;
										    jQuery(data.vehicles).each(function( i,content ) {
									            var runningPosition = content.running_position;
									            if(runningPosition===1){
									                //currentLeader = content.driver.full_name;
													currentLeaderFirstName=content.driver.first_name;
													currentLeaderLastName=content.driver.last_name;
									                currentLeaderVehicleNumber=content.vehicle_number;
									                return false;
									            }
									        });
										    jQuery(".race-center-series-1 .lap").text(currentLap);

										    jQuery(".race-center-series-1 tr .status").attr('class', 'status');
										    jQuery(".race-center-series-1 tr .status").addClass("flag-"+currentFlagStatus);
										    if(currentFlagStatus == "4" || currentFlagStatus == "9"){
										    	jQuery(".race-center-series-1 .titlebar th.col1Header").text("Result");
										    }
										    else{
										    	jQuery(".race-center-series-1 .titlebar th.col1Header").text("Current Leader");
										    }
									        jQuery(".race-center-series-1 .leader-car-num").text(currentLeaderVehicleNumber);
									        jQuery(".race-center-series-1 .leader-car-driver").html(currentLeaderFirstName + '<br/>' +currentLeaderLastName);
										}

										setInterval(function(){
										    jQuery.getJSON("https://www.nascar.com/live/feeds/series_1/4610/live_feed.json").done(function(data) {
										    	getData(data);
							                });
										}, refreshSecs);
									});
						        </script>
								<table class="race-center-series-1">
									<tbody>
										<tr class="titlebar">
												<th class="col1Header">Current Leader</th>
											<th class="col2Header">Lap</th>
											<th>Status</th>
										</tr>
										<tr>
											<td class="name"><span class="leader-car-num"></span><span class="leader-car-driver"></span></td>
											<td class="lap"></td>
											<td class="status"><span></span></td>
										</tr>
									</tbody>
