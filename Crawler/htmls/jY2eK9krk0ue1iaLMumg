         context1.section = dvar_genre + '|' + context1.show_title;
         }else{
         context1.section = context1.page_type;
          };
       
      if(DW_onid != "undefined"){
        try{
        context1.ontology_id = DW_onid.replace(/\:/g,"");
        }catch(e){

        }

       }
       context1.user_status = false;context1.device_exp = 'desktop';
        }catch(e){

        }
