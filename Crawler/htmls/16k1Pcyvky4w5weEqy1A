var cbsiPlayer;
var cbsiCarousel;
function VideoPlayerRegistered(playerID){
    if (playerID == '744042151485'){
        cbsiPlayer = CBSi.app.VideoPlayer.playerRefs['744042151485'];
        cbsiCarousel = new CBSi.widget.VideoCarousel({ thumbSize: 'medium', itemsPerPanel:9, carouselContainer:'.carouselCnt', playerObj:cbsiPlayer, objName:'cbsiCarousel', navStyle:'bullet', navItemsPerPage:18 });
        cbsiCarousel.init();

        cbsiPlayer.onPlayerReady = function () {
            cbsiCarousel.render(cbsiPlayer.state.videoData.data);

            cbsiPlayer.onPlaylistStart = function(e) {
                cbsiCarousel.render(cbsiPlayer.state.videoData.data);
            }

            // modify the url here for the microsite, so that share links go to the
            // microsite rather than the video home page
            cbsiPlayer.getShareURL = function(cid) {
                var video = cbsiPlayer.state.videoData.data[cid][0];
                var url = 'http://'+cbsiPlayer.config.host+'/video/player/jonah-keri-show/'+cid;
                return url;
            }
        }
    }
}
</script>

