         node.parentNode.insertBefore(gads, node);
      })();

      var adSlots = {}; //var to hold ad slots.

      googletag.cmd.push(function() {

         adSlots["XFP_top_strip"] = googletag.defineSlot("/8264/aw-cbssports/mlb/media/video/video_jonahkeri", [5,5], "top_strip").addService(googletag.pubads()).setTargeting("pos", "top"); 
