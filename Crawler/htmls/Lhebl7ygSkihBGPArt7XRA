<script type="text/javascript">

if (document.id('prodDetailMoreImgs')) {
window.addEvent('load', function(){
  // MORE IMAGE SWAP
  $('prodDetailMoreImgs').getElements('img').each(function(img){
      img.addEvent('click', function(){
        $('enlargeLink1').set('href', img.get('id'));
        $('enlargeLink2').set('href', img.get('id'));
        $('detailLargeImage').set('src', img.get('rel'));
		if ($('detailImageZoom')) {
		$('detailImageZoom').setStyle('background-image','url('+img.get('id')+')');
		}
		      })
  });
});
}

  function openPopup(sku) {
    var url = 'detail.php?p=' + p + '&tpl=when_available&ajaxNoCache=' + $time();   
    url += '&email_me_sku=' + sku;
    emailMePopup = new Popup({
        url: url
    });
    $('popup').setStyle('text-align', 'left');
  }
  function emailFriendPopup(sku) {
    var url = 'detail.php?p=' + p + '&tpl=email_friend&ajaxNoCache=' + $time();   
    url += '&email_me_sku=' + sku;
    FriendPopup = new Popup({
        url: url,
		width:430
    });
    $('popup').setStyle('text-align', 'left');
  }
  function callAjax(){
    var url = 'detail.php?p=' + p + '&tpl=when_available&ajaxNoCache=' + $time();
    url += '&email_me_email=' + $('email_me_email').value;
    emailMePopup.options.url = url;
    emailMePopup.load();
  }


