<script type="text/javascript">
	var isPhantom = (navigator.userAgent != null) && navigator.userAgent.indexOf("PhantomJS") >= 0;

	var htmlEle = document.documentElement;

	if (!isPhantom) {
		htmlEle.className = htmlEle.className + " loading";
	}

	window.setTimeout(function() {
		var htmlEle = document.documentElement;
		htmlEle.className = htmlEle.className.replace(/(\s|^)loading(\s|$)/, " ");
	}, 10000);

	function getQueryStringParamByName(name) {
		var name, regex, results;
		name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
		regex = new RegExp("[\\?&]" + name + "=([^&#]*)");
		results = regex.exec(window.location.search);
		if (results !== null) {
			return decodeURIComponent(results[1].replace(/\+/g, " "));
		} else {
			return "";
		}
	}

	// get hash
	window.locationHash = location.hash.substr(1);
	</script>
