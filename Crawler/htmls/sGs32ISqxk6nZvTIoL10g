    <div id="bitaWrapper" data-component="gametrackerBita" data-gametracker-bita-options='{"gameMode":"postgame"}'>
        <style>
#bitaCnt {
    position: absolute;
    left: 0px;
    top: 0px;
    background: #ffffff;
    z-index: 3999;
    display: none;
}

#bitaCnt.nobg {
    background: none;
}

#bitaCnt .muteBtn {
    display: none;
    position: absolute;
    color: #efefef;
    cursor: pointer;
    width: 32px;
    height: 32px;
    background: transparent url('https://sports.cbsimg.net/images/bita/mutebtn.png') no-repeat 0px 0px;
}

#bitaCnt .muteBtn.on {
    background-position: -33px 0px;
}
</style><div id="bitaCnt"></div>

<script type="text/javascript">

var BITA = {
    config : {"BILLBOARD":{"HEIGHT":258,"VIDEO_WIDTH":460,"ENABLED":"true","MINS":3,"VIDEO_HEIGHT":258,"CONTAINER_BG":"#1a222e","OFFTIME":90,"YOFFSET":118,"DURATION":60,"WIDTH":620,"ADCALL_HTML5":"https://pubads.g.doubleclick.net/gampad/ads?sz=320x240&pp=vpaid_js&iu=/8264/vaw-sports/desktop/cbssports/gc_billboard&impl=s&gdfp_req=1&env=vp&output=xml_vmap1&unviewed_position_start=1&url=[referrer_url]&correlator=[timestamp]&cust_params=sport%3Dcollegefootball%26plyr%3Dgc_billboard","VIDEO_XOFFSET":80,"VIDEO_YOFFSET":0,"XOFFSET":20,"ADCALL_FLASH":"https://pubads.g.doubleclick.net/gampad/ads?sz=320x240&iu=/8264/vaw-sports/desktop/cbssports/gc_billboard&impl=s&gdfp_req=1&env=vp&output=xml_vmap1&unviewed_position_start=1&url=[referrer_url]&correlator=[timestamp]&cust_params=sport%3Dcollegefootball%26plyr%3Dgc_billboard","CONTAINER":"#sbtop","BUFFER":10,"SESSION":""},"BITA":{"HEIGHT":258,"VIDEO_WIDTH":460,"ENABLED":"true","MINS":3,"VIDEO_HEIGHT":258,"CONTAINER_BG":"#1a222e","OFFTIME":90,"YOFFSET":118,"WIDTH":620,"DURATION":60,"ADCALL_HTML5":"https://pubads.g.doubleclick.net/gampad/ads?sz=272x204&pp=vpaid_js&iu=/8264/vaw-sports/desktop/cbssports/bia&impl=s&gdfp_req=1&env=vp&output=xml_vmap1&unviewed_position_start=1&url=[referrer_url]&correlator=[timestamp]&cust_params=sport%3Dcollegefootball%26plyr%3Dbia","VIDEO_YOFFSET":0,"VIDEO_XOFFSET":80,"XOFFSET":20,"ADCALL_FLASH":"https://pubads.g.doubleclick.net/gampad/ads?sz=272x204&iu=/8264/vaw-sports/desktop/cbssports/bia&impl=s&gdfp_req=1&env=vp&output=xml_vmap1&unviewed_position_start=1&url=[referrer_url]&correlator=[timestamp]&cust_params=sport%3Dcollegefootball%26plyr%3Dbia","CONTAINER":"#sbtop","SESSION":"","BUFFER":10}},
    debugMode : Boolean(parseInt('')),
    bufferTimer : null,
    adCall : {
        flash : '',
        html5 : ''
    },
    callback : null,
    partner : 'cbssports',
    enabled : ((navigator.userAgent.toLowerCase().indexOf('ipod') > -1 || navigator.userAgent.toLowerCase().indexOf('ipad') > -1 || navigator.userAgent.toLowerCase().indexOf('iphone') > -1 || navigator.userAgent.toLowerCase().indexOf('android') > -1) && !!document.createElement('video').canPlayType('video/mp4; codecs="avc1.42E01E, mp4a.40.2"') || '' == '1' || '' == 'disable_bita') ? false : true,
    muted : false,
    video : {
        player_id : 'BITA',
        autoplay : false,
        dynamic_controls : false,
        enable_controls : false,
        comp_ads_enabled : false,
        log_window : false,
        platform_required : false
    },
    container : (typeof jQuery != 'undefined' && jQuery) ? $('#bitaCnt') : null,
    srcDomain : '',
    _timer : null,
    _cookie : {
        id : '',
        domain : '.cbssports.com'
    },
    _xfpObj : null,
    _setCookie : function (name, value, expires) {
        if (!expires) expires = new Date();
        document.cookie = name + "=" + value + "; expires=" + expires.toGMTString() + ";domain=.cbssports.com; path=/";
    },
    _readCookie : function (name) {
        if (document.cookie == '') {
            return false;
        } else {
            var firstChar, lastChar;
            var theBigCookie = document.cookie;
            name = name + '=';
            firstChar = theBigCookie.indexOf(name);
            if (firstChar != -1) {
                firstChar += name.length;
                lastChar = theBigCookie.indexOf(';', firstChar);
                if (lastChar == -1) lastChar = theBigCookie.length;
                return unescape(theBigCookie.substring(firstChar, lastChar));
            } else {
                return false;
            }
        }
    },
    _killCookie : function (name, path, domain) {
        var theValue = this._readCookie(name);
        if (theValue) {
            // set an already-expired cookie
            document.cookie = name + '=' + theValue + '; expires=Fri, 13-Apr-1970 00:00:00 GMT' + ((path) ? ';path=' + path : '') + ((domain) ? ';domain=' + domain : '');
        }
    },
    init : function () {
        //console.log('BITA: init');
        this.container = $('#bitaCnt');

        this.muted = (document.cookie.indexOf('BITA_mute_state') != -1) ? (this._readCookie('BITA_mute_state') == 'true') : false

        // set xfp obj
        this._xfpObj = this._getXFPObj();

        // reset the BITA limit cookie
        this._killCookie('_BITA_'+this._cookie.id, '', this._cookie.domain);
        this._killCookie('_BILLBOARD_'+this._cookie.id, '', this._cookie.domain);
    },
    show : function () {
        //console.log('BITA: show');
        var cnt = this.container;

        if (!cnt) { return; }

        // don't exceed the limit
        if (this._readCookie('_'+this.mode+'_'+this._cookie.id) == 'true' && !this.debugMode) {
            this.callback();
            return;
        }

        // we'll create and destroy the video on show/hide
        var params = [];
        for (var p in this.video) {
            params.push(p+'='+this.video[p]);
        }

        var src = '/video/player/embed?args='+encodeURIComponent(params.join('&'));
        if (this.srcDomain != '') {
            src = 'https://'+this.srcDomain+src;
        }

        var embedWrapper = document.createElement('div');
        embedWrapper.id = 'embedVideoContainer_BITA';
        embedWrapper.className = 'embedVideo';
        cnt.append(embedWrapper);

        var embedIframe = document.createElement('iframe');
        embedIframe.id = 'embedVideo_BITA';
        embedIframe.name = 'embedVideo_BITA';
        embedIframe.width = this.video.width;
        embedIframe.height = this.video.height;
        embedIframe.setAttribute('marginwidth', 0);
        embedIframe.setAttribute('marginheight', 0);
        embedIframe.setAttribute('scrolling', 'no');
        embedIframe.setAttribute('frameborder', 0);
        embedIframe.setAttribute('allowTransparency', 'true');
        embedIframe.setAttribute('allowfullscreen', 'true');
        $(embedWrapper).append(embedIframe);

        var muteBtn = document.createElement('div');
        muteBtn.className = 'muteBtn'+(this.muted?' on':'');
        $(embedWrapper).append(muteBtn);

        // move container to desired parent and show container
        cnt.appendTo(this.parentCnt);
        cnt.css('opacity', .0001);
        cnt.show();

        // need to set the src after the iframe is added or we get mayhem in IE9
        $('#embedVideo_BITA').attr('src', src);

        this.setContainer(this.width, this.height, this.xOffset+'px', this.yOffset+'px');
        this.setVideo(this.video.width, this.video.height, this.video.xOffset+'px', this.video.yOffset+'px', undefined, undefined, 'absolute');

        // set cookie if necessary
        if (document.cookie.indexOf('_'+this.mode+'_'+this._cookie.id) == -1) {
            var expires = new Date(new Date().getTime() + (this.timeLimit * 60 * 1000));
            this._setCookie('_'+this.mode+'_'+this._cookie.id, true, expires);
        }

        // set off timer, +5s at the end for a lag buffer
        if (this._timer) { clearTimeout(this._timer); }
        this._timer = setTimeout("BITA.callback()", ((this.duration+this.buffer+5)*1000));
    },
    hide : function () {
        //console.log('BITA: hide');
        var cnt = this.container;

        if (!cnt) { return; }

        // clear the video
        cnt.html('');

        // delete player refs
        window.cbsiPlayerBITA = {};

        try {
            delete CBSi.app.VideoPlayer.playerRefs.BITA;
        } catch (e) {}

        // hide the container
        cnt.hide();

    },
    onSetContainer : function (ref) {
        return function () {
            ref.setContainer(ref.width, ref.height, ref.xOffset+'px', ref.yOffset+'px');
        }
    },
    setContainer : function (w, h, l, t, r, b, pos) {
        //if (this.container.is(':hidden')) { return; }
        //console.log('setContainer('+w+', '+h+', '+l+', '+t+', '+r+', '+b+', '+pos+')');

        var cnt = this.container;

        if (typeof w != 'undefined') cnt.width(w);
        if (typeof h != 'undefined') cnt.height(h);
        if (typeof l != 'undefined') cnt.css('left', l);
        if (typeof t != 'undefined') cnt.css('top', t);
        if (typeof r != 'undefined') cnt.css('right', r);
        if (typeof b != 'undefined') cnt.css('bottom', b);
        if (typeof pos != 'undefined') cnt.css('position', pos);
    },
    setVideo : function (w, h, l, t, r, b, pos) {
        //if (this.container.is(':hidden')) { return; }
        //console.log('setVideo('+w+', '+h+', '+l+', '+t+', '+r+', '+b+', '+pos+')');

        var cnt = this.container.children('.embedVideo');
        var ifr = cnt.children('iframe');
        var mute = cnt.children('.muteBtn');

        if (typeof w != 'undefined') {
            cnt.width(w);
            ifr.width(w);
        }
        if (typeof h != 'undefined') {
            cnt.height(h);
            ifr.height(h);
        }

        if (typeof cbsiPlayerBITA != 'undefined' && typeof cbsiPlayerBITA.resizePlayer != 'undefined' && typeof w != 'undefined' && typeof h != 'undefined') {
            cbsiPlayerBITA.resizePlayer(w,h);
        }

        mute.css('right', '10px');
        mute.css('bottom', '10px');

        if (typeof l != 'undefined') cnt.css('left', l);
        if (typeof t != 'undefined') cnt.css('top', t);
        if (typeof r != 'undefined') cnt.css('right', r);
        if (typeof b != 'undefined') cnt.css('bottom', b);
        if (typeof pos != 'undefined') cnt.css('position', pos);
    },
    _getXFPObj : function () {
        var xfpObj = {};

        // normandy uses xfpObj string
        if (typeof window.xfpObj != 'undefined') {
            var xfpDataPairs = window.xfpObj.split('~');
            for (var i = 0; i < xfpDataPairs.length; i++) {
                var pair = xfpDataPairs[i].split('=');
                xfpObj[pair[0]] = pair[1];
            }
        }
        // Highlander uses SportsPageVars.ads.data.gpt.targeting object.
        // There is no 'arena' property in SportsPageVars, closest is the 'cid' which is the arena.
        // We need to adjust for backwards of un-hyphenated sport values for college.
        else if (typeof ((((window.SportsPageVars || {}).ads || {}).data || {}).gpt || {}).targeting != 'undefined') {
            xfpObj = window.SportsPageVars.ads.data.gpt.targeting;
            if (typeof xfpObj.exclude != 'undefined') {
                xfpObj.arena = xfpObj.exclude;
            }
        }

        return xfpObj;
    },
    getCustParams : function() {
        if (!this._xfpObj) { this._xfpObj = this._getXFPObj(); }

        var xfpObj = this._xfpObj;
		console.log("XFP Obj: ", xfpObj);
        var params = {
            sport       : xfpObj.arena,
            cid         : xfpObj.cid,
            env         : xfpObj.env,
            firstpg     : xfpObj.firstpg,
            ftag        : xfpObj.ftag,
            ptype       : xfpObj.ptype,
            session     : xfpObj.session,
            subses      : xfpObj.subses,
            usergroup   : xfpObj.user,
            vguid       : xfpObj.vguid,
            exclude     : xfpObj.exclude,
            favteam     : xfpObj.favteam,
            setbrand    : xfpObj.setbrand,
            sportsmktg  : xfpObj.sportsmktg,
            srvclvl     : xfpObj.srvclvl,
            team        : xfpObj.team,
            user        : xfpObj.user
        };

        var custParams = {};
        for (var p in params) {
            if (typeof params[p] != 'undefined' && params[p] != '') {
                custParams[p] = params[p];
            }
        }

        return custParams;
    },
    buildAdURL : function(url, len) {
        var urlObj = url.split('?');
        var ad = {
            domain : urlObj[0],
            queryObj : (urlObj.length > 1 && urlObj[1] != '') ? this.unpack(urlObj[1]) : {}
        };

        // build cust_params
        var CP = { podid : 't'+len };
        var pageCP = this.getCustParams();
        console.log("Page CP: ",pageCP);
        var settingsCP = (typeof ad.queryObj.cust_params != 'undefined' && ad.queryObj.cust_params != '') ? this.unpack(decodeURIComponent(ad.queryObj.cust_params)) : {};
        console.log("Settings CP: ",settingsCP);
        $.extend(CP, pageCP, settingsCP);
        //ad.queryObj.cust_params = encodeURIComponent(this.pack(CP));
        ad.queryObj.cust_params = this.pack(CP);
        console.log("Cust Params: " + ad.queryObj.cust_params);
        console.log("Params packed: " + this.pack(ad.queryObj));

        var resp = ad.domain+'?'+this.pack(ad.queryObj);
        console.log("Ad Response: " + resp);

        return resp;
    },
    unpack : function (qryStr) {
        var pairs = qryStr.split("&");
        var queryObj = {};
        for (var p = 0; p < pairs.length; p++) {
            var thisPair = pairs[p].split("=");
            if (thisPair.length == 2 && thisPair[0] != '' && thisPair[1] != '') {
                queryObj[thisPair[0]] = decodeURIComponent(thisPair[1]);
            }
        }
        return queryObj;
    },
    pack : function (queryObj) {
        var pairs = [];
        for (o in queryObj) {
            if (o != '' && queryObj[o] != '')
            pairs.push(o + '=' + encodeURIComponent(queryObj[o]));
        }
        return pairs.join("&");
    }
};

var breakOn = function (cb) {
    //console.log('BITA: breakOn');
    if ($(BITA.container).is(':visible')) { return; }

    var sessionMatch = (!BITA.config.BITA.SESSION.match(/\w/) || BITA.config.BITA.SESSION.match(/\w/) && BITA.config.BITA.SESSION.indexOf(BITA._xfpObj.session) != -1);

    if (!BITA.enabled || BITA.config.BITA.ENABLED == 'false' || !sessionMatch) {
        breakOff();
        return;
    }

    BITA.duration = BITA.config.BITA.DURATION;
    BITA.buffer = BITA.config.BITA.BUFFER;

    BITA.parentCnt = $(BITA.config.BITA.CONTAINER);
    BITA.container.css('background', BITA.config.BITA.CONTAINER_BG);
    BITA.timeLimit = BITA.config.BITA.MINS;
    BITA.mode = 'BITA';

    BITA.xOffset = BITA.config.BITA.XOFFSET;
    BITA.yOffset = BITA.config.BITA.YOFFSET;
    BITA.width = BITA.config.BITA.WIDTH;
    BITA.height = BITA.config.BITA.HEIGHT;
    BITA.video.xOffset = BITA.config.BITA.VIDEO_XOFFSET;
    BITA.video.yOffset = BITA.config.BITA.VIDEO_YOFFSET;
    BITA.video.width = BITA.config.BITA.VIDEO_WIDTH;
    BITA.video.height = BITA.config.BITA.VIDEO_HEIGHT;

    BITA.adCall.flash = BITA.buildAdURL(BITA.config.BITA.ADCALL_FLASH, BITA.config.BITA.DURATION);
    BITA.adCall.html5 = BITA.buildAdURL(BITA.config.BITA.ADCALL_HTML5, BITA.config.BITA.DURATION);

    BITA.callback = breakOff;
    if (BITA.bufferTimer) { clearTimeout(BITA.bufferTimer); }
    BITA.bufferTimer = setTimeout("BITA.show();", (BITA.buffer*1000));
    if (typeof cb != 'undefined') { cb(); }
};

var breakOff = function (cb) {
    //console.log('BITA: breakOff');
    if (BITA.bufferTimer) { clearTimeout(BITA.bufferTimer); }
    BITA.hide();
    if (typeof cb != 'undefined') { cb(); }
};

var billboardOn = function (cb) {
    //console.log('BITA: billboardOn');
    if ($(BITA.container).is(':visible')) { return; }

    var fromGT = (('').toLowerCase().indexOf('gametracker/live') != -1);
    var sessionMatch = (!BITA.config.BILLBOARD.SESSION.match(/\w/) || BITA.config.BILLBOARD.SESSION.match(/\w/) && BITA.config.BILLBOARD.SESSION.indexOf(BITA._xfpObj.session) != -1);

    if (fromGT || !BITA.enabled || BITA.config.BILLBOARD.ENABLED == 'false' || !sessionMatch) {
        billboardOff();
        return;
    }

    BITA.duration = BITA.config.BILLBOARD.DURATION;
    BITA.buffer = BITA.config.BILLBOARD.BUFFER;

    BITA.parentCnt = $(BITA.config.BILLBOARD.CONTAINER);
    BITA.container.css('background', BITA.config.BILLBOARD.CONTAINER_BG);
    BITA.timeLimit = BITA.config.BILLBOARD.MINS;
    BITA.mode = 'BILLBOARD';

    BITA.xOffset = BITA.config.BILLBOARD.XOFFSET;
    BITA.yOffset = BITA.config.BILLBOARD.YOFFSET;
    BITA.width = BITA.config.BILLBOARD.WIDTH;
    BITA.height = BITA.config.BILLBOARD.HEIGHT;
    BITA.video.xOffset = BITA.config.BILLBOARD.VIDEO_XOFFSET;
    BITA.video.yOffset = BITA.config.BILLBOARD.VIDEO_YOFFSET;
    BITA.video.width = BITA.config.BILLBOARD.VIDEO_WIDTH;
    BITA.video.height = BITA.config.BILLBOARD.VIDEO_HEIGHT;

    BITA.adCall.flash = BITA.buildAdURL(BITA.config.BILLBOARD.ADCALL_FLASH, BITA.config.BILLBOARD.DURATION);
    BITA.adCall.html5 = BITA.buildAdURL(BITA.config.BILLBOARD.ADCALL_HTML5, BITA.config.BILLBOARD.DURATION);

    BITA.callback = billboardOff;
    BITA.show();
    if (typeof cb != 'undefined') { cb(); }
};

var billboardOff = function (cb) {
    //console.log('BITA: billboardOff');
    BITA.hide();
    $(BITA.container).removeClass('nobg');
    if (typeof window.GameTrackerLayout != 'undefined') {
        window.GameTrackerLayout.openGameTracker();
    }

    if (typeof cb != 'undefined') { cb(); }
};

var cbsiPlayerBITA;

function VideoPlayerRegistered(playerID) {
    if (playerID == 'BITA') {
        cbsiPlayerBITA = CBSi.app.VideoPlayer.playerRefs.BITA;
        cbsiPlayerBITA.onPlayerInit = function () {
            $(window).scroll();
            BITA.container.css('opacity', 1);

            var adCall = (cbsiPlayerBITA.config.isUVPJSSupported) ? BITA.adCall.html5 : BITA.adCall.flash;

            cbsiPlayerBITA.loadAdByUrl(adCall);

            try {
                if (BITA.muted) {
                    cbsiPlayerBITA.api.mute();
                }
                else {
                    cbsiPlayerBITA.api.unMute();
                }
            } catch (e) {
                CBSi.log(e);
            }

            $('#bitaCnt .muteBtn').click( function() {
                $(this).toggleClass('on');

                var expires = new Date(new Date().getTime() + (86400 * 365 * 10 * 1000));

                if ($(this).hasClass('on')) {
                    BITA.muted = true;
                    BITA._setCookie('BITA_mute_state', 'true', expires);
                    cbsiPlayerBITA.api.mute();
                }
                else {
                    BITA.muted = false;
                    BITA._setCookie('BITA_mute_state', 'false', expires);
                    cbsiPlayerBITA.api.unMute();
                }
            });
            $('#bitaCnt .muteBtn').show();

            cbsiPlayerBITA.resizePlayer(BITA.video.width,BITA.video.height);
        };

        // separate pod end hooks for flash and html5
        if (cbsiPlayerBITA.config.isUVPJSSupported) {
            cbsiPlayerBITA.onAdPodEnd = function () {
                if (BITA._timer) { clearTimeout(BITA._timer); }
                setTimeout("BITA.callback()", 100);
            };
        }
        else {
            cbsiPlayerBITA.onDisposeAd = function () {
                if (BITA._timer) { clearTimeout(BITA._timer); }
                setTimeout("BITA.callback()", 100);
            };
        }

    }

}

// Loader
if (typeof jQuery != 'undefined' && jQuery) {
    $(function() {
        BITA.init();
    });
}
