&quot;2&quot;:&quot;14&quot;,&quot;3&quot;:&quot;7&quot;,&quot;4&quot;:&quot;0&quot;}},&quot;field_position&quot;:{&quot;distance&quot;:&quot;20&quot;,&quot;team_in_possession&quot;:&quot;760&quot;,&quot;side&quot;:&quot;PITT&quot;,&quot;goal_to_go&quot;:&quot;No&quot;,&quot;yardline&quot;:&quot;47&quot;,&quot;redzone&quot;:&quot;No&quot;,&quot;down&quot;:&quot;4&quot;,&quot;nextplay&quot;:&quot;&quot;},&quot;last_score&quot;:{&quot;id&quot;:&quot;175&quot;,&quot;home_score&quot;:&quot;21&quot;,&quot;description&quot;:&quot;49-M.Ammendola 25 yards Field Goal is Good.&quot;,&quot;away_score&quot;:&quot;59&quot;,&quot;drive_plays&quot;:&quot;11&quot;,&quot;drive_time&quot;:&quot;04:49&quot;,&quot;type&quot;:&quot;FieldGoal&quot;,&quot;scoring_team&quot;:&quot;760&quot;,&quot;drive_yds&quot;:&quot;5&quot;,&quot;time_remaining&quot;:&quot;10:11&quot;,&quot;quarter&quot;:&quot;4&quot;},&quot;last_play&quot;:{&quot;score_on_play&quot;:&quot;No&quot;,&quot;short_score&quot;:&quot;&quot;,&quot;medium&quot;:&quot;Rush for -2 yd&quot;,&quot;short&quot;:&quot;-2 yd run&quot;,&quot;id&quot;:&quot;194&quot;,&quot;team_in_possession&quot;:&quot;760&quot;,&quot;distance&quot;:&quot;18&quot;,&quot;side&quot;:&quot;PITT&quot;,&quot;under_review&quot;:&quot;No&quot;,&quot;description&quot;:&quot;14-T.Cornelius kneels at PIT 47 for -2 yards.&quot;,&quot;drive_id&quot;:&quot;27&quot;,&quot;yardline&quot;:&quot;45&quot;,&quot;down&quot;:&quot;3&quot;,&quot;subplays&quot;:{&quot;subplay&quot;:[{&quot;rush&quot;:{&quot;rusher_name&quot;:&quot;&quot;,&quot;yards_on_play&quot;:&quot;-2&quot;,&quot;team_in_possession&quot;:&quot;760&quot;,&quot;rusher&quot;:&quot;0&quot;,&quot;yards_to_endzone&quot;:&quot;45&quot;},&quot;order&quot;:&quot;1&quot;,&quot;type&quot;:&quot;Rush&quot;}]},&quot;quarter&quot;:&quot;4&quot;,&quot;time_remaining&quot;:&quot;1:00&quot;},&quot;awayscore&quot;:{&quot;total&quot;:&quot;59&quot;,&quot;timeouts_remaining&quot;:&quot;2&quot;,&quot;quarter&quot;:{&quot;1&quo