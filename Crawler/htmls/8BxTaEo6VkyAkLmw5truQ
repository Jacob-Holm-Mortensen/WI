//Submit functionality for Player Stats
function playerStatsClick(formCheck,tour,year) {

    if ( tour == null ) {
        alert("Please select a tour to continue");
    }
    else if ( formCheck.PlayerList.value == "" ) {
        alert("Please select a player to continue");
    }
    else {
        window.location = "/golf/players/playerpage/" + formCheck.PlayerList.value;
    }
}
</script>
