    <script type="text/javascript" src="https://sports.cbsimg.net/js/global-footer-min-v00101.js"></script><script>



$( document ).ready(function() {

    var currentHeaderState = 0;
    var isIE8 = $.browser.msie && +$.browser.version === 8;

    var topOrig = $('#hubHead').offset().top;
    var topOrigTable;
    //if (isIE8){
        //topOrigTable = $('#oddsTable').scrollTop();
    //}
    //else{
        topOrigTable = $('#oddsTable').offset().top;
    //}
    //var topOrigTable = $('#oddsTable').offset().top;

    var bottomOrig;
    //if (isIE8){
        //bottomOrig = $('#oddsTable').scrollTop() + $('#oddsTable').height();
    //}
    //else{
        bottomOrig = $('#oddsTable').offset().top + $('#oddsTable').height() - $('#hubHead').height();
    //}
    //var bottomOrig = $('#oddsTable').offset().top + $('#oddsTable').height() - $('#hubHead').height();

    var bottomOffset;
    //if (isIE8){
        //bottomOffset = $('#oddsTable').scrollTop() + $('#oddsTable').height() - $('#hubHead').height();
    //}
    //else{
        bottomOffset = $('#oddsTable').offset().top + $('#oddsTable').height() - $('#hubHead').height();
    //}
    //var bottomOffset = $('#oddsTable').offset().top + $('#oddsTable').height() - $('#hubHead').height();

    $(window).scroll(function () {

        var topelem;
        //if (isIE8){
            //top = $('#hubHead').scrollTop();
            //topelem = document.getElementById('hubHead').scrollTop;
            //topelem = $('#hubHead').offset().top;
            //alert("setting top " + topelem);
        //}
        //else{
            topelem = $('#hubHead').offset().top;
        //}
        //var top = $('#hubHead').offset().top;
        //console.log(top);

        var ywindowoffset;
        if (isIE8){
            if (document.compatMode == "CSS1Compat") {
                ywindowoffset = document.documentElement.scrollTop;
            }
            else{
                ywindowoffset = document.body.scrollTop;
            }
        }
        else{
            ywindowoffset = window.pageYOffset;
        }
        //var ywindowoffset = window.pageYOffset;
        //var bottomOffset = $('#oddsTable').offset().top + $('#oddsTable').height() - $('#hubHead').height();
        //console.log(ywindowoffset + " - " + topelem + " - " + bottomOrig);
         //alert(ywindowoffset + " - " + topelem + " - " + bottomOrig + " - " + isIE8);

        // move to fixed
        if ((ywindowoffset > topelem) && (ywindowoffset < bottomOffset) && (currentHeaderState == 0)){
            $('#hubHead').css('position','fixed');
            $('#hubHead').css('top','0px');
            $('#hubHead').css('z-index','5000');
            currentHeaderState = 1;
            //console.log("here");
        }

        // above scroll
        if ((ywindowoffset < topOrigTable) && (currentHeaderState == 1)){
            $('#hubHead').css('position','relative');
            currentHeaderState = 0;
        }

        // below scroll
        if ((ywindowoffset > bottomOffset) && (currentHeaderState == 1)){
            $('#hubHead').css('position','relative');
            currentHeaderState = 0;
        }

        //console.log("------");
    });

