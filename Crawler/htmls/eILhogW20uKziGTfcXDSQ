b+" #circle_poll_grey_img_0").css("display","none");a(b+" #circle_poll_orange_img_0").css("display","block");a(b+" #circle_poll_grey_img_1").css("display","block");a(b+" #circle_poll_orange_img_1").css("display","none");a(this).unbind("click");var k="";var i=f.aggregate_results.split("&");a.each(i,function(m,l){var n=l.split("=");if(a(this).attr("data-voteid")!=n[0]){k=n[0]}});c.submitReVote(a(this).attr("data-voteid"),f.id,k);c.getStats(true,true);c.setReVote(f)})}if((a(this).attr("data-cellid")=="1")&&(d=="none")){a(this).click(function(k){a(b+" #circle_poll_grey_img_0").css("display","block");a(b+" #circle_poll_orange_img_0").css("display","none");a(b+" #circle_poll_grey_img_1").css("display","none");a(b+" #circle_poll_orange_img_1").css("display","block");a(this).unbind("click");var l="";var j=f.aggregate_results.split("&");var i=this;a.each(j,function(n,m){var o=m.split("=");if(a(i).attr("data-voteid")!=o[0]){l=o[0]}});c.submitReVote(a(this).attr("data-voteid"),f.id,l);c.getStats(true,true);c.setReVote(f)})}})},submitReVote:function(d,e,b){var c=this;a.ajax({type:this.settings.sendURLMethod,url:this.settings.reSendURL({questionID:e,vote:d,revote:b}),context:this,cache:false,async:false,success:function(f){c.resetAll();if(c.delayLoadResults){c.delayLoadResults(f)}if(this.settings.saveOnCookie){c.setPollCookie(f)}if(this.settings.allowReVote){c.setReVote(f)}},dataType:this.settings.getURLDataType})},setCheckMarks:function(c){var e=c.aggregate_results.split("&");var d=0;var b=this.settings.container;a.each(e,function(g,f){var h=f.split("=");if(h[0]==c.answer){if(d==0){a(b+" #circle_poll_grey_img_0").css("display","none");a(b+" #circle_poll_orange_img_0").css("display","block");a(b+" #circle_poll_grey_img_1").css("display","block");a(b+" #circle_poll_orange_img_1").css("display","none")}if(d==1){a(b+" #circle_poll_grey_img_0").css("display","block");a(b+" #circle_poll_orange_img_0").css("display","none");a(b+" #circle_poll_grey_img_1").css("display","none");a(b+" #circle_poll_orange_img_1").css("display","block")}}d++})}}})(jQuery);
               CBSi.widget.CirclePoll.prototype.map  = function(dataObj){
                var html     = [],
                    poll     = [],
                    votes    = {},
                    self     = this,
                    template = self.settings.template,
                    objMap   = function(obj){
                        return {
                            name   : obj['name'],
                            photo  : obj['photo'],
                            team   : '',
                            id     : obj['id'],
                            votes  : obj['votes']
                        }
                    };
                    
                self.totalMap = [];
                self.singleVotes = [];
                self.totalVotes = 0;
                self.questionID = dataObj['code'];
                if (dataObj.aggregate_results.match(/home=[0-9]*&away=[0-9]*/)){
                          var ArraySwapVotes = dataObj.aggregate_results.split('&');
                          dataObj.aggregate_results = ArraySwapVotes[1] + '&' + ArraySwapVotes[0];
                }
                var Arrayvotes = dataObj.aggregate_results.split('&');

                $.each(Arrayvotes , function(k,v){
                    var getVotes = v.split('=');
                    var photo;
                    if (dataObj.photos){
                        var ArrayPhotos = dataObj.photos.split('&');
                        $.each(ArrayPhotos , function(k,v){
                            var getPhotos = v.split('=');
                            if (getVotes[0] == getPhotos[0]){
                                photo = getPhotos[1];   
                            }
                        });
                    }
                    else{
                        photo = '/images/players/unknown_player.gif';
                    }
                    poll.push({'name' : getVotes[0] , 'votes' : getVotes[1], 'id' : getVotes[0], 'photo' : photo});              
                });
                
                $.each(poll , function(k,v){
                    var map          = objMap(v),
                        divId        = k,
                        displayName  = (self.settings.useDisplayName) ?  map.name  : '',
                        displayPhoto = (!self.settings.photoOverride) ?  map.photo : self.settings.photoOverride,
                        displayTeam  = (self.settings.useDisplayTeam) ?  map.team  : '';
                    
                        self.totalVotes = self.totalVotes + parseInt(map.votes);
                        self.singleVotes.push(map.votes);
                        self.totalMap.push(map);
                        
                        html.push(template(map,divId)); 
                });
                
                return html;
    };
    
   CBSi.widget.CirclePoll.prototype.delayLoadResults  = function(dataObj){
    
        return this.map(dataObj);
    
   }

            CBSi.widget.CirclePoll.prototype.template=function(b,a){return'<div class="circle_poll_left circle_poll_position circle_base"  data-cellId = "'+a+'" data-voteId = "'+b.id+'">                            <div class="circle_base ie_circle_base circle_poll_overlay" data-cellId = "'+a+'"></div>                            <div class="circle_blue ie_circle_blue circle_poll_overlay" data-cellId = "'+a+'"></div>                            <div class="circle_click_area"  data-cellId = "'+a+'">                                <div id="circle_poll_grey_img_'+a+'" class="circle_poll_grey_img"><img src="'+CBSi.imageServer+'/images/fantasy/football/homepage/wwys/check_gray.png"></div>                                <div id="circle_poll_orange_img_'+a+'" class="circle_poll_orange_img"><img src="'+CBSi.imageServer+'/images/fantasy/football/homepage/wwys/check_orange.png"></div>                                <div class="circle_poll_button wwys_button'+a+'"  data-corner="100px">                                    <span class="updatenum updatenum'+a+'">00%</span>                                    <div class="circle_poll_img circle_poll_img'+a+'" data-cellId = "'+a+'">                                        <img src="https://sports.cbsimg.net'+b.photo+'" class="circle_poll_photo_'+a+'">                                    </div>                                </div>                                <canvas class="canvas'+a+'" width="58" height="51"></canvas>                            </div>                            <div class="circle_poll_name_'+a+'">'+b.name+'</div>                            <div class="circle_poll_position_'+a+'">'+b.team+"</div>                        </div>"};CBSi.widget.CirclePoll.prototype.orTemplate=function(){return'<div class="circle_poll_or">OR</div>'};
</script>

