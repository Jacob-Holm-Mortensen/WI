<script>

function registerVote(game_abbr,homeAway,pick_type){
    var feature_code = "2013NCAAFpickum-" + game_abbr;
    var key = "home,away";
    
    getAggResults(game_abbr,homeAway,feature_code,key);
}

function getAggResults(game_abbr,vote,feature_code,key){

    var serverType = 'poll';
    var results_url = '';
    results_url = '/data/social/grade/' + feature_code + game_abbr + '/' + key;
    if (vote.length > 0){
        results_url =  results_url + "/" + vote;
    }
    $.ajax({
        url: results_url,
        dataType: 'json',
        success:function(result){
            if (typeof(result.aggregate_results) !== 'undefined'){
                displayAggResults(game_abbr,vote,result.aggregate_results,feature_code,key);
            }
            else{
                //$('#gradePopup' + grade_number).html("");
            }
        }
    });
}

function displayAggResults(game_abbr,vote,agg_results,feature_code,key){
    var percents = new Array();
    feature_code_array = feature_code.split(',');
    percents = changeNumbersToPercents(agg_results);
    change_single_value(vote, game_abbr, percents[1], percents[0], 0);
    setPicksCookie(game_abbr,vote, percents[1], percents[0]);
}

function change_single_value(homeAway, game_abbr, awayPercent, homePercent, disable_pick){
    //console.log("Calling change_val: " + game_abbr);
    var homeSelector = '.homePercent.'+game_abbr;
    var awaySelector = '.awayPercent.'+game_abbr;
    //$('.homePercent.'+game_abbr).html(homePercent);
    $(homeSelector).html(homePercent);
    //$('.awayPercent.'+game_abbr).html(awayPercent);
    $(awaySelector).html(awayPercent);
    if (homeAway == "home"){
        $('.homeImage.'+game_abbr).html("<img src=\"https://sports.cbsimg.net/images/nfl/features/writers/poll_selected.png\">");
        $('.homeImage.'+game_abbr).removeClass('pickContainerButtonUnselected').addClass('pickContainerButtonSelected');
        $('.homePick.'+game_abbr).removeClass('unselectedPickContainer').addClass('selectedPickContainerCorrect');
        $('.homePercent.'+game_abbr).removeClass('percentContainer').addClass('percentContainerCorrect');
        $('.WLCtr.'+game_abbr+"home").css('display','block');
        $('.homePick.'+game_abbr).click(function() { return false; });
        $('.homePick.'+game_abbr).attr('onclick','').unbind('click');
        //if (disable_pick == 1){
            $('.awayPick.'+game_abbr).click(function() { return false; });
            $('.awayPick.'+game_abbr).attr('onclick','').unbind('click');
        //}
        $('.awayImage.'+game_abbr).html("<img src=\"https://sports.cbsimg.net/images/nfl/features/writers/poll_off.png\">");
        $('.awayImage.'+game_abbr).addClass('pickContainerButtonUnselected').removeClass('pickContainerButtonSelected');
        //$('.awayPick.'+game_abbr).addClass('unselectedPickContainer').removeClass('selectedPickContainer');
        $('.awayPick.'+game_abbr).addClass('selectedPickContainerNotCorrect').removeClass('selectedPickContainer');
    }
    if (homeAway == "away"){
        $('.awayImage.'+game_abbr).html("<img src=\"https://sports.cbsimg.net/images/nfl/features/writers/poll_selected.png\">");
        $('.awayImage.'+game_abbr).removeClass('pickContainerButtonUnselected').addClass('pickContainerButtonSelected');
        $('.awayPick.'+game_abbr).removeClass('unselectedPickContainer').addClass('selectedPickContainerCorrect');
        $('.awayPercent.'+game_abbr).removeClass('percentContainer').addClass('percentContainerCorrect');
        //$('.WLCtr.'+game_abbr+"away").css('display','block');
        $('.awayPick.'+game_abbr).click(function() { return false; });
        $('.awayPick.'+game_abbr).attr('onclick','').unbind('click');
        //if (disable_pick == 1){
            $('.homePick.'+game_abbr).click(function() { return false; });
            $('.homePick.'+game_abbr).attr('onclick','').unbind('click');
        //}
        $('.homeImage.'+game_abbr).html("<img src=\"https://sports.cbsimg.net/images/nfl/features/writers/poll_off.png\">");
        $('.homeImage.'+game_abbr).addClass('pickContainerButtonUnselected').removeClass('pickContainerButtonSelected');
        $('.homePick.'+game_abbr).addClass('selectedPickContainerNotCorrect').removeClass('selectedPickContainer');
    }
    if (homeAway == ""){
        $('.homePick.'+game_abbr).click(function() { return false; });
        $('.homePick.'+game_abbr).attr('onclick','').unbind('click');
        $('.awayPick.'+game_abbr).click(function() { return false; });
        $('.awayPick.'+game_abbr).attr('onclick','').unbind('click');
        $('.homePick.'+game_abbr).addClass('selectedPickContainerNotCorrect').removeClass('selectedPickContainer');
        $('.awayPick.'+game_abbr).addClass('selectedPickContainerNotCorrect').removeClass('selectedPickContainer');
        $('.homePercent.'+game_abbr).removeClass('percentContainerCorrect').addClass('percentContainer');
        $('.awayPercent.'+game_abbr).removeClass('percentContainerCorrect').addClass('percentContainer');
    }
    
}

function changeNumbersToPercents(agg_results){
    var resultArr = new Array();
    var arrvalues = agg_results.split("&");
    var total_votes = 0;
    for(i = 0; i < arrvalues.length; i++){
        var tmpItem = arrvalues[i].split("=");
        total_votes = parseInt(total_votes) + parseInt(tmpItem[1]);
    }
    for(i = 0; i < arrvalues.length; i++){
        var tmpItem = arrvalues[i].split("=");
        if (total_votes > 0){
            resultArr[i] = Math.round((tmpItem[1]/total_votes)*100) + "%";
        }
        else{
            resultArr[i] = "0%";
        }
    }
    return resultArr;
}

function setPicksCookie(game_abbr,pick,awayPercent,homePercent){
    //console.log("Code: " + tmp_grade_code);
    var cookieVal = readCookie("2013NCAAFpicks");
    //console.log("Starting Cookie: " + cookieVal);
    // remove from end of string
    var searchVal = "&" + game_abbr + "=away=[0-9]+=[0-9]+";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    var searchVal = "&" + game_abbr + "=home=[0-9]+=[0-9]+";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    // remove from beginning of string
    var searchVal = game_abbr + "=away=[0-9]+=[0-9]+&";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    var searchVal = game_abbr + "=home=[0-9]+=[0-9]+&";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    // remove from anywhere else
    var searchVal = game_abbr + "=away=[0-9]+=[0-9]+";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    var searchVal = game_abbr + "=home=[0-9]+=[0-9]+";
    var re = new RegExp(searchVal,"g");
    cookieVal = cookieVal.replace(re,"");
    //console.log("After Replacement: " + cookieVal);
    var cookievalues = cookieVal.split("&");
    var cookieSep = "&";
    if (cookieVal == ""){
        cookieSep = "";
    }
    var awayPercentFormatted = awayPercent.replace(/\%/,"");
    var homePercentFormatted = homePercent.replace(/\%/,"");
    var newCookieVal = cookieVal + cookieSep + game_abbr + "=" + pick + "=" + awayPercentFormatted + "=" + homePercentFormatted;
    createCookie("2013NCAAFpicks", newCookieVal, 180);
    //console.log(newCookieVal);
}
    
function setDefaultPicksCookie(cookie_name){
    var defaultCookieVal = "";
    createCookie(cookie_name, defaultCookieVal, 180);
}

function createCookie(name,value,days) {
    if (days) {
        var date = new Date();
        date.setTime(date.getTime()+(days*24*60*60*1000));
        var expires = "; expires="+date.toGMTString();
    }
    else var expires = "";
    document.cookie = name+"="+value+expires+"; path=/";
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
            if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }   
    return null;
}
    
/*
    function getValueFromCookie(cookie, grade_number){
        if (cookie == null) {
            return '';
        }
        var cookievalues = cookie.split("&");
        var returnVal = "";
        //console.log("Cookie passed in:" + cookie);
        for(i = 1; i <= cookievalues.length; i++){
            var tmpPollNum = cookievalues[i-1].split("=");
            if (tmpPollNum[0] == grade_number){
                returnVal = tmpPollNum[1];
            }
        }
        //console.log("returning: " +  returnVal);
        return returnVal;
    }
*/   
    
function init_page_from_cookie(){
    var cookie_name = "2013NCAAFpicks";
    
    var curCookie = readCookie(cookie_name);
    if (curCookie == null){
        setDefaultPicksCookie(cookie_name);
    }
    else{
        parseCookieSetDefaults(cookie_name);
    }
}

function parseCookieSetDefaults(cookie_name){
    var curCookie = readCookie(cookie_name);
    if (curCookie != null){
        var cookieArrValues = curCookie.split("&");
        //console.log(cookieArrValues);
        for(i = 0; i < cookieArrValues.length; i++){
            //console.log(cookieArrValues[i]);
            var gameArrValues = cookieArrValues[i].split("=");
            if (cookieArrValues[i].length > 0){
                change_single_value(gameArrValues[1], gameArrValues[0], gameArrValues[2]+"%", gameArrValues[3]+"%",1);
            }
        }
    }
}

$(document).ready(function() {
    init_page_from_cookie();
});



</script>

