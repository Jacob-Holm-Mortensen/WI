                              index_headertag_lightspeed.add_session_end_hook(cb, true);
                              index_headertag_lightspeed.refresh();
                          } else {
                              googletag.display("mpu_bottom");
                          }
                });
                if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="mpu_bottom" style="width:300px"><br>').insertBefore('#mpu_bottom');
            }
            </script>
        </div>
                <div class="clearBoth">&nbsp;</div>
            </div>  <!-- END column -->
            <div class="clearBoth spacer20">&nbsp;</div>
        </div><!-- END row -->
    </div><!-- END box-Rg -->
</div><!-- END container -->

