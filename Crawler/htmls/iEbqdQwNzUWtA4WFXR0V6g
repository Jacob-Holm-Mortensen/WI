&quot;:&quot;0&quot;,&quot;3&quot;:&quot;0&quot;,&quot;4&quot;:&quot;0&quot;}},&quot;field_position&quot;:{&quot;distance&quot;:&quot;15&quot;,&quot;team_in_possession&quot;:&quot;886&quot;,&quot;side&quot;:&quot;ECU&quot;,&quot;goal_to_go&quot;:&quot;No&quot;,&quot;yardline&quot;:&quot;40&quot;,&quot;redzone&quot;:&quot;No&quot;,&quot;down&quot;:&quot;3&quot;,&quot;nextplay&quot;:&quot;&quot;},&quot;last_score&quot;:{&quot;id&quot;:&quot;213&quot;,&quot;home_score&quot;:&quot;17&quot;,&quot;description&quot;:&quot;16-C.Fox runs 5 yards for a touchdown. 93-B.Johnson extra point is good.&quot;,&quot;away_score&quot;:&quot;64&quot;,&quot;drive_plays&quot;:&quot;4&quot;,&quot;drive_time&quot;:&quot;00:51&quot;,&quot;type&quot;:&quot;Touchdown&quot;,&quot;scoring_team&quot;:&quot;886&quot;,&quot;drive_yds&quot;:&quot;-19&quot;,&quot;time_remaining&quot;:&quot;6:50&quot;,&quot;quarter&quot;:&quot;4&quot;},&quot;last_play&quot;:{&quot;score_on_play&quot;:&quot;No&quot;,&quot;short_score&quot;:&quot;&quot;,&quot;medium&quot;:&quot;Rush for -3 yd&quot;,&quot;short&quot;:&quot;-3 yd run&quot;,&quot;id&quot;:&quot;228&quot;,&quot;team_in_possession&quot;:&quot;886&quot;,&quot;distance&quot;:&quot;12&quot;,&quot;side&quot;:&quot;ECU&quot;,&quot;under_review&quot;:&quot;No&quot;,&quot;description&quot;:&quot;6-A.Bush kneels at ECU 40 for -3 yards.&quot;,&quot;drive_id&quot;:&quot;28&quot;,&quot;yardline&quot;:&quot;37&quot;,&quot;down&quot;:&quot;2&quot;,&quot;subplays&quot;:{&quot;subplay&quot;:[{&quot;rush&quot;:{&quot;rusher_name&quot;:&quot;&quot;,&quot;yards_on_play&quot;:&quot;-3&quot;,&quot;team_in_possession&quot;:&quot;886&quot;,&quot;rusher&quot;:&quot;0&quot;,&quot;yards_to_endzone&quot;:&quot;37&quot;},&quot;order&quot;:&quot;1&quot;,&quot;type&quot;:&quot;Rush&quot;}]},&quot;quarter&quot;:&quot;4&quot;,&quot;time_remaining&quot;:&quot;0:45&quot;},&quot;awayscore&quot;:{&quot;total&quot;:&quot;64&quot;,&quot;timeouts_remaining&quot;:&quot;2&quot;,&quot;quarter&quot;