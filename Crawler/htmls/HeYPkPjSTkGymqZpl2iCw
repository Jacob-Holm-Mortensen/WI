<script>
    if (typeof isDiversity !== 'undefined') {
        $('.entry-section')
            .html('<img src="/wp-content/themes/cbscorporation/assets/_i/diversity/diversity_logo_gold.png" alt="CBS Corporation Diversity: Inclusion, Outreach, Impact">')
            .css({
                'padding': '28px 0 0 16px',
            });


    }


    // ::kludge:: Width of sequence elements. Used to set elements offstage for animation start.
    const DA_SEQUENCE_WIDTH = 580;
    // Time of sequence fade out animation, in milliseconds.
    const DA_TIMING_FADE_SEQUENCE = 600;
    // Time of element reveal animation, in milliseconds.
    const DA_TIMING_REVEAL_ELEMENT = 400;
    // Overall animation predelay in milliseconds. Unimplemented!
    const DA_TIMING_WAIT_ANIMATION = 2000;
    // Wait time between animating each element in a sequence, in milliseconds.
    const DA_TIMING_WAIT_ELEMENT = 80;
    // Wait time between each sequence, in milliseconds.
    const DA_TIMING_WAIT_SEQUENCE = 6000;

    // Index for next element to select from pool (0-based).
    var _daAnimationElementIndex = 0;
    // Track called state of animation function.
    var _daAnimationInit = false;


    // Point of entry.
    $( document ).ready(function() {
        animateDiversity();
    });


    // Trigger Diversity animation.
    function animateDiversity() {
        if ( _daAnimationInit ) return;
        // Animation composed of several sequences.
        // Each sequence has several separately animated elements.
        // Animation loops indefinitely.

        animateNextSequence();
        setInterval( animateNextSequence, DA_TIMING_WAIT_SEQUENCE );
        setTimeout( setAnimateSequenceHideInterval, (DA_TIMING_WAIT_SEQUENCE - DA_TIMING_FADE_SEQUENCE - DA_TIMING_WAIT_ELEMENT) );

        _daAnimationInit = true;
    }


    // Start sequence hide animation and schedule reoccuring hide animations.
    function setAnimateSequenceHideInterval() {
        setInterval( animateSequenceHide, DA_TIMING_WAIT_SEQUENCE );
        // Trigger first sequence hide.
        animateSequenceHide();
    }


    //
    function animateNextSequence() {
        var daElements = $( "#entry-section>img:not(:first-child)" );
        var daElementPool = $( "#anim-diversity-pool>img" );
        var elementCount = daElements.length;
        var elementPoolCount = daElementPool.length;

        // Set starting values for sequence elements.
        // Set img src for all sequence elements by grabbing next batch of sequence images from pool.
        var newSrc;
        var elementNum;
        var elementPoolIndex = _daAnimationElementIndex;  //nextIndex;
        for (elementNum = 0; elementNum < elementCount; elementNum++) {
            elementPoolIndex = reduceToRange( elementPoolIndex, 0, elementPoolCount-1 );
            newSrc = $( "#anim-diversity-pool>img:eq( "+elementPoolIndex+" )" ).attr("src");
            $( "#entry-section>img:not(:first-child):eq( "+elementNum+" )" ).attr("src", newSrc);
            $( "#entry-section>img:not(:first-child):eq( "+elementNum+" )" ).css({
                'opacity': 0
            });
            elementPoolIndex++;
        }

        // Establish reveal animations for all sequence elements.
        var waitTime = 0;
        var elementNums = [];
        for (elementNum = 0; elementNum < elementCount; elementNum++) {
            // setTimeout always passes reference when call triggered, not values at time of establishing call.
            // Instead of trying to send elementNum value, send elementNums array reference to then mutate in anim function.
            elementNums.push( elementNum );
            setTimeout( function(){ animateElementReveal( elementNums ); }, waitTime );
            waitTime = waitTime + DA_TIMING_WAIT_ELEMENT;
        }

        _daAnimationElementIndex = reduceToRange( _daAnimationElementIndex+elementCount, 0, elementPoolCount-1 );

    }

    function animateElementReveal( animElementNums ) {
        if (animElementNums.length>0) {
            var elementNum = animElementNums.shift();
            $( "#entry-section>img:not(:first-child):eq( "+elementNum+" )" ).css({
                'left': DA_SEQUENCE_WIDTH+'px',
                'opacity': 1
            });
            $( "#entry-section>img:not(:first-child):eq( "+elementNum+" )" ).animate({
                left: "-="+DA_SEQUENCE_WIDTH+"px"
            }, DA_TIMING_REVEAL_ELEMENT, function() {
            });
            //}
        }
    }

    function animateSequenceHide() {
        $( "#entry-section>img:not(:first-child)" ).animate({
            opacity: "0"
        }, DA_TIMING_FADE_SEQUENCE, function() {
        });

    }

    // Return a wrapped value int fitting within minRange - maxRange.
    function reduceToRange( value, minRange, maxRange ) {
        if ( (minRange >= maxRange) || ((value>=minRange) && (value<=maxRange)) ) {
            return value;
        }
        var rangeCount = maxRange - minRange + 1;
        if ( value>maxRange ) {
            value = value - rangeCount;
        } else {
            value = value + rangeCount;
        }
        return reduceToRange( value, minRange, maxRange );
    }


</script>
