<script type="text/javascript">
    $(function() {

                var currentLatestEpisodesPage = 1;
        function loadMoreLatestEpisodes(){
            $.ajax({
                type: 'GET',
                url: '/carousels/homepage/loadmore/' + currentLatestEpisodesPage + '/',
                dataType: 'json',
                success: function (data) {
                    $('#homepage-latest-episodes .loading-more').hide();
                    $( "#homepage-latest-episodes .episode-list" ).append( data.html );
                    $('#homepage-latest-episodes .load-more').show();
                    currentLatestEpisodesPage++;
                    Homepage.tracking.latestEpisodesExtra(data.cids);
                }
            });
        }

        function loadMoreClick(){
            $('#homepage-latest-episodes .load-more').hide();
            $('#homepage-latest-episodes .loading-more').show();
            loadMoreLatestEpisodes();
        }
        $('#homepage-latest-episodes .load-more').click(loadMoreClick);
    });
</script>
