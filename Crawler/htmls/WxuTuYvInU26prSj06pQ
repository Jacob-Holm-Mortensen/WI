        if (document.createEvent) { // W3C
            /**
            * The following code manually triggers a 'resize' event for the lightning carousel so that the Vilynx thumbnails get resized correclty after the ad skin changes the layout.
            */
            var resizeEvent = document.createEvent('Event');
            resizeEvent.initEvent('resize', true, true);
            window.dispatchEvent(resizeEvent);

        } else { // Internet Explorer creates events differently from other browsers
            var event = document.createEventObject();
            document.documentElement.fireEvent("onresize", event);
        }
    }
</script>
</body>
</html>