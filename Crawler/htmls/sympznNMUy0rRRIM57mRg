					<script type="text/javascript">
				jQuery(function($) {					
					$('#section-dl-menu').flexslider({
						asNavFor: '#section-dl',
						animation: false,
						animationLoop: false,
						controlNav: false,
						direction: 'horizontal',
						itemWidth: 115,
						itemMargin: 8,
						keyboard: false,
						pauseOnAction: true,
						slideshow: false,
						useCSS: false,
						video: true
					});
					$('#section-dl').flexslider({
						sync: '#section-dl-menu',
						animation: "fade",
						animationLoop: true,
						controlNav: false,
						directionNav: false,
						keyboard: false,
						pauseOnAction: true,
						slideshow: true,
						slideshowSpeed: 5000,
						animationSpeed: 800,
						video: true,
						start: function() {

							var ua = navigator.userAgent.toLowerCase();
							if (ua.indexOf('ipad') > -1) return;
							
							$('#section-dl-menu .flex-active-slide .wrapper').on('click', function(e){
								var $active_slide_links = $('#section-dl.canvas li.flex-active-slide a.node');
								if ($active_slide_links.length == 1) {
									//trigger and attached events
									$active_slide_links.trigger('click');
									// and if default triger doesn't work (and it won't), redirect manually
									if ($active_slide_links.attr('target') == '_blank') {
										window.open($active_slide_links.attr('href'));
									} else {
										window.location = $active_slide_links.attr('href');
									}
								}
							});	
						},
						after: function() {

							var ua = navigator.userAgent.toLowerCase();
							if (ua.indexOf('ipad') > -1) return;
							
							$('#section-dl-menu .flex-active-slide .wrapper').on('click', function(e){
								var $active_slide_links = $('#section-dl.canvas li.flex-active-slide a.node');
								if ($active_slide_links.length == 1) {
									//trigger and attached events
									$active_slide_links.trigger('click');
									// and if default triger doesn't work (and it won't), redirect manually
									if ($active_slide_links.attr('target') == '_blank') {
										window.open($active_slide_links.attr('href'));
									} else {
										window.location = $active_slide_links.attr('href');
									}
								}
							});
						},
						before: function() {

							$( '.dl-canvas-video iframe' ).on('click', function() {
								$('#section-dl-menu .flex-active-slide .wrapper').off('click');
							})

							$('#section-dl-menu .flex-active-slide .wrapper').off('click');

							var slide_video_player_ids = [];
							$('#section-dl ul.slides li.video-content .dl-canvas-video iframe').each(function(){
								slide_video_player_ids.push($(this).attr('id'));
							});
							if (slide_video_player_ids.length > 0) {
								for (var i in slide_video_player_ids) {
									if (typeof anvp[slide_video_player_ids[i]] != 'undefined') {
										anvp[slide_video_player_ids[i]].pause();
									}
								}
							}
						}
					});
				});
			</script>
							
		
	
	
