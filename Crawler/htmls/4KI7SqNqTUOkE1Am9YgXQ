<script type="text/javascript">
	new SmartBanner({
		daysHidden: 7,   // days to hide banner after close button is clicked (defaults to 15)
		daysReminder: 7, // days to hide banner after "VIEW" button is clicked (defaults to 90)
		maxVisits:5, // visits after which to show the banner if closing it was done with the close button
		appStoreLanguage: 'us', // language code for the App Store (defaults to user's browser language)
		title: 'Sportsline',
		author: 'Official App',
		button: 'OPEN',
		store: {
			ios: 'App Store',
			android: 'Google',
			windows: 'Windows Store'
		},
		price: {
			ios: 'Free',
			android: 'Free',
			windows: 'Free'
		}
		// , force: 'ios' // Uncomment for platform emulation
	});

	$(window).load(function() {
		//console.debug('positionSmartBanner()');
		//positionSmartBanner(false);
	});

	var smartBannerPositioned = false;

	function positionSmartBanner(bannerHideEventTrigger){
		var smartbannerHeight=0;
		var elementsToChange=["#MainNavMobile .mobile-nav-gradient", "#MainNavMobile .logo-wrapper", "#MainNavMobile .close", "#MainNavMobile .green-bar", "#MainNavDesktop", ".brand-bar"];

		var topNavHeight = $('.brand-bar').outerHeight();

		// smartBannerPositioned check prevents function from executing more than once since it's called from util.hideLoading() in Ember app
		if (show_smart_banner && smartBannerPositioned === false) {
			smartBannerPositioned = true;
			// Display Banner //
			$("#SmartBannerGlobalWrapper").css("display", "block");

			// Make sure DOM is loaded.  If not, try again.
			$.each(elementsToChange, function(index, value) {
				if($(value).height()==null)
				{
					/*setTimeout(function(){
						positionSmartBanner(false);
					}, 1500);*/
					return false;
				}
			});

			// Set Vars
			smartbannerHeight=$('#SmartBannerGlobalWrapper').height();
			var greenBarHeight = $('.green-bar').outerHeight();

			// Sets the Element CSS
			$.each(elementsToChange, function(index, value) {
				if (value!="#MainNavDesktop")
				{
					$(value).css("top", smartbannerHeight + "px");
				}
				else
				{
					$(value).css("top", (smartbannerHeight) + (topNavHeight) + "px");
				}
			});
		}

		 // This runs if the close or button is clicked
		if (bannerHideEventTrigger)
		{
			// Get the nav padding
			var navPadding = ($('#ScrollableNav').height() - $('#ScrollableNav .nav-wrapper').height()) / 2;

			// Hide the Banner Wrapper
			$("#SmartBannerGlobalWrapper").css("display", "none");

			// Reset Positioning
			$.each(elementsToChange, function(index, value) {
				if (value != "#MainNavDesktop") {
					if ($('html').hasClass('mobile-ad') && value !== '.brand-bar') {
						$(value).css("top", 50);
					} else {
						$(value).css("top", "0");
					}
				} else {
					$(value).css("top", topNavHeight);
				}
			});

			if ($('html').hasClass('mobile-ad')) {
				$('.ad-box.pin-mobile').removeAttr('style');
			}

			$('.laag-container').removeAttr('style'); // remove any possible margin that's been applied to the laag container

			var isNavOpen= $("#MainNavMobile .close").css("display")=="block" ? true: false;

			if (isNavOpen) {
				$("body #App").css({"top": window.innerHeight - greenBarHeight});
				$('#ScrollableNav').find('.nav-wrapper').css({"padding-top": "" + navPadding + "px"});
				$("html body #App").addClass("noTransitions");
				setTimeout(function(){
					$("html body #App").find('.page').css({"margin-top": "0px"});
					$("html body, html body #App").removeClass("noTransitions");
				}, 500);
			}
		}

		// notify ember that the smart app banner has finished loading
		Ember.Instrumentation.instrument("smartAppBannerLoad");
	}
</script>
