<!-- League Leaders -->
<script type="text/javascript" language="JavaScript1.1">	
function goToPage(f) {
var fileName = '';
var category = '';
var league_type = '';
for (var i = 0; i < f.elements.length; i++) { 
    if (f.elements[i].checked && f.elements[i].value.indexOf(' Find ')<0) {
        if (f.elements[i].name == 'category') {
            category = f.elements[i].value;
        }
    } 
}

var league_type = f.league_type.value;
fileName = league_type + "/" + category + "/regularseason";
var url = "/collegefootball/stats/" + f.component.value  +"/" + fileName ;
window.location = url;
return false;
} 
//-->
</script>

