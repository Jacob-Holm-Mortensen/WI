<script type="text/javascript">
$(document).ready(function() {
	$('#conference a').click(function(e) {
		e.preventDefault();
	});
	
	var tdHeight = $('#teamWrapper').closest('td').height();
	$('#teamWrapper').height(tdHeight);
	
	$('#conference a').mouseover(function() {
		$('#conference a').css('background-color', '');
		$(this).css('background-color', '#F0F0F0');
		$('#teamWrapper div').css('display', 'none');			
		var idx = $('#conference a').index(this);
		$('#teamWrapper div').eq(idx).show();
	});
	
	$('#conference a').first().trigger('mouseover');
});
</script>
    
