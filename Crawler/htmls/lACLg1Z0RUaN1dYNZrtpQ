,&quot;3&quot;:&quot;0&quot;,&quot;4&quot;:&quot;3&quot;,&quot;5&quot;:&quot;7&quot;,&quot;6&quot;:&quot;3&quot;}},&quot;field_position&quot;:{&quot;distance&quot;:&quot;0&quot;,&quot;team_in_possession&quot;:&quot;853&quot;,&quot;side&quot;:&quot;USC&quot;,&quot;goal_to_go&quot;:&quot;No&quot;,&quot;yardline&quot;:&quot;25&quot;,&quot;redzone&quot;:&quot;No&quot;,&quot;down&quot;:&quot;0&quot;,&quot;nextplay&quot;:&quot;Kickoff&quot;},&quot;last_score&quot;:{&quot;id&quot;:&quot;243&quot;,&quot;home_score&quot;:&quot;27&quot;,&quot;description&quot;:&quot;40-C.McGrath 43 yards Field Goal is Good.&quot;,&quot;away_score&quot;:&quot;24&quot;,&quot;drive_plays&quot;:&quot;4&quot;,&quot;drive_time&quot;:&quot;&quot;,&quot;type&quot;:&quot;FieldGoal&quot;,&quot;scoring_team&quot;:&quot;877&quot;,&quot;drive_yds&quot;:&quot;0&quot;,&quot;time_remaining&quot;:&quot;&quot;,&quot;quarter&quot;:&quot;6&quot;},&quot;last_play&quot;:{&quot;score_on_play&quot;:&quot;Yes&quot;,&quot;short_score&quot;:&quot;FG - C. McGrath&quot;,&quot;medium&quot;:&quot;43 yd FG by C. McGrath is good&quot;,&quot;short&quot;:&quot;43 yd FG&quot;,&quot;id&quot;:&quot;243&quot;,&quot;team_in_possession&quot;:&quot;877&quot;,&quot;distance&quot;:&quot;9&quot;,&quot;side&quot;:&quot;TEXAS&quot;,&quot;under_review&quot;:&quot;No&quot;,&quot;description&quot;:&quot;40-C.McGrath 43 yards Field Goal is Good.&quot;,&quot;drive_id&quot;:&quot;33&quot;,&quot;yardline&quot;:&quot;24&quot;,&quot;down&quot;:&quot;4&quot;,&quot;subplays&quot;:{&quot;subplay&quot;:[{&quot;fieldgoal&quot;:{&quot;yards_on_play&quot;:&quot;43&quot;,&quot;team_in_possession&quot;:&quot;877&quot;,&quot;kicker_name&quot;:&quot;C. McGrath&quot;,&quot;kicker&quot;:&quot;2870641&quot;,&quot;yards_to_endzone&quot;:&quot;24&quot;},&quot;order&quot;:&quot;1&quot;,&quot;type&quot;:&quot;FieldGoal&quot;}]},&quot;quarter&quot;:&quot;6&quot;,&quot;time_remaining&quot;:&quot;&quot;},&quot;awayscore&quot;:{&quot;total&quot;:&quot;24&quot;,&quot;ti