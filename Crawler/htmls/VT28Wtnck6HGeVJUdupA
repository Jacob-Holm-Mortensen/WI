            showVersionsHistoryDlg = function() {
                var dialog = Knowledge.Dialog.getInstance({
                    id: 'articleVersionListDlg_KnowledgeDialog',
                    accessibleMode: false,
                    componentId: 'articleVersionListDlg',
                    title: 'Version History',
                    width: 600,
                    height: 400,
                    initializerName: null,
                    validatorName: null,
                    encoderName: null
                });
                if(dialog.show) {
                    dialog.show();
                } else {
                    alert("Error: Implementation error. Show() function not implemented");
                }
            }
             //workaround for pages without header (when showheader="false")
            if ((window.sfdcPage) && (sfdcPage.executeOnloadQueue)) sfdcPage.executeOnloadQueue();
