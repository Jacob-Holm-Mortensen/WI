    <script type="text/javascript">
      jQuery.noConflict();
    </script>
    <script type="text/javascript">
    jQuery(function($) {
      
    enterEvent();
    
    //open newsletter signup dropdown on link click
    openDropdownEvent();
    
    //open register options lightbox
    $('.newsEmailBtn').on('click', function(e){
        if(ValidateForm('promomain2')){
            newsletterRegister(document.promomain2.email.value);
        }
    });
    $('.newsRegOptLightAction').on('click', function(e){
        if(ValidateForm('promomain')){
            newsletterRegister(document.promomain2.email.value);
        }
    });
    
    //close dropdown when click on close button or away from dropdown
    $('body').on('click', function(e){
        closeDropdown();
        openDropdownEvent();
        stopPropEvents();
    });
    $('.newsCloseBtn').on('click', function(e){
        closeDropdown();
        hideOverlay('bodyOverlay');
        hideNewsPopup('#newsletterPopupInfo');
        openDropdownEvent();
        stopPropEvents();
    });
    
    //update info button event    
    $('.newsUpdateBtn').on('click', function(e){
		if (ValidateForm('promo')) {
			newsletterUpdate();
			$('.newsUpdateBtn').hide();
		}
    });
    
    //prevent body event from closing the dropdown when dropdown or newlink is clicked
    stopPropEvents();
    
    function newsPopup(element, popupId, email, alignRight, evar){
        if(typeof email != 'undefined'){hideNewsPopup('#newsletterPopupEmail');}
        var ph = $(popupId).outerHeight();
        var pw = $(popupId).outerWidth();
        var p = $(popupId).detach();
        var h = $(window).height();
        var w = (element != '') ? $(element).outerWidth(true) : $(window).width();
        var scrollAmount = getYOffset();
        var itemPos = (element != '') ? $(element).position() : '';
        var posTop = h/2 - ph/2;
        var posLeft = w/2 - pw/2;
        if(alignRight) {posLeft -= pw/4;}
        if(element != ''){
            $('body').append(p);
            showOverlay('initialOverlay');
			//$(popupId).css({'top': posTop, 'left': posLeft});
            $(popupId).css({'top': posTop});
            $(popupId).css('marginLeft', '-' + $(popupId).width() / 2 + 'px');
            $(popupId).clearQueue().finish().fadeIn('500');
        }else{
            showOverlay();
            $('body').append(p);
            $(popupId).css({'top': posTop, 'left': posLeft})
                           .fadeIn('500')
                           .find("input[name='email']")
                           .val(email);
        }
		newsletterPopupImpressionTracking('delagentcbs,delagentglobalrollup',evar);
        escapeEvent(element);
    }
    
    function openDropdownEvent(){
        $('.newsPopupAction').on('click', function(e){
            newsPopup($(this), '#newsletterPopupEmail', null, null, 'eVar69');
            $(this).unbind();
            showOverlay();
            closeDropdownEvent();
            stopPropEvents();
        });
    }
    
    function closeDropdownEvent(){
        $('.newsPopupAction').on('click', function(e){
            closeDropdown();
            $(this).unbind();
            hideOverlay('bodyOverlay');
            openDropdownEvent();
            stopPropEvents();
        });
    }
    
    function stopPropEvents(){
        $('.newsPopupAction, #newsletterPopupEmail').on('click', function(e){
            e.stopPropagation();
        });
    }
    
    function escapeEvent(element){
        var $this = $(element).prop('id') + ', #newsletterPopupEmail, body';
        $($this).on({
            'keydown': function(e){
                if (e.keyCode == 27) {
                    $(element).unbind();
                    closeDropdown();
                    hideOverlay('bodyOverlay');
                    hideNewsPopup('#newsletterPopupInfo');
                    openDropdownEvent();
                    stopPropEvents();
                }
            }
        });   
    }
    
    function closeDropdown(){
        $('#newsletterPopupEmail').fadeOut('500');
    }
    
    function enterEvent(){
        $("#signupFormPopup, #signupForm").each(function(){
            var formName = $(this).prop('name');
            var formID = '#' + $(this).prop('id');
            if($(this).prop('id') == 'signupFormPopup'){
                $(formID).on({
                    'keydown': function(e){
                        if (e.keyCode == 13) {
                            //submit email on enter keypress if in the signup form popup
                            if(ValidateForm('promomain2')){
                                newsletterRegister(document.promomain2.email.value);
                                
                            }
                            
                        }
                    },
                    'submit': function(){return false;}
                });    
            }else{
            $(formID).on({
                'keydown': function(e){
                    if (e.keyCode == 13) {
                        if(ValidateForm('promomain')){
                            newsletterRegister(document.promomain.email.value);
                        }
                    }
                },
                'submit': function(){return false;}
            });   
            }
            
        });
    }
    
    function newsletterRegister(email){
        $.ajax({
            type: 'POST',
            url: '/register.php?f=register',
            data: { 'email':email},
            dataType: 'html',
            beforeSend: function(){
                hideNewsPopup('#newsletterPopupEmail');
            },
            error: ajaxErrors,
            success: function(data){
                hideOverlay('bodyOverlay');
                newsPopup('', '#newsletterPopupInfo', email, null, 'eVar70');
                escapeEvent('#newsletterPopupInfo');				dataLayer.push({'event': 'newsletterPopupSubmit'});            }
        });
    }
    
    function newsUpdateBtnEvent(){        
        $('.newsUpdateBtn').on('click', function(e){
            newsletterUpdate();
        });
    }
    
    function delayedClose() {
        closeDropdown();
        hideOverlay('bodyOverlay');
        hideNewsPopup('#newsletterPopupInfo');
        openDropdownEvent();
        stopPropEvents();
    }
    
    function newsletterUpdate(){
        $.ajax({
            type: 'POST',
            url: '/register.php?f=register',
            data: $('#newsregister').serialize(),
            dataType: 'html',
            beforeSend: function(){
                showOverlay('#newsletterPopupInfo');
                clearHideHtml('.registerComplete');
            },
            error: ajaxErrors,
            success: function(data){
                $('.registerComplete').html('Thanks for registering!').show();
            },
            complete: function(data, status){
                hideOverlay('ajaxOverlay');
                setTimeout(delayedClose, 1000);
            }
        });
    }
    
    function hideNewsPopup(popupId){
        $(popupId).fadeOut('500').find('input[type=text], select').each(function(i){
           /*if($(this).attr('name') == 'email'){
               $(this).val('Enter Your Email Address');
            }else{
                $(this).val('');
            }*/
			//$(this).prop('disabled', true);
        });
        clearHideHtml('.registerComplete');
    }
    
    function clearHideHtml(selector){
        $(selector).html('').hide();
    }
    
    function showOverlay(element){
        var overlay = (typeof element != 'undefined' && element != '') ? getOverlay('ajaxOverlay', true) : getOverlay('bodyOverlay');
        (typeof element != 'undefined' && element != '') ? $(element).append(overlay) : $('body').append(overlay);
    }
    
    function hideOverlay(id){
        var id = '#' + id;
        $(id).remove();
    }
    
    function getOverlay(id, showLoader){
        showLoader = (typeof showLoader != 'undefined') ? showLoader : false;
        var overlay = $('<div/>', {
            id: id,
            css: {
            'position': (showLoader) ? 'absolute' : 'fixed',
            'top': '0px',
            'left': '0px',
            'display': 'block',
            'opacity': (showLoader) ? '0.3' : '0.7',
            'width': '100%',
            'height': '100%', 
            'background': "#000 url('') no-repeat center center",
            'z-index': (showLoader) ? '9500' : '8500'  
            },
            click: function(e){        
                if(!showLoader){
                    hideOverlay(id);
                    hideNewsPopup('#newsletterPopupInfo');
                }
            }
        });
        return overlay;
    }
    
    function ajaxErrors(request, type, errorThrown){
      var message = "There was an error with the AJAX request.\n";
      switch (type) {
        case 'timeout':
          message += "The request timed out.";
          break;
        case 'notmodified':
          message += "The request was not modified but was not retrieved from the cache.";
          break;
        case 'parsererror':
          message += "XML/Json format is bad.";
          break;
        default:
          message += "HTTP Error (" + request.status + " " + request.statusText + ").";
      }
      message += "\n";
      alert(message);  
    }
    
    function getYOffset() {
      var pageY;
      if(typeof(window.pageYOffset)=='number') {
         pageY=window.pageYOffset;
      } else {
         pageY=document.documentElement.scrollTop;
      }
      return pageY;
    }
 
});

</script>
    

<script type="text/javascript">
    jQuery.noConflict();
    (function($) {
        
        var nlPopup = {
            init:function(popupCookie){
                nlPopup.checkCookie(popupCookie);
            },
            setCookie:function(c_name,value,exdays){
                var exdate=new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
                document.cookie=c_name + "=" + c_value + ";path=/";
            },
            checkCookie:function(c_name){
                var i, x = 0, cookieName, cookieValue, siteCookies = document.cookie.split(';');
                for(i=0; i < siteCookies.length; i++){
                    cookieName = siteCookies[i].substr(0,siteCookies[i].indexOf("="));
                    cookieValue = siteCookies[i].substr(siteCookies[i].indexOf("=")+1);
                    cookieName=cookieName.replace(/^\s+|\s+$/g,"");
                    if(cookieName === c_name){x = 1;}
                }
            
                if(x === 1){
                    return false;
                }else{
                    nlPopup.displayPopup();
                    nlPopup.setCookie(c_name, 1, 10);
                }
            },
            displayPopup:function(){
					$(window).bind("load", function() {
						$('.newsPopupAction').click();
					});
            }
        }
        

        nlPopup.init('DA_NEWSLETTER_POPUP');

	})(jQuery);
</script>
				 									

