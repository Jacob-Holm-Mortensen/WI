	<h2 class="module-heading title col-12">More From 48 Hours</h2>
	<div class="carousel col-12" data-component="carousel" data-carousel-options='{"slideHeight":"390","numOfVisibleSlides":3,"transitionType":"slideHorizontalInfinite","autoplayDelay":0,"incrementAmount":3}'>
	    <div class="viewport" data-item="viewport">
