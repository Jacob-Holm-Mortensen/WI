	(function($) {
		$.widget( "ui.autocomplete", $.ui.autocomplete, {
			   options: {
				  renderMenu: null
			   },
			   _renderMenu: function( ul, items ) {
				  if ( $.isFunction( this.options.renderMenu ) ) {
					 this.options.renderMenu( ul, items );
				  }
				  this._super( ul, items );
			   },
			});
		 
	    $( "#sli_search_1" ).autocomplete({
      minLength: 0,
      source: function( request, response ) { jQuery.ajax({
                   type: "GET",
                   url: '/auto_sli.php/?cnt=5',
                   dataType:"json",
                   data:{term: request.term},
                   beforeSend: function(){
                       $( "#sli_search_1" ).addClass('loadinggif');
                   },
                   success: function(data)
                   {
					 $( "#sli_search_1" ).removeClass('loadinggif');
					 response(data);
                   }
           }); },
      autoFocus: false,
      delay: 0,
      select: function( event, ui ) {
		$( "#sli_search_1" ).val( ui.item.label );
		location.href = ui.item.url;
		return false;
		},
      focus: function( event, ui ) {
        $( "#sli_search_1" ).val( ui.item.label );
        return false;
      },
      open: function(){
        $('.ui-autocomplete').css('width', '1046px');
        }
    })
    .autocomplete( "instance" )._renderItem = function( ul, item ) {
      return $( "<li>" )
        .append(   "<img src=" + item.icon + ">" + "<span class='ui-product-name'>" + item.label + 
        "</span>"  + "<span class='ui-product-desc'>" + item.desc + 
        "</span><span class='ui-product-price ui-product-price-" + item.price.replace(".","") + " productAddLink'>$" + item.price + "</span><div style='clear:both'>" )
        .appendTo( ul );
    };
    $(document).click(function(event) { 
    if(!$(event.target).closest('.ui-autocomplete').length) {
        if($('.ui-autocomplete').is(":visible")) {
            $('.ui-autocomplete').autocomplete( "close" );
        }
    }        
})
  })(jQuery);
  
  
</script>







												
	
			
	
			  





