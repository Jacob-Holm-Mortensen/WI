      MoreImgsImage.css("border", "1px solid #d2d2d2");
      $(".more1").parent().css("border", "1px solid black");
      $("#prodDetailMoreImgs img").on("click", function() {
        MoreImgsImage.css("border-color", "#d2d2d2");
        $(this).parent().css("border-color", "black");
        if ($(this).hasClass("videoThumbnail") ||
          $(this).hasClass("playbutton")) {
            return;
        }
        hideVideo();
        $("#enlargeLink1").attr("href", $(this).attr("id"));
        $("#enlargeLink2").attr("href", $(this).attr("id"));
        $("#detailLargeImage").attr("src", $(this).attr("rel"));
