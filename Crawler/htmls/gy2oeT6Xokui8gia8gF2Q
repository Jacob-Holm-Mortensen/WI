    <script type="text/javascript">
  
  var upsellOverlay;
      function showUpsellInfo(){
        upsellOverlay = new Element('div', {'id': 'upsellOverlay'}).setStyle('opacity', 0).injectInside(document.body);
        upsellOverlay.addEvent('click', function(){ window.removeEvents('scroll'); toggleSelects(false);$('upsellOverlay').destroy(); $('upsellPopup').destroy(); });
        toggleSelects(true);
        positionupsellOverlay();var openOverlay = new Fx.Morph(upsellOverlay,{duration: 500});     
	    openOverlay.start({'opacity': [0, .5]});       
        popup = new Element("div", {id: "upsellPopup"}).setStyle('visibility', 'hidden').injectInside(document.body);
        popup.set('html',  $('upsellPopupContent').get('html'));
        var size = popup.getSize();
        popup.setStyles({
            'top': ((window.getHeight()/2) + window.getScrollTop()  - (size.y / 2)),
            'left': ((window.getWidth() / 2) + window.getScrollLeft() - (size.x / 2))
        });
        popup.setStyle('display', 'block');
        popup.setStyle('visibility', 'visible');
        window.addEvents({'scroll': function(upsellOverlay){positionupsellOverlay()}, 'resize': function(upsellOverlay){positionupsellOverlay()}});
      }
      
      function positionupsellOverlay(){
	    $('upsellOverlay').setStyles({'left': 0, 'top': window.getScrollTop(), 'height': window.getHeight()});
	  }

      function closeUpsellPopup(){
	  window.removeEvents('scroll');
       $('upsellOverlay').destroy(); $('upsellPopup').destroy();
		 toggleSelects(false);
	  }
      
      function toggleSelects(open){
        var ie6 = Browser.Engine.trident4;
        [ie6 ? "select" : "embed"].forEach(function(tag) {
			Array.forEach(document.getElementsByTagName(tag), function(el) {
				if (open) {
                    el.lbBackupStyle = el.style.visibility;
                    el.style.visibility = "hidden";
                } else {
                    if($defined(el.lbBackupStyle)){                        
                        el.style.visibility =  el.lbBackupStyle;
                    }
                }
			});
		}); 
      }
  
</script>

    

  
  <script type="text/javascript">
    window.addEvent('load', function() {
      $('detailLargeImage').addEvent('mousemove', function(e) {
        var newX = (e.client.x - this.getLeft()) / this.offsetWidth;
        var newY = (e.client.y - this.getTop() + window.getScrollTop()) / this.offsetHeight;
        newX = newX * 100;
        newY = newY * 100;
        var newXY = newX + "% " + newY + "%"; 
        $('detailImageZoom').setStyle('background-position', newXY ) ;
      });
    });
    window.addEvent('domready', function() {
      $('detailLargeImage').addEvents({
        'mouseenter': function(e) {
          $('detailImageZoomHolder').setStyle('display', 'block' );
          $$('select').each(function(e) { 
            e.setStyle('visibility', 'hidden'); 
          });
        },
        'mouseleave': function(e) {
          $('detailImageZoomHolder').setStyle('display', 'none' );
          $$('select').each(function(e) { 
            e.setStyle('visibility', 'visible'); 
          });
        }
      });
    });
  </script>
  
  

