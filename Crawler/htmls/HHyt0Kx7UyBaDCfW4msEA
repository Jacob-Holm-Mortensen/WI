    <script src="http://grfx.cstv.com/frontend/bxslider-4/jquery.bxslider.min.js"></script>
    <script>

    var setPromoSize = function() {
      var h  = $(window).height() - 33;
      $("#splash-promo").css({'min-height': h});
      $("#splash-bg").css({'height': h+50});
    }

    $(function() {

      setPromoSize();

      $(window).on("resize", function(){
        setPromoSize();
      })

      $("body").on("click", "#scroll-down", function(e) {
        e.preventDefault();
        var sectTop = $("#splash-sect-1").offset().top;
        $('html,body').animate({
          scrollTop: sectTop-100
        }, 450);
      })

      $('#splash-promo-slider').bxSlider({
        adaptiveHeight: true,
        mode: 'fade',
        controls: false,
        pager: true,
        auto: true,
        pause: 10000,
        touchEnabled: false,
        onSliderLoad: function() {
          var lgOrSm = $(window).width() < 1024 ? 'sm' : 'lg';
          $("#splash-bg").css({"background-image": 'url(' + $('#splash-promo-slider li:eq(0)').data('bg-' + lgOrSm) + ')' })
        },
        onSlideBefore: function($slideElement, oldIndex, newIndex) {
          var lgOrSm = $(window).width() < 1024 ? 'sm' : 'lg';
          $("#splash-bg").css({"background-image": 'url(' + $('#splash-promo-slider li:eq(' + newIndex + ')').data('bg-' + lgOrSm) + ')' })
        }
      });

    });
    </script>

