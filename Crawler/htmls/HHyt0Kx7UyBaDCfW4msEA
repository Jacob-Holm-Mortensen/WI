<script>

var $app = {
  params:  function(val) {
    if (!val) return '';
    var a = val.replace('?','').split('&');

    if (a == "") return {};
    var b = {};
    for (var i = 0; i < a.length; ++i) {
      var p = a[i].split('=');
      if (p.length != 2) continue;
      b[p[0]] = decodeURIComponent(p[1].replace(/\+/g, " "));
    }
    return b;
  }
};

$(function(){ 

      // SPLASH
    $("body").on("click", "#splash-mobile-nav", function() {

      if ($("#splash-nav ul").hasClass("nav-closed")) {
        $("#splash-nav ul").addClass("nav-open");
        $("#splash-nav ul").removeClass("nav-closed");
      } else {
        $("#splash-nav ul").addClass("nav-closed");
        $("#splash-nav ul").removeClass("nav-open");
      }

    });


  $("body").on("click", "#splash-modal-close, #splash-modal-bg", function(e) {
    e.preventDefault();
    $("#splash-modal-schools, #splash-modal-bg").hide();
  });
  $("body").on("click", ".show-schools", function(e) {
    e.preventDefault();
    $("#splash-modal-schools, #splash-modal-bg").fadeIn();
  });


  $("body").on("click", ".launch-site", function(e) {
    e.preventDefault();

    $.cookies.set('CSL_ignoreSplash', 'true');

    if (window.location.search) {

      var loc = $app.params(window.location.search);
      var newLoc =  'http://www.collegesportslive.com/?';
      if (window.location.search) {
        _.each(loc, function(val, key) {
          newLoc += '&'+key+'='+val;
        });
      }
      newLoc = newLoc.replace('?&', '?');
      newLoc = newLoc.replace('??', '?');
      window.location.href = newLoc;

      return;

    } else {
      window.location.href="http://www.collegesportslive.com/";
      return;    
    }

  });

  $("body").on("click", ".splash-subscribe", function(e) {

      e.preventDefault();

      var additional = "";
      var loc;
      if (window.location.search) {
        loc = $app.params(window.location.search);
        var href = $app.params($(this).attr('href'));
        if (!loc.ftag && href.ftag) {
          additional += "&ftag=" + href.ftag;
        }
        _.each(loc, function(val, key) {
          additional += "&"+key+'='+val;
        });
      } else {
        additional = $(this).attr('href').replace('?', '&');
      }
      additional = additional.replace('#', '');


      loc = 'http://www.collegesportslive.com/';
      loc = loc.replace(/\#/, '');
      loc = loc.replace(/\?.*$/, '');


      window.location.href = 'https://reg.cstv.com/register/?school_code=cslive&license_ids=' + additional + '&redir=' + loc;
    });



  $("body").on("click", ".auto-popup-login", function(e) {
    e.preventDefault();

    $.cookies.set('CSL_ignoreSplash', 'true');
      var loc = $app.params(window.location.search);
      var newLoc =  'http://www.collegesportslive.com/?show_login=true';
      if (window.location.search) {
        _.each(loc, function(val, key) {
          newLoc += '&'+key+'='+val;
        });
      }
      newLoc = newLoc.replace('?&', '?');
      newLoc = newLoc.replace('??', '?');
      window.location.href = newLoc;
      return;
  });


  


});
</script>

