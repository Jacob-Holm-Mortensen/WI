<script>


    $('#footerLinksCol1 a, #footerLinksCol2 a, #footerLinksCol3 a, #footerLinksCol4 a').click(function(e){
        $('#footerLinksCol1 a, #footerLinksCol2 a, #footerLinksCol3 a, #footerLinksCol4 a').removeClass('hover');
        $(this).addClass('hover');

        var clickedFirst = $('#footerLinksCol1 a.hover');
        var clickedSecond = $('#footerLinksCol2 a.hover');
        var clickedThird = $('#footerLinksCol3 a.hover');
        var clickedFourth = $('#footerLinksCol4 a.hover');

        // get element position
        var posFirst = $(clickedFirst).parent().index();
        var posSecond = $(clickedSecond).parent().index();
        var posThird = $(clickedThird).parent().index();
        var posFourth = $(clickedFourth).parent().index();

        // determine clicked link position in term of the entire footer
        var pos1 = (posFirst >= 0)? posFirst : 0;
        var pos2 = (posSecond >= 0)? posSecond + $('#footerLinksCol1 a').length : 0;
        var pos3 = (posThird >= 0)? posThird + $('#footerLinksCol1 a').length + $('#footerLinksCol2 a').length : 0;
        var pos4 = (posFourth >= 0)? posFourth + $('#footerLinksCol1 a').length + $('#footerLinksCol2 a').length + $('#footerLinksCol3 a').length: 0;

        // retrieve the link text which was clicked
        var linkTxt1 = (posFirst >= 0)? $(clickedFirst).html() : '';
        var linkTxt2 = (posSecond >= 0)? $(clickedSecond).html() : '';
        var linkTxt3 = (posThird >= 0)? $(clickedThird).html() : '';
        var linkTxt4 = (posFourth >= 0)? $(clickedFourth).html() : '';

        var pos = pos1 + pos2 + pos3 + pos4; // only one variable will have value.  All other will be empty.
        var linkTxt = linkTxt1 + linkTxt2 + linkTxt3 + linkTxt4; // only one variable will have value.  All other will be empty.
        om.trackClick({item:"footer|" + linkTxt + "|" + pos + "|click"});
    });

    /*
     this.getTealiumGlobalNavData = function() {
     var clicked = $('.nav-menu .nav-item a.hover');
     var pos = $(clicked).parent().index();
     return "global nav|" + $(clicked).html() + "|" + pos + "|click";
     }*/



</script>

<script type="text/javascript">
