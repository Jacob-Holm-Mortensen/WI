<script>
    function initPlayer(pid) {

        var cbsplayer = new CBSPlayer.Video();
        cbsplayer.pid = pid;
        cbsplayer.container = 'VIDEO_CONTENT';
        cbsplayer.userStatus = CBS.Registry.user.sb;
        cbsplayer.autoPlay = true;
        cbsplayer.relatedVideos = false;
        cbsplayer.dimension.width = 650;
        cbsplayer.dimension.height = 374;
        cbsplayer.setConfigs({"playerPath":"\/\/can.cbs.com\/thunder\/player\/chrome\/cbscom\/prod\/vod\/canplayer.swf","jsPlayerPath":"\/\/can.cbs.com\/thunder\/player\/chrome\/cbscom\/prod\/vod\/canplayer.js","configs":"\/\/can.cbs.com\/thunder\/player\/chrome\/cbscom\/prod\/vod\/config\/skin_config_override.xml","uvpcs":"\/\/can.cbs.com\/thunder\/player\/chrome\/cbscom\/prod\/vod\/config\/uvpc_config_override.xml","uvpcsCbsn":"\/\/can.cbs.com\/thunder\/player\/chrome\/cbscom\/prod\/vod\/config\/uvpc_config_override_cbsn.xml"});
        cbsplayer.init().enableCasting().embed();
    }

    function initPlayerIpad(pid, cid) {

        var __o = $('#svod-videoverlay').find('#video-content-wrapper');
        __o.html('<div id="centeredPlayerWrapper"><div id="centeredPlayerContainer"><div id="videoplayer" class="video-player"><div id="mycbsiplayer"></div></div></div>').hide();

        var video = new CBS.Video();
        video.settings.width = 848;
        video.settings.height = 477;
        video.settings.pid = pid;
        video.settings.content_id = cid;
        video.hi5 = {};
        video.hi5.device_partner = "cbs_ipad";
        video.hi5.preview_img = "http://thumbnails.cbsig.net/CBS_Production_Entertainment_VMS/2014/09/15/329314371559/AA_UPSELL_SITE_424106_640x360.jpg";
        video.hi5.title = "CBS All Access - Preview";

        video.page_options = {};
        video.page_options.share_url = "";
        video.page_options.base_url_video = "";

        window['video'] = video;

        var js = document.createElement("script");
        js.type = "text/javascript";
        js.src = "/assets/js/cbs/module/video/cbs.module.video.hi5.js";
        document.body.appendChild(js);
        setTimeout(function () {
            __o.find('#videoplayer').css('width','848px').css('height','477px');
            __o.show();
        }, 500)
    }

    function playVideo(_pid, _cid) {
        var full_wid = $(document).width();
        var full_height = $(document).height();
        $('#svod-videouterlay').css('width', full_wid).css('height', full_height);

        var el_wid = $('#svod-videoverlay').width();
        var el_left = ($(document).width() - el_wid) / 2;
        $('#svod-videoverlay').css('left', el_left);


                initPlayer(_pid);
        

        $('#svod-videouterlay').show();
        $('#svod-videoverlay').show();
    }

    function closeVideoWindowButton() {
        try {
            if (rcpHolder != 'undefined') {
                rcpHolder.stopVideo();
                $("#video-content-wrapper").html('').html('<div id="VIDEO_CONTENT"></div>');
            }
        } catch (e) {
            $("#video-content-wrapper").html('').html('<div id="VIDEO_CONTENT"></div>');
        }

        $('#svod-videouterlay').hide();
        $('#svod-videoverlay').hide();
    }
</script>

