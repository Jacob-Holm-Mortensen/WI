    document.addEventListener('sp.blocking', function (e) {
        cbsplayer.hasAdBlocker = true;
        om.trackView({
            adBlockStatus: true
        });
    });
    document.addEventListener('sp.not_blocking', function (e) {
        cbsplayer.hasAdBlocker = false;
        om.trackView({
            adBlockStatus: false
        });
