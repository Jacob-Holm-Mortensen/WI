    $(function () {
        $('body').bind('playlistStart', function () {
            try {
                DW.levt('video', 'log', {'uid':CBS.getUID(), 'contentid': cbsplayer.contentId});
            } catch (e) {
            }
        });
        $('body').bind('playlistEnd', function() {
