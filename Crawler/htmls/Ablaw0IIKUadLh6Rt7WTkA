    function rnowCenter() {
        rnowReference = window.open("/info/user_services/help/helpcenter.php?ref=" + window.location.toString(), "RightNow", "width=850,height=806,resizable,scrollbars=yes,toolbar=yes,menubar=no,location=no,status=1");
