        if( $('#descriptionReadMore').length ){
            $('#descriptionReadMore').click(function () {
                $('#descriptionReadMore').remove();
                $('.video-description .elipse').remove();
                var newHTML = $('.video-description .endString').html();
                $('.video-description .endString').remove();
                $('.video-description').html($('.video-description').html().trim() + newHTML );

            });
        }

    });

