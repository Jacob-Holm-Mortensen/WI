    function setCookie(name,value,expires,path,domain){var today=new Date();today.setTime(today.getTime());if(expires){expires=expires*1000*60*60*24}var expires_date=new Date(today.getTime()+(expires));document.cookie=name+"="+escape(value)+((expires)?";expires="+expires_date.toGMTString():"")+((path)?";path="+path:"")+((domain)?";domain="+domain:"")}function getCookie(name){var c_name=name+"=";var dc=document.cookie;if(dc.length>=0){s_index=dc.indexOf(c_name);if(s_index!=-1){s_index+=c_name.length;e_index=dc.indexOf(";",s_index);if(e_index==-1){e_index=dc.length}return unescape(dc.substring(s_index,e_index))}}return null}function check(){if(getCookie("CBS")){return}else{}};

