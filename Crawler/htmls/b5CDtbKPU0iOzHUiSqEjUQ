<script src="https://sports.cbsimg.net/js/jquery/plugins/touchwipe/jquery.touchwipe.min.js"></script>
<script type="text/javascript">
  $(document).ready(function() {

      $('#carousel_ul ').touchwipe({
          wipeLeft: function() {
              slide("right");
          },
          wipeRight: function() {
              slide("right");
          },
          wipeUp: function(e) {},
          wipeDown: function(e) {},
          preventDefaultEvents: false
      });

      $("#leader_plus_top").parent().css('height', '110px')
      $("#leader_plus_top").parent().css('background-color', 'white')


      //options( 1 - ON , 0 - OFF)
      var auto_slide = 1;
      var hover_pause = 1;
      var key_slide = 1;

      if (navigator.userAgent.match(/(iPad)/g)) {
          auto_slide = 0;
          hover_pause = 0;
      }

      var valuedot = $("#carousel_ul li ").first().attr("id");
      //Url Defaults
      if (valuedot == "firstSlide") {
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      }

      // 'cbs-sports'   => 1, 'fantasy-app'  => 2, 'maxpreps'     => 3, 'college-app'  => 4 

      if ("" == "cbs-sports") {
          auto_slide = 0;
          hover_pause = 1;
      } else if ("" == "fantasy-app") {
          auto_slide = 0;
          hover_pause = 1;
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#zero").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          if(navigator.userAgent.match(/Android/i)){
                    window.location="https://play.google.com/store/apps/details?id=com.cbs.sports.fantasy&amp;feature=search_result#?t=W251bGwsMSwyLDEsImNvbS5jYnMuc3BvcnRzLmZhbnRhc3kiXQ";
           }
          if(navigator.userAgent.match(/(iPad|iPhone|iPod)/g)){
                    window.location="https://itunes.apple.com/us/app/cbs-sports-fantasy-football/id658308834?mt=8";
          }
          if(navigator.userAgent.match(/Windows Phone/i)){
                    window.location="http://apps.microsoft.com/windows/en-us/app/9f38c74f-f69c-4913-a3f4-7334ce286da9";
          }
      } else if ("" == "maxpreps") {
          auto_slide = 0;
          hover_pause = 1;
          $("#zero").prependTo("#carousel_ul");
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          if(navigator.userAgent.match(/Android/i)){
             window.location="https://play.google.com/store/apps/details?id=com.maxpreps.mpscoreboard";
           }
      } else if ("" == "draftkit") {
          auto_slide = 0;
          hover_pause = 1;
          $("#zero").prependTo("#carousel_ul");
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          if(navigator.userAgent.match(/Android/i)){
                    window.location="https://play.google.com/store/apps/details?id=com.cbs.sports.draftkit";
           }
      } else if ("" == "college-app") {
          auto_slide = 0;
          hover_pause = 1;
          $("#two").insertBefore('#firstSlide');
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
      } else {
        auto_slide = 1;
        hover_pause = 1;
      }
    //DropDown option change
      $(".optionsSelectWrapper select").change('change', function() {
          $(this).parent().find('.optionsSelectText').html($(this).find('option:selected').text());
          var appleurl=$(this).find('option:selected').attr("data-apple");
          var googleurl=$(this).find('option:selected').attr("data-google");
          $("#applestoreslide").attr('href', appleurl);
          $("#googlestoreslide").attr('href', googleurl);
          $("#applestorefooter").attr('href', appleurl);
          $("#googlestorefooter").attr('href', googleurl);
      });
      $(".optionsSelectWrapper select").trigger('change');

    //Set value Autoslide
      var auto_slide_seconds = 5000;

    //Autoslide =1 when carousel moves
      if (auto_slide == 1) {
          var timer = setInterval('slide("right")', auto_slide_seconds);
          $('#hidden_auto_slide_seconds').val(auto_slide_seconds);
      }
    //Click Events Dots navigation
      $('#dotone').click(function() {
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#zero").prependTo("#carousel_ul");
          $("#firstSlide").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      });

      $('#dotfour').click(function() {
          $("#one").prependTo("#carousel_ul");
          $("#zero").prependTo("#carousel_ul");
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
      });
      $('#dottwo').click(function() {
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#zero").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      });
      $('#dotthree').click(function() {

          $("#zero").prependTo("#carousel_ul");
          $("#firstSlide").prependTo("#carousel_ul");
          $("#two").prependTo("#carousel_ul");
          $("#one").prependTo("#carousel_ul");
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      });


      if (hover_pause == 1) {
          $('#carousel_container').hover(function() {
              clearInterval(timer)
          }, function() {
              timer = setInterval('slide("right")', auto_slide_seconds);
          });

          $('#left_scroll').hover(function() {
              clearInterval(timer)
          }, function() {
              timer = setInterval('slide("right")', auto_slide_seconds);
          });

          $('#right_scroll').hover(function() {
              clearInterval(timer)
          }, function() {
              timer = setInterval('slide("right")', auto_slide_seconds);
          });
      }
  });


  function slide(where) {
      var valuedot = $("#carousel_ul li ").first().attr("id");
      if (valuedot == "firstSlide") {
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      }

      if (valuedot == "zero") {
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      }
      if (valuedot == "one") {
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
      }
      if (valuedot == "two") {
          $("#dotone").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_on.png')");
          $("#dottwo").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotthree").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
          $("#dotfour").css('background-image', "url('https://sports.cbsimg.net/images/mobile/dot_off.png')");
      }

      var item_width = $('#carousel_ul li').outerWidth() + 10;

      if (where == 'left') {
          var left_indent = "0";
      } else {
          var left_indent = "-980";
      }
      $('#carousel_ul:not(:animated)').animate({
          'left': left_indent
      }, 500, function() {
          if (where == 'left') {
              $('#carousel_ul li:first').before($('#carousel_ul li:last'));
          } else {
              $('#carousel_ul li:last').after($('#carousel_ul li:first'));
          }
          if (where == "right") {
              $('#carousel_ul').css({
                  'left': '0px'
              });
          } else {
              $('#carousel_ul').css({
                  'left': '0px'
              });
          }
      });
      
  }
</script>

    
        </div><!-- END pageContainer-->
</div><!-- END pageRow-->

