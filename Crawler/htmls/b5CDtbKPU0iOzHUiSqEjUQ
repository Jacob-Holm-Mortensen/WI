    <div id="leader_plus2_bottom" class="footerAd">
<script type="text/javascript">
if (switchXFP != 'off') {
    googletag.cmd.push(function() {
        if (typeof index_headertag_lightspeed !== 'undefined') {
                  var cb = (function () {
                      return function () {
                          var slots = googletag.pubads().getSlots();
                          index_headertag_lightspeed.set_slot_targeting(slots);
                           googletag.display("leader_plus2_bottom");
                        };
                  })();
                  index_headertag_lightspeed.add_session_end_hook(cb, true);
                  index_headertag_lightspeed.refresh();
              } else {
                  googletag.display("leader_plus2_bottom");
              }
    });
    if (location.search.substring(1).indexOf('DCLK')>-1) $('<input type="text" value="leader_plus2_bottom" style="width:728px"><br>').insertBefore('#leader_plus2_bottom');
}
</script>
