<script language="javascript" type="text/javascript">
	function ieRedrawFixCollapsed () {
		jQuery(".cbs-mod-rc").css({position: 'relative'});
		jQuery(".cbs-mod-rc-2x").css({position: 'relative'});
		jQuery(".playBtnOverlay").css({position: 'relative'});
	}
	function ieRedrawFixExpanded () {
		jQuery(".cbs-mod-rc").css({position: 'static'});
		jQuery(".cbs-mod-rc-2x").css({position: 'static'});
		jQuery(".playBtnOverlay").css({position: 'static'});
	}
</script>
<![endif]-->

