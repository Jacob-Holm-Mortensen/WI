<script>
    var showMarquee;
    function renderAdCallback(adObj) {
        if(typeof(showMarquee) !== "undefined"){
            if (navigator.userAgent.indexOf("Firefox") > 0) { //For some reason Firefox doesn't calculate the page height correctly at the moment this callback is fired. Delaying the callback by a second seems to fix it.
                setTimeout(function(){
                    showMarquee.setScrollContainerHeight();
                }, 1000);
            } else {
                showMarquee.setScrollContainerHeight();
            }
        }
    }

        CBS.Marquee.BlankShowLanding();
    

</script>



