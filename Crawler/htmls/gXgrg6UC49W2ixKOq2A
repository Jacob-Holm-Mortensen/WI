        cbsplayer.options.flashVars.cpSession = 'true';
        cbsplayer.options.flashVars.cpPre = 'true';
    }else{
        cbsplayer.options.flashVars.cpSession = 'false';
        cbsplayer.options.flashVars.cpPre = 'false';
    }

    cbsplayer.saveContinuousPlaySessionData = function(contsess, contPlayState, seriesTitle){

        var storage = JSON.parse(localStorage.getItem('canplayer_cbs')) || {};
        storage.cp = {};

        if (contsess) {
            storage.cp.contsess = contsess;
        }

        if (contPlayState) {
            storage.cp.contPlayState = contPlayState;
        }

        if (seriesTitle) {
            storage.cp.seriesTitle = seriesTitle;
        }

        localStorage.setItem('canplayer_cbs', JSON.stringify(storage));
    };

    cbsplayer.continuousPlayNextVideoMetadata = function(){

        var nextVideo = {};

        if(cbsplayer.nextVideo){
            nextVideo.seriesTitle = cbsplayer.nextVideo.series_title;
            nextVideo.seasonNumber = cbsplayer.nextVideo.season_number;
            nextVideo.videoTitle = cbsplayer.nextVideo.title;
            nextVideo.episodeNumber = cbsplayer.nextVideo.episode_number;
            nextVideo.airDate = cbsplayer.nextVideo.airdate;
            nextVideo.duration = cbsplayer.nextVideo.duration_raw;
            nextVideo.showPreviewImage = cbsplayer.nextVideo.thumb['large'];
            nextVideo.videoPageURL = "http://www.cbs.com" + cbsplayer.nextVideo.url;
        }
        return nextVideo;
    };

    cbsplayer.eraseContinuousPlaySessionData = function(){
        var storage = JSON.parse(localStorage.getItem('canplayer_cbs'));

        if(storage){  // Clean up old cp session data
            delete storage.cp;
            delete storage.contsess;
            delete storage.contPlayState;
            delete storage.seriesTitle;
        }

        localStorage.setItem('canplayer_cbs', JSON.stringify(storage));
    };

    
    cbsplayer.title = "EIF Presents: XQ Super School Live";
    cbsplayer.container = 'VIDEO_CONTENT';
    cbsplayer.partner = 'cbs';
    cbsplayer.relatedVideos = true;
    cbsplayer.relatedPlayInline = false;


    cbsplayer.tmsSeriesId = '';
