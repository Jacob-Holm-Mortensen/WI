var skyBoxScroll = function() {
    if ($(window).scrollTop() != 0) {
        window.scrollTo(window.pageXOffset, window.pageYOffset - 1);
        window.scrollTo(window.pageXOffset, window.pageYOffset + 1);
    }
}
